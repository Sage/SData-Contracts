<?xml version="1.0" encoding="utf-8"?>
<?xml-stylesheet type="text/xsl" href="../Tools/XsdToHtml.xslt"?>
<xs:schema xmlns:sme="http://schemas.sage.com/sdata/sme/2007" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://schemas.sage.com/sc/2009" targetNamespace="http://schemas.sage.com/sc/2009" elementFormDefault="qualified" id="sc" version="1.1.1">
  <xs:element name="operatingCompany" type="operatingCompany--type" sme:role="resourceKind" sme:pluralName="operatingCompanies" sme:compliance="must" sme:hasUuid="true" sme:canGet="false" sme:unsupported="true" />
  <xs:complexType name="operatingCompany--type">
    <xs:all>
      <xs:element name="active" type="xs:boolean" minOccurs="0" sme:label="Operating Company Active Flag">
        <xs:annotation>
          <xs:documentation>
            Flag to indicate if the Company is an active Company.
            Default = True
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="name" type="xs:normalizedString" minOccurs="0" sme:label="Operating Company Name" sme:isMandatory="true">
        <xs:annotation>
          <xs:documentation>
            The entity name (e.g. Home-style Kitchens).
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="shortName" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Operating Company Short Name">
        <xs:annotation>
          <xs:documentation>
            A shorter name for the Company.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="currency" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Operating Company Currency">
        <xs:annotation>
          <xs:documentation>
            The Company currency (e.g. GBP, USD, EUR).
            ISO 4217
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="identifier" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Operating Company Identifier" sme:isUniqueKey="true">
        <xs:annotation>
          <xs:documentation>
            Identifier(s) of the Company as used by the third parties (e.g. Company Registration Number, DUNS number, 1099 type, CIS number, siret number, Trading Number, etc).
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="taxLocale" type="xs:normalizedString" minOccurs="0" sme:label="Operating Company Tax Locale">
        <xs:annotation>
          <xs:documentation>
            The tax-locale applicable to the Operating Company (e.g. District)
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="taxReference" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Operating Company Tax Reference" sme:isUniqueKey="true">
        <xs:annotation>
          <xs:documentation>
            Tax reference associated with the Company (e.g. VAT number, numero intracommnunautaire, FEIN)
            NIF (Spain)
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="language" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Operating Company Language">
        <xs:annotation>
          <xs:documentation>
            Language used by the Company (e.g. EN-GB)
            ISO 639-1
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="postalAddresses" type="postalAddress--list" minOccurs="0" sme:label="Operating Company Postal Addresses" sme:isCollection="true" sme:relationship="child">
        <xs:annotation>
          <xs:documentation>
            Postal address(es) associated with the Operating Company. See PostalAddress definition.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="phoneNumbers" type="phoneNumber--list" minOccurs="0" sme:label="Operating Company Phone Numbers" sme:isCollection="true" sme:relationship="association">
        <xs:annotation>
          <xs:documentation>
            Phone numbers associated with the Operating Company. See PhoneNumber definition.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="legalEntityType" type="operatingCompanyLegalEntityType--enum" minOccurs="0" sme:label="Operating Company Legal Entity Type">
        <xs:annotation>
          <xs:documentation>
            The type of legal business entity the Operating Company is, e.g. Partnership, Limited Company, etc.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>
    <xs:anyAttribute namespace="##other" processContents="lax" />
  </xs:complexType>
  <xs:complexType name="operatingCompany--list">
    <xs:sequence>
      <xs:element name="operatingCompany" type="operatingCompany--type" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
    <xs:anyAttribute namespace="##other" processContents="lax" />
  </xs:complexType>
  <xs:element name="person" type="person--type" sme:role="resourceKind" sme:pluralName="people" sme:compliance="must" sme:canGet="false" sme:unsupported="true" sme:canPut="true" sme:canPost="true" sme:canDelete="true" />
  <xs:complexType name="person--type">
    <xs:all />
    <xs:anyAttribute namespace="##other" processContents="lax" />
  </xs:complexType>
  <xs:element name="postalAddress" type="postalAddress--type" sme:role="resourceKind" sme:pluralName="postalAddresses" sme:label="postalAddresses" sme:compliance="must" sme:canGet="true" sme:canPageNext="true" sme:canPageIndex="true" sme:canPut="false" sme:canPost="false" sme:canDelete="false" sme:hasUuid="true"/>
  <xs:complexType name="postalAddress--type">
    <xs:all>
      <xs:element name="active" type="xs:boolean" minOccurs="0" sme:label="PostalAddress Active Flag"
                  sme:isReadOnly="true">
        <xs:annotation>
          <xs:documentation>
            Flag to indicate if the Postal Address is an active one. Default =
            True
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="reference" type="xs:normalizedString" minOccurs="0" sme:label="PostalAddress Reference" sme:isUniqueKey="true"
                  sme:unsupported="true">
        <xs:annotation>
          <xs:documentation>Reference for the Postal Address.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="name" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="PostalAddress Name"
                  sme:unsupported="true">
        <xs:annotation>
          <xs:documentation>
            A name for the Postal Address (e.g. Billing Address).
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="description" type="xs:string" nillable="true" minOccurs="0" sme:label="PostalAddress Description"
                  sme:unsupported="true">
        <xs:annotation>
          <xs:documentation>
            A longer description of the postal Postal Address.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="address1" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="PostalAddress Address 1">
        <xs:annotation>
          <xs:documentation>1st Line of Postal Address</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="address2" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="PostalAddress Address 2">
        <xs:annotation>
          <xs:documentation>2nd Line of Postal Address</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="address3" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="PostalAddress Address 3">
        <xs:annotation>
          <xs:documentation>3rd Line of Postal Address</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="address4" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="PostalAddress Address 4">
        <xs:annotation>
          <xs:documentation>4th Line of Postal Address</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="townCity" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="PostalAddress TownCity">
        <xs:annotation>
          <xs:documentation>Town or City</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="county" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="PostalAddress County">
        <xs:annotation>
          <xs:documentation>County</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="stateRegion" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="PostalAddress StateRegion">
        <xs:annotation>
          <xs:documentation>
            State/Region. For USA addresses use ISO 3166-2:US for state codes. For
            UK addresses use ISO 3166-2:GB. For French addresses use ISO 3166-2:FR. For German
            addresses use ISO 3166-2:DE. For Canada addresses use ISO 3166-2:CA.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="zipPostCode" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="PostalAddress ZipPost Code">
        <xs:annotation>
          <xs:documentation>
            Zip or Post code. Should be formatted as raw text no formatting
            included.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="country" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="PostalAddress Country">
        <xs:annotation>
          <xs:documentation>Country. ISO 3166-1 alpha-2 country codes only</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="primacyIndicator" type="xs:boolean" minOccurs="0" sme:label="PostalAddress Primacy Indicator">
        <xs:annotation>
          <xs:documentation>
            A flag to indicate if the Address is the primary/default Address. Only
            one Postal Address can have Primacy against an associated entity (e.g. Contact, Company,
            etc). Default = False
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="type" type="postalAddressType--enum" minOccurs="0" sme:label="PostalAddress Type" sme:isMandatory="true">
        <xs:annotation>
          <xs:documentation>
            The type of the Postal Address, e.g., billing, sales address, home,
            etc. A new Postal Address will be required for each type, even though the address
            itself may be the same. Default = Correspondence
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="notes" type="note--list" nillable="true" minOccurs="0" sme:label="PostalAddress Notes" sme:relationship="child" sme:isCollection="true"
                  sme:unsupported="true">
        <xs:annotation>
          <xs:documentation>Notes associated with the Postal Address.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>
    <xs:anyAttribute namespace="##other" processContents="lax" />
  </xs:complexType>
  <xs:complexType name="postalAddress--list">
    <xs:sequence>
      <xs:element name="postalAddress" type="postalAddress--type" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
    <xs:anyAttribute namespace="##other" processContents="lax" />
  </xs:complexType>
  <xs:element name="email" type="email--type" sme:role="resourceKind" sme:pluralName="emails" sme:label="emails" sme:compliance="must" sme:canGet="true" sme:canPageNext="true" sme:canPageIndex="true" sme:canPut="false" sme:canPost="false" sme:canDelete="false" sme:hasUuid="true"/>
  <xs:complexType name="email--type">
    <xs:all>
      <xs:element name="active" type="xs:boolean" minOccurs="0" sme:label="Email Active Flag"
                  sme:isReadOnly="true" sme:unsupported="true">
        <xs:annotation>
          <xs:documentation>
            Flag to indicate if the Email is an active one. Default = True
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="reference" type="xs:normalizedString" minOccurs="0" sme:label="Email Reference" sme:isUniqueKey="true" sme:unsupported="true">
        <xs:annotation>
          <xs:documentation>Reference associated with the Email.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="reference2" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Email Reference2" sme:unsupported="true">
        <xs:annotation>
          <xs:documentation>Another reference associated with the email.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="type" type="emailType--enum" minOccurs="0" sme:label="Email Type" sme:unsupported="true">
        <xs:annotation>
          <xs:documentation>
            A simple string value used to label this email address. It allows for
            email addresses such as Work, Personal, Preferred, etc. Can also be used to
            distinguish instant messaging type addresses.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="address" type="xs:normalizedString" minOccurs="0" sme:label="Email Address" sme:isMandatory="true">
        <xs:annotation>
          <xs:documentation>
            The Email address itself (e.g. joe.bloggs@acme.com).
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="url" type="xs:normalizedString" minOccurs="0" sme:label="mailto:URL" sme:isUniqueKey="true" sme:unsupported="true">
        <xs:annotation>
          <xs:documentation>
            URL that allows an email to be automatically generated that is to be
            set to the email address. Could be created automatically.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="primacyIndicator" type="xs:boolean" minOccurs="0" sme:label="Email Primacy Indicator" sme:unsupported="true">
        <xs:annotation>
          <xs:documentation>
            Flag to indicate if this Email is the primary one associated with the
            linked resource. Default = False
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="notes" type="note--list" nillable="true" minOccurs="0" sme:label="Email Notes" sme:relationship="child" sme:isCollection="true" sme:unsupported="true">
        <xs:annotation>
          <xs:documentation>Notes associated with email.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>
    <xs:anyAttribute namespace="##other" processContents="lax" />
  </xs:complexType>
  <xs:complexType name="email--list">
    <xs:sequence>
      <xs:element name="email" type="email--type" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
    <xs:anyAttribute namespace="##other" processContents="lax" />
  </xs:complexType>
  <xs:element name="phoneNumber" type="phoneNumber--type" sme:role="resourceKind" sme:pluralName="phoneNumbers" sme:label="phoneNumbers" sme:compliance="must" sme:canGet="true" sme:canPageNext="true" sme:canPageIndex="true" sme:canPut="false" sme:canPost="false" sme:canDelete="false" sme:hasUuid="true"/>
  <xs:complexType name="phoneNumber--type">
    <xs:all>
      <xs:element name="active" type="xs:boolean" minOccurs="0" sme:label="PhoneNumber Active Flag"
                  sme:isReadOnly="true">
        <xs:annotation>
          <xs:documentation>
            Flag to indicate if the phone number is an active one. Default =
            True
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="reference" type="xs:normalizedString" minOccurs="0" sme:label="PhoneNumber Reference" sme:isUniqueKey="true"
                  sme:unsupported="true">
        <xs:annotation>
          <xs:documentation>Reference associated with the phone number.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="reference2" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="PhoneNumber Reference2"
                  sme:unsupported="true">
        <xs:annotation>
          <xs:documentation>
            Another reference associated with the phone number.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="uri" type="xs:normalizedString" minOccurs="0" sme:label="PhoneNumber URI"
                  sme:unsupported="true">
        <xs:annotation>
          <xs:documentation>
            A phone unique reference number used to represent the number in a
            formal way. Useful for programmatic access, such as a VoIP/PSTN/CTI. Could be derived
            by concatenating attributes.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="type" type="phoneNumberType--enum" minOccurs="0" sme:label="PhoneNumber Type">
        <xs:annotation>
          <xs:documentation>
            A value that identifies the type of phone number (general, fax, car,
            home, internal, mobile, other, pager, satellite, VoIP, work).
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="name" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="PhoneNumber Name"
                  sme:unsupported="true">
        <xs:annotation>
          <xs:documentation>
            A simple string value used to label this phone number. In most cases,
            Label is not necessary as Phone Type uniquely identifies a number and allows UIs to
            display a proper label such as Mobile, Home, Work, etc. However, in the case where
            one person has (for example) multiple mobile phones, this property can be used to
            disambiguate them. Could be derived from Phone Type
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="text" type="xs:normalizedString" minOccurs="0" sme:label="PhoneNumber Text" sme:isMandatory="true" sme:isReadOnly="true">
        <xs:annotation>
          <xs:documentation>
            Human-readable phone number, containing all attributes. Could be
            derived by concatenating other properties.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="countryCode" type="xs:normalizedString" minOccurs="0" sme:label="PhoneNumber Country Code">
        <xs:annotation>
          <xs:documentation>
            Country code for the phone number based on ITU-T recommendation E.164
            country codes. ITU-T E. 164 country codes only.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="areaCode" type="xs:normalizedString" minOccurs="0" sme:label="PhoneNumber Area Code">
        <xs:annotation>
          <xs:documentation>Area code for the phone number</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="number" type="xs:normalizedString" minOccurs="0" sme:label="PhoneNumber Number">
        <xs:annotation>
          <xs:documentation>Telephone number.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="extension" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="PhoneNumber Extension">
        <xs:annotation>
          <xs:documentation>Any extension applicable to the phone number.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="primacyIndicator" type="xs:boolean" minOccurs="0" sme:label="PhoneNumber Primacy Indicator">
        <xs:annotation>
          <xs:documentation>
            Flag to indicate if this Phone Number is the primary one associated
            with the linked resource. Default = False
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="notes" type="note--list" nillable="true" minOccurs="0" sme:label="PhoneNumber Note" sme:relationship="child" sme:isCollection="true"
                  sme:unsupported="true">
        <xs:annotation>
          <xs:documentation>Notes associated with phone number.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>
    <xs:anyAttribute namespace="##other" processContents="lax" />
  </xs:complexType>
  <xs:complexType name="phoneNumber--list">
    <xs:sequence>
      <xs:element name="phoneNumber" type="phoneNumber--type" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
    <xs:anyAttribute namespace="##other" processContents="lax" />
  </xs:complexType>
  <xs:element name="note" type="note--type" sme:role="resourceKind" sme:pluralName="notes" sme:compliance="may" sme:canGet="false" sme:unsupported="true" />
  <xs:complexType name="note--type">
    <xs:all>
      <xs:element name="active" type="xs:boolean" minOccurs="0" sme:label="Note Active Flag">
        <xs:annotation>
          <xs:documentation>
            Flag to indicate if the note is an active one. Default = True
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="reference" type="xs:normalizedString" minOccurs="0" sme:label="Note Reference" sme:isUniqueKey="true">
        <xs:annotation>
          <xs:documentation>Reference associated with the note.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="reference2" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Note Reference2">
        <xs:annotation>
          <xs:documentation>Another reference associated with the note.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="text" type="xs:string" minOccurs="0" sme:label="Note Text" sme:isMandatory="true">
        <xs:annotation>
          <xs:documentation>The note text itself.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="privacyFlag" type="xs:boolean" minOccurs="0" sme:label="Note Privacy Flag">
        <xs:annotation>
          <xs:documentation>
            Flag to indicate if the note is public or private. Default = No
            (not private).
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="dateStamp" type="xs:date" minOccurs="0" sme:label="Note Date Stamp">
        <xs:annotation>
          <xs:documentation>Date and time of note</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="primacyIndicator" type="xs:boolean" minOccurs="0" sme:label="Note Primacy Indicator">
        <xs:annotation>
          <xs:documentation>
            Flag to indicate if this Note is the primary one associated with the
            linked resource. Default = False
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="user" type="xs:normalizedString" minOccurs="0" sme:label="Note User">
        <xs:annotation>
          <xs:documentation>Details of the user who created the note.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>
    <xs:anyAttribute namespace="##other" processContents="lax" />
  </xs:complexType>
  <xs:complexType name="note--list">
    <xs:sequence>
      <xs:element name="note" type="note--type" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
    <xs:anyAttribute namespace="##other" processContents="lax" />
  </xs:complexType>
  <xs:element name="financialAccount" type="financialAccount--type" sme:role="resourceKind" sme:pluralName="financialAccounts" sme:compliance="may" sme:canGet="false" sme:unsupported="true" />
  <xs:complexType name="financialAccount--type">
    <xs:all>
      <xs:element name="financialAccountGroups" type="financialAccountGroup--list" nillable="true" minOccurs="0" sme:label="FinancialAccount Groups" sme:relationship="association" sme:isCollection="true">
        <xs:annotation>
          <xs:documentation>
            Link to the Financial Account Group associated with this Financial Account
            See Financial Account Group definition.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="active" type="xs:boolean" minOccurs="0" sme:label="FinancialAccount Active Flag">
        <xs:annotation>
          <xs:documentation>
            Flag to indicate if the FinancialAccount is an active one. Default = True
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="reference" type="xs:normalizedString" minOccurs="0" sme:label="FinancialAccount Reference" sme:isUniqueKey="true">
        <xs:annotation>
          <xs:documentation>Reference associated with the Financial Account.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="reference2" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="FinancialAccount Reference2">
        <xs:annotation>
          <xs:documentation>
            Another reference associated with the Financial Account.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="accountCategory" type="financialAccountAccountCategory--enum" nillable="true" minOccurs="0" sme:label="FinancialAccount Account Category">
        <xs:annotation>
          <xs:documentation>
            The account category applicable to the FinancialAccount (see enum for list of values).
            Account categories classify each account into major accounting categories for financial reporting.
            Implementation teams should try and map to these where possible (based on IFRS/GAAP so should be common). However the list can be extended to add other account categories.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="accountType" type="financialAccountAccountType--enum" nillable="true" minOccurs="0" sme:label="FinancialAccount Account Type">
        <xs:annotation>
          <xs:documentation>
            The account type applicable to the FinancialAccount (see enum for list of values). Implementation teams should try and map to these where possible (based on IFRS/GAAP so should be common). However the list can be extended to add other account types.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="postingType" type="financialAccountPostingType--enum" nillable="true" minOccurs="0" sme:label="FinancialAccount Posting Type">
        <xs:annotation>
          <xs:documentation>
            The type of posting applicable to the FinancialAccount (memo, posting, grouping). Default =
            Posting. Memo accounts are not posting accounts - they are used to show text only, e.g. for
            reporting purposes. Grouping accounts are super-types used to group other accounts together,
            e.g. all sales accounts. Posting accounts receive financial postings
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="reportingType" type="financialAccountReportingType--enum" nillable="true" minOccurs="0" sme:label="FinancialAccount Reporting Type">
        <xs:annotation>
          <xs:documentation>
