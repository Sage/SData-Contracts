<?xml version="1.0" encoding="utf-8"?>
<?xml-stylesheet type="text/xsl" href="../Tools/XsdToHtml.xslt"?>
<xs:schema id="famErp" elementFormDefault="qualified" targetNamespace="http://schemas.sage.com/famErp/2009" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:sme="http://schemas.sage.com/sdata/sme/2007" xmlns="http://schemas.sage.com/famErp/2009" xmlns:famErp="http://schemas.sage.com/famErp/2009" xmlns:sc="http://schemas.sage.com/sc/2009" xmlns:crmErp="http://schemas.sage.com/crmErp/2008" xmlns:hrmErp="http://schemas.sage.com/hrmErp/2009">
  <xs:import namespace="http://schemas.sage.com/sc/2009" schemaLocation="common.xsd"/>
  <xs:import namespace="http://schemas.sage.com/crmErp/2008" schemaLocation="crmErp.xsd"/>
  <xs:import namespace="http://schemas.sage.com/hrmErp/2009" schemaLocation="hrmErp.xsd"/>
  <xs:element name="fixedAsset" type="fixedAsset--type" sme:role="resourceKind" sme:pluralName="fixedAssets" sme:compliance="must" sme:canGet="true" sme:canPut="true" sme:canPost="true" sme:canDelete="true" />
  <xs:complexType name="fixedAsset--type">
    <xs:all>
      <xs:element name="active" sme:label="FixedAsset Active Flag" minOccurs="0" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>Flag to indicate if the Fixed Asset is an ‘active’ one .
               
Default = True
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="FixedAssetGroup" sme:label="FixedAsset FixedAssetGroup" minOccurs="0" type="fixedAssetGroup--list" sme:relationship="association" sme:isCollection="true" nillable="true">
        <xs:annotation>
          <xs:documentation>Identifier of the Fixed Asset group this Fixed Asset belongs to.
               
See “FixedAssetGroup” definition.
FixedAssets can only belong to one FixedAsset Group of type “Family”, but may belong to many groups of type “user defined”.

</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="reference" sme:label="FixedAsset reference" minOccurs="0" type="xs:string" sme:isUniqueKey="true">
        <xs:annotation>
          <xs:documentation>reference for the Fixed Asset (typically the one used by the buying organization).
               

</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="reference2" sme:label="FixedAsset Reference2" minOccurs="0" type="xs:string" sme:isExtension="true" nillable="true">
        <xs:annotation>
          <xs:documentation>Another reference for the Fixed Asset.
               

</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="operatingCompany" sme:label="FixedAsset OperatingCompany" minOccurs="0" type="sc:operatingCompany--type" sme:relationship="reference" sme:isCollection="false">
        <xs:annotation>
          <xs:documentation>Link to the OperatingCompany associated with the Fixed Asset.
               
See “Company” definition (where Company Type = “Operating Company”).
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="workplace" sme:label="FixedAsset Workplace" minOccurs="0" type="workplace--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
        <xs:annotation>
          <xs:documentation>Link to the Workplace (within the Operating Company) associated with the Fixed Asset
               
See “Workplace” definition
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="status" sme:label="FixedAsset Status" minOccurs="0" type="xs:string" sme:isExtension="true" nillable="true">
        <xs:annotation>
          <xs:documentation>Status of the Fixed Asset.
               

</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ownershipStatus" sme:label="FixedAsset Ownership Status" minOccurs="0" type="fixedAssetOwnershipStatus--enum" nillable="true">
        <xs:annotation>
          <xs:documentation>Status of ownership for the Fixed Asset (planned – also known as expenditure - or actual).
               

</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="schedule" sme:label="FixedAsset Schedule" minOccurs="0" type="fixedAssetSchedule--list" sme:relationship="association" sme:isCollection="true" nillable="true">
        <xs:annotation>
          <xs:documentation>A link to the (depreciation) schedule associated with the Fixed Asset.
               
See “FixedAssetSchedule” definition.
An asset may be on more than one schedule as there may be one for tax and one for accounting.
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="name" sme:label="FixedAsset Name" minOccurs="0" type="xs:string" sme:isMandatory="true" nillable="true">
        <xs:annotation>
          <xs:documentation>Name for the Fixed Asset.
               

</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="shortName" sme:label="FixedAsset Short Name" minOccurs="0" type="xs:string" nillable="true">
        <xs:annotation>
          <xs:documentation>A shorter name for the Fixed Asset.
               

</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="description" sme:label="FixedAsset Description" minOccurs="0" type="xs:string" nillable="true">
        <xs:annotation>
          <xs:documentation>The Fixed Asset description.
               

</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="alternativeDescription1" sme:label="FixedAsset Alternative Description 1" minOccurs="0" type="xs:string" sme:isExtension="true" nillable="true">
        <xs:annotation>
          <xs:documentation>An alternative Fixed Asset description.
               

</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="alternativeDescription2" sme:label="FixedAsset Alternative Description 2" minOccurs="0" type="xs:string" sme:isExtension="true" nillable="true">
        <xs:annotation>
          <xs:documentation>Another alternative Fixed Asset description.
               

</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="holdingTerms" sme:label="FixedAsset Holding Term" minOccurs="0" type="fixedAssetHoldingTerms--enum">
        <xs:annotation>
          <xs:documentation>The Fixed Asset holding terms (clarifies ownership terms around the asset) 
               

</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="contractReference" sme:label="FixedAsset Contract Reference" minOccurs="0" type="xs:string" nillable="true">
        <xs:annotation>
          <xs:documentation>For holding terms that are either leasing contract, or concession, there may be an associated contract – this is the contract reference.
               

</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="assetType" sme:label="FixedAsset Asset Type" minOccurs="0" type="fixedAssetAssetType--enum" nillable="true">
        <xs:annotation>
          <xs:documentation>The asset type associated with the Fixed Asset.
               

</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="permanence" sme:label="FixedAsset Permanence" minOccurs="0" type="fixedAssetPermanence--enum" nillable="true">
        <xs:annotation>
          <xs:documentation>Flag to indicate the permanence (or otherwise) of the Fixed Asset.
               

</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="catalogueNumber" sme:label="FixedAsset Catalogue Number" minOccurs="0" type="xs:string" nillable="true">
        <xs:annotation>
          <xs:documentation>reference for the Fixed Asset in a Supplier catalogue.
               

</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="supplierTradingAccount" sme:label="FixedAsset Supplier TradingAccount" minOccurs="0" type="crmErp:tradingAccount--list" sme:relationship="association" sme:isCollection="true" nillable="true">
        <xs:annotation>
          <xs:documentation>Link to the Fixed Asset Supplier trading account.
               
See “TradingAccount” definition (CRM schema).
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="supplierReference" sme:label="FixedAsset Supplier reference" minOccurs="0" type="xs:string" nillable="true">
        <xs:annotation>
          <xs:documentation>reference for the Fixed Asset as used by the Supplier.
               

</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="manufacturerCompany" sme:label="FixedAsset Manufacturer Company" minOccurs="0" type="xs:string" sme:relationship="reference" sme:isCollection="false" sme:isExtension="true" nillable="true">
        <xs:annotation>
          <xs:documentation>Link to the Company associated with manufacturing the Fixed Asset.
               
See “Company” definition (CRM schema).
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="manufacturerReference" sme:label="FixedAsset Manufacturer reference" minOccurs="0" type="xs:string" sme:isExtension="true" nillable="true">
        <xs:annotation>
          <xs:documentation>reference for the Fixed Asset as used by the manufacturer.
               

</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="legalTaxCodes" sme:label="FixedAsset Legal Tax Code " minOccurs="0" type="crmErp:taxCode--list" sme:relationship="association" sme:isCollection="true" nillable="true">
        <xs:annotation>
          <xs:documentation>Tax codes governing the legal tax applicable to this Fixed Asset (e.g. VAT, GST).
               
See “TaxCode” definition (CRM schema).
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="legalTaxRecoveryFlag" sme:label="FixedAsset Legal Tax Recovery Flag" minOccurs="0" type="fixedAssetLegalTaxRecoveryFlag--enum" nillable="true">
        <xs:annotation>
          <xs:documentation>Flag to indicate if legal tax (VAT, GST) is recoverable on the Fixed Asset.
               

</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="legalTaxRecoveryCodes" sme:label="FixedAsset Legal Tax Recovery Code " minOccurs="0" type="crmErp:taxCode--list" sme:relationship="association" sme:isCollection="true" nillable="true">
        <xs:annotation>
          <xs:documentation>Tax codes governing what legal tax is recoverable (if legal tax recovery flag = recoverable)
               
See “TaxCode” definition (CRM schema).
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IASTaxCodes" sme:label="FixedAsset IAS Tax Code " minOccurs="0" type="crmErp:taxCode--list" sme:relationship="association" sme:isCollection="true" nillable="true">
        <xs:annotation>
          <xs:documentation>Tax codes governing the IAS tax applicable to this Fixed Asset (e.g. VAT, GST).
               
See “TaxCode” definition (CRM schema).
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IASTaxRecoveryFlag" sme:label="FixedAsset IAS Tax Recovery Flag" minOccurs="0" type="xs:string" nillable="true">
        <xs:annotation>
          <xs:documentation>Flag to indicate if IAS tax (VAT, GST) is recoverable on the Fixed Asset.
               
Recoverable
Non-Recoverable
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IASTaxRecoveryCodes" sme:label="FixedAsset IAS Tax Recovery Code " minOccurs="0" type="crmErp:taxCode--list" sme:relationship="association" sme:isCollection="true" nillable="true">
        <xs:annotation>
          <xs:documentation>Tax codes governing what IAS tax is recoverable (if IAS tax recovery flag = recoverable)
               
See “TaxCode” definition (CRM schema).
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="legalFinancialAccountGroup" sme:label="FixedAsset Legal FinancialAccountGroup" minOccurs="0" type="financialAccountGroup--list" sme:relationship="association" sme:isCollection="true" nillable="true">
        <xs:annotation>
          <xs:documentation>Link to the legal Financial Account Group associated with this Fixed Asset.
               
See “Financial Account Group” definition.
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="legalFinancialAccounts" sme:label="FixedAsset Legal FinancialAccount " minOccurs="0" type="sc:financialAccount--list" sme:relationship="association" sme:isCollection="true" nillable="true">
        <xs:annotation>
          <xs:documentation>Legal financial accounting code applicable to the Fixed Asset.
               
See “FinancialAccount” definition (CRM schema).

</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="legalCostCentres" type="sc:costCentre--list" nillable="true" minOccurs="0" sme:label="Fixed Asset legal CostCentre" sme:relationship="association" sme:isCollection="true">
        <xs:annotation>
          <xs:documentation></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="legalDepartments" type="sc:department--list" nillable="true" minOccurs="0" sme:label="Fixed Asset Legal Departments" sme:relationship="association" sme:isCollection="true">
        <xs:annotation>
          <xs:documentation></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IASFinancialAccountGroup" sme:label="FixedAsset IAS FinancialAccountGroup" minOccurs="0" type="financialAccountGroup--list" sme:relationship="association" sme:isCollection="true" nillable="true">
        <xs:annotation>
          <xs:documentation>Link to the IAS Financial Account Group associated with this Fixed Asset.
               
See “Financial Account Group” definition.
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IASFinancialAccounts" sme:label="FixedAsset IAS FinancialAccount " minOccurs="0" type="sc:financialAccount--list" sme:relationship="association" sme:isCollection="true" nillable="true">
        <xs:annotation>
          <xs:documentation>IAS financial accounting code applicable to the Fixed Asset.
               
See “FinancialAccount” definition (CRM schema).

</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IASCostCentres" type="sc:costCentre--list" nillable="true" minOccurs="0" sme:label="Fixed Asset IAS CostCentre" sme:relationship="association" sme:isCollection="true">
        <xs:annotation>
          <xs:documentation></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IASDepartments" type="sc:department--list" nillable="true" minOccurs="0" sme:label="Fixed Asset IAS Departments" sme:relationship="association" sme:isCollection="true">
        <xs:annotation>
          <xs:documentation></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="tradeIdentificationNumber" sme:label="FixedAsset Trade Identification Number" minOccurs="0" type="xs:string" sme:isUniqueKey="true" nillable="true">
        <xs:annotation>
          <xs:documentation>The GTIN (global trade identification number) for the Fixed Asset (e.g. EAN-13, UPC-13 for barcodes).
               
Used for handling bar codes.
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="replacement" sme:label="FixedAsset Replacement" minOccurs="0" type="fixedAsset--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
        <xs:annotation>
          <xs:documentation>If this Fixed Asset has been superseded this is a link to the ‘new’ Fixed Asset.
               
See “FixedAsset” definition.
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="replacementDate" sme:label="FixedAsset Replacement Date" minOccurs="0" type="xs:date" nillable="true">
        <xs:annotation>
          <xs:documentation>For superceded or replaced Fixed Assets, this is the start date of the replacement.
               

</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="currency" sme:label="FixedAsset Currency" minOccurs="0" type="xs:string" nillable="true">
        <xs:annotation>
          <xs:documentation>The currency of the Fixed Asset (e.g. GBP, USD, EUR).
               

</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="operatingCompanyCurrency" sme:label="FixedAsset OperatingCompany Currency" minOccurs="0" type="xs:string" nillable="true">
        <xs:annotation>
          <xs:documentation>The currency as used by OperatingCompany, the business operating the Sage application (the “base” currency).
               

</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="operatingCompanyCurrencyExchangeRate" sme:label="FixedAsset OperatingCompany Currency Exchange Rate" minOccurs="0" type="xs:decimal" nillable="true">
        <xs:annotation>
          <xs:documentation>Rate used to convert from Fixed Asset Currency into Fixed Asset OperatingCompany Currency, if different.
               

</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="operatingCompanyCurrencyExchangeRateOperator" sme:label="FixedAsset OperatingCompany Currency Exchange Rate Operator" minOccurs="0" type="fixedAssetOperatingCompanyCurrencyExchangeRateOperator--enum" nillable="true">
        <xs:annotation>
          <xs:documentation>Operator to be used (multiplication or division) to convert from Fixed Asset Currency to Fixed Asset OperatingCompany Currency.
               
“*” (multiplication) or “/” division.

E.g. if the operator is division then it is Sales Order Values in Sales Order Currency / Sales Order Operating Company Currency Exchange Rate = Sales Order Values in Sales Order OperatingCompany currency.
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="operatingCompanyCurrencyExchangeRateDate" sme:label="FixedAsset OperatingCompany Currency Exchange Rate Date" minOccurs="0" type="xs:date" nillable="true">
        <xs:annotation>
          <xs:documentation>Date of the exchange rate used to convert from Fixed Asset Currency to Fixed Asset OperatingCompany Currency, if different.
               
Default = System date
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="legalCost" sme:label="FixedAsset Legal Cost " minOccurs="0" type="xs:decimal" nillable="true">
        <xs:annotation>
          <xs:documentation>Depending on the costing method used, this is the “cost” of the Fixed Asset.
               

</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IASCost" sme:label="FixedAsset IAS Cost " minOccurs="0" type="xs:decimal" nillable="true">
        <xs:annotation>
          <xs:documentation>Depending on the costing method used, this is the “cost” of the Fixed Asset.
               

</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="legalNatureOfAsset" sme:label="FixedAsset Legal Nature of Asset" minOccurs="0" type="xs:string" nillable="true">
        <xs:annotation>
          <xs:documentation>The legal nature of the Fixed Asset
               

</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IASNatureOfAsset" sme:label="FixedAsset IAS Nature of Asset" minOccurs="0" type="xs:string" nillable="true">
        <xs:annotation>
          <xs:documentation>The IAS nature of the Fixed Asset
               

</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="purchaseRequisitionLines" sme:label="FixedAsset Purchase Requisition Line" minOccurs="0" type="crmErp:purchaseRequisitionLine--list" sme:relationship="association" sme:isCollection="true" nillable="true">
        <xs:annotation>
          <xs:documentation>Purchase Requisition Line(s) associated with the Fixed Asset.
               
See “PurchaseRequisitionLine” definition (CRM schema).
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="purchaseOrderLines" sme:label="FixedAsset Purchase Order Line" minOccurs="0" type="crmErp:purchaseOrderLine--list" sme:relationship="association" sme:isCollection="true" nillable="true">
        <xs:annotation>
          <xs:documentation>Purchase Order Line(s) associated with the Fixed Asset.
               
See “PurchaseOrderLine” definition (CRM schema).
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="purchaseOrderDeliveryLines" sme:label="FixedAsset Purchase Order Delivery Line" minOccurs="0" type="crmErp:purchaseOrderDeliveryLine--list" sme:relationship="association" sme:isCollection="true" nillable="true">
        <xs:annotation>
          <xs:documentation>Purchase Order Delivery Line(s) associated with the Fixed Asset.
               
See “PurchaseOrderDeliveryLine” definition (CRM schema).
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="purchaseInvoiceLines" sme:label="FixedAsset Purchase Invoice Line" minOccurs="0" type="crmErp:purchaseInvoiceLine--list" sme:relationship="association" sme:isCollection="true" nillable="true">
        <xs:annotation>
          <xs:documentation>Purchase Invoice Line(s) associated with the Fixed Asset.
               
See “PurchaseInvoiceLine” definition (CRM schema).
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="purchaseReturnLines" sme:label="FixedAsset Purchase Return Line" minOccurs="0" type="crmErp:purchaseReturnLine--list" sme:relationship="association" sme:isCollection="true" nillable="true">
        <xs:annotation>
          <xs:documentation>Purchase Return Line(s) associated with the Fixed Asset.
               
See “PurchaseReturnLine” definition (CRM schema).
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="purchaseReturnDeliveryLines" sme:label="FixedAsset Purchase Return Delivery Line" minOccurs="0" type="crmErp:purchaseReturnDeliveryLine--list" sme:relationship="association" sme:isCollection="true" nillable="true">
        <xs:annotation>
          <xs:documentation>Purchase Return Delivery Line(s) associated with the Fixed Asset.
               
See “PurchaseReturnDeliveryLine” definition (CRM schema).
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="purchaseCreditLines" sme:label="FixedAsset Purchase Credit Line" minOccurs="0" type="crmErp:purchaseCreditLine--list" sme:relationship="association" sme:isCollection="true" nillable="true">
        <xs:annotation>
          <xs:documentation>Purchase Credit Line(s) associated with the Fixed Asset.
               
See “PurchaseCreditLine” definition (CRM schema).
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="useCommoditiesFlag" sme:label="FixedAsset Use Commodities Flag" minOccurs="0" type="xs:boolean" nillable="true">
        <xs:annotation>
          <xs:documentation>Flag to indicate if Fixed Asset is created from Commodities (see below).
               
Default = “False”
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="commodity" sme:label="FixedAsset Commodity" minOccurs="0" type="crmErp:commodity--list" sme:relationship="association" sme:isCollection="true" nillable="true">
        <xs:annotation>
          <xs:documentation>A link to the associated Commodity that created the Fixed Asset. Commodities are products/items that can be bought that can be turned into Fixed Assets.
               
See “Commodity” definition (CRM schema).
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="lease" sme:label="FixedAsset Lease" minOccurs="0" type="lease--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
        <xs:annotation>
          <xs:documentation>For Fixed Assets that are leased, this is a link to the lease.
               
See “Lease” definition.
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="grant" sme:label="FixedAsset Grant" minOccurs="0" type="grant--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
        <xs:annotation>
          <xs:documentation>For Fixed Assets that have a grant associated with them, this is a link to the grant.
               
See “Grant” definition.
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="project" sme:label="FixedAsset Project" minOccurs="0" type="crmErp:project--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
        <xs:annotation>
          <xs:documentation>Project associated with the Fixed Asset.
               
See “Project” definition (CRM schema).
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="unitOfMeasure" sme:label="FixedAsset Unit Of Measure" minOccurs="0" type="crmErp:unitOfMeasure--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
        <xs:annotation>
          <xs:documentation>A link to the associated unit of measure for the Fixed Asset. Units of measure cover each, pack sizes, etc.
               
See “Unit Of Measure” definition (CRM schema).
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="quantity" sme:label="FixedAsset Quantity" minOccurs="0" type="xs:decimal" nillable="true">
        <xs:annotation>
          <xs:documentation>The quantity of the Fixed Asset.
               
Default to 1
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="associatedChildFixedAsset" sme:label="FixedAsset Associated Child FixedAsset" minOccurs="0" type="fixedAsset--list" sme:relationship="child" sme:isCollection="true" nillable="true">
        <xs:annotation>
          <xs:documentation>Link to Child Fixed Asset associated with this Fixed Asset.
               
FixedAssets can be ‘nested’ to create FixedAsset hierarchies.
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="associatedParentFixedAsset" sme:label="FixedAsset Associated Parent FixedAsset" minOccurs="0" type="fixedAsset--type" sme:relationship="parent" sme:isCollection="false" nillable="true">
        <xs:annotation>
          <xs:documentation>Link to Parent Fixed Asset associated with this Fixed Asset.
               
FixedAssets can be ‘nested’ to create FixedAsset hierarchies.
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="cashGeneratingUnit" sme:label="FixedAsset Cash Generating Unit " minOccurs="0" type="xs:string" nillable="true">
        <xs:annotation>
          <xs:documentation>The cash generating unit associated with the Fixed Asset (IAS).
               

</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="businessUnit" sme:label="FixedAsset Business Unit" minOccurs="0" type="xs:string" nillable="true">
        <xs:annotation>
          <xs:documentation>The business unit associated with the Fixed Asset (IAS).
               

</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="geographicalUnit" sme:label="FixedAsset Geographical Unit" minOccurs="0" type="xs:string" nillable="true">
        <xs:annotation>
          <xs:documentation>The geographical unit associated with the Fixed Asset (IAS).
               

</xs:documentation>
