	<xs:element name="salesReturn" type="salesReturn--type" sme:role="resourceKind" sme:pluralName="salesReturns" sme:compliance="should" sme:canGet="true" sme:canPut="true" sme:canPost="true" sme:hasUuid="true" sme:isSyncSource="true"/>
