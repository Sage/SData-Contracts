          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dutyTotal" type="xs:decimal" nillable="true" minOccurs="0" sme:label="PurchaseInvoice Duty Total" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
