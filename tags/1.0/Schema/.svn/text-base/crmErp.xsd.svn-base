				</xs:annotation>
			</xs:element>
			<xs:element name="text2" type="xs:string" nillable="true" minOccurs="0" sme:label="PurchaseInvoice Text2">
				<xs:annotation>
					<xs:documentation>
            Text/commentary (e.g. special Delivery instructions) for the Purchase
            Invoice   Footer Text
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="projects" type="project--list" nillable="true" minOccurs="0" sme:label="PurchaseInvoice Project" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Identifier of the Project this Purchase Invoice is for.   See “Project”
            definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="projectLines" type="projectLine--list" nillable="true" minOccurs="0" sme:label="PurchaseInvoice ProjectLine" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
