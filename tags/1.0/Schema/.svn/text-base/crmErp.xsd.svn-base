          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="priority" type="interactionPriority--enum" nillable="true" minOccurs="0" sme:label="Interaction Priority">
				<xs:annotation>
					<xs:documentation>
            Priority of the Interaction e.g. High, Medium, Low  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tradingAccount" type="tradingAccount--type" nillable="true" minOccurs="0" sme:label="Interaction Customer Supplier TradingAccount" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
