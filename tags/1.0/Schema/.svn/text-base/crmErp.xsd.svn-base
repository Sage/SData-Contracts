			<xs:element name="purchaseOrderLine" type="purchaseOrderLine--type" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:anyAttribute/>
	</xs:complexType>
	<xs:element name="purchaseRequisition" type="purchaseRequisition--type" sme:role="resourceKind" sme:pluralName="purchaseRequisitions" sme:compliance="may" sme:canGet="true" sme:canPost="true"/>
	<xs:complexType name="purchaseRequisition--type">
		<xs:all>
			<xs:element name="active" type="xs:boolean" minOccurs="0" sme:label="PurchaseRequisition Active Flag">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate if the Purchase Requisition is an ‘active’ one .  
            Default = True
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference" type="xs:string" minOccurs="0" sme:label="PurchaseRequisition reference" sme:isUniqueKey="true">
				<xs:annotation>
					<xs:documentation> reference for the Purchase Requisition.   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference2" type="xs:string" nillable="true" minOccurs="0" sme:label="PurchaseRequisition Reference2" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Additional reference for the Purchase Requisition.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="status" type="purchaseRequisitionStatus--enum" nillable="true" minOccurs="0" sme:label="PurchaseRequisition Status">
				<xs:annotation>
					<xs:documentation> Status of the Purchase Requisition   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="statusFlag" type="xs:boolean" nillable="true" minOccurs="0" sme:label="PurchaseRequisition Status Flag">
				<xs:annotation>
					<xs:documentation>
            Additional Boolean status flag (e.g. Requisition on hold (or
            “blocked”), off hold).   True is on hold, False is not on hold.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="statusFlagText" type="xs:string" nillable="true" minOccurs="0" sme:label="PurchaseRequisition Status Flag Text">
				<xs:annotation>
					<xs:documentation>
            Purchase Requisition status flag text.   Default = “On Hold”
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tradingAccount" type="tradingAccount--type" nillable="true" minOccurs="0" sme:label="PurchaseRequisition Supplier TradingAccount" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Link to the Purchase Requisition Supplier trading account.   See
            “TradingAccount” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="sellerContact" type="contact--type" nillable="true" minOccurs="0" sme:label="PurchaseRequisition Seller Contact" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Link to the Contact at Supplier who is handling the Purchase Quotation.
              See “Contact” definition. Default = Primary TradingAccount Contact
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="supplierReference" type="xs:string" nillable="true" minOccurs="0" sme:label="PurchaseRequisition Supplier reference">
				<xs:annotation>
					<xs:documentation>
            Supplier own reference for the Purchase Requisition (e.g. a SO number).
             
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="pricelist" type="priceList--type" nillable="true" minOccurs="0" sme:label="PurchaseRequisition PriceList" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Link to the Price List used when generating the Purchase Requisition  
            See “PriceList” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customerTradingAccount" type="tradingAccount--type" nillable="true" minOccurs="0" sme:label="PurchaseRequisition Customer TradingAccount" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Link to the Purchase Requisition Customer trading account (e.g. if PO
            is for direct Delivery / drop ship to a Customer).   See “TradingAccount” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customerReference" type="xs:string" nillable="true" minOccurs="0" sme:label="PurchaseRequisition Customer reference">
				<xs:annotation>
					<xs:documentation>
            Customer own reference for the Purchase Requisition.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="type" type="xs:string" nillable="true" minOccurs="0" sme:label="PurchaseRequisition Type">
				<xs:annotation>
					<xs:documentation> The Purchase Requisition type.   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="copyFlag" type="xs:boolean" minOccurs="0" sme:label="PurchaseRequisition Copy Flag">
				<xs:annotation>
					<xs:documentation>
            Indicates if this Purchase Requisition is a copy or not.   Default is
            false (no)
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="originatorDocument" type="purchaseRequisition--type" nillable="true" minOccurs="0" sme:label="PurchaseRequisition Originator Document reference" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            reference to another Purchase Requisition if required (e.g. if
            Requisition is a copy, then this will show which Requisition was copied, if the
            Requisition is from a blanket Requisition, this will show the blanket Requisition
            details).   See “PurhaseOrder” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="postalAddresses" type="sc:postalAddress--list" nillable="true" minOccurs="0" sme:label="PurchaseRequisition PostalAddress" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Link to Postal Address(es) associated with the Purchase Requisition.  
            See “PostalAddress” definition. Could be multiple e.g. shipping, billing, etc
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deliveryTerms" type="xs:string" nillable="true" minOccurs="0" sme:label="PurchaseRequisition Delivery Terms">
				<xs:annotation>
					<xs:documentation> The terms for Delivery.   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deliveryDate" type="xs:date" nillable="true" minOccurs="0" sme:label="PurchaseRequisition Delivery Date">
				<xs:annotation>
					<xs:documentation> The anticipated date of the Delivery.   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="currency" type="xs:string" minOccurs="0" sme:label="PurchaseRequisition Currency">
				<xs:annotation>
					<xs:documentation>
            The currency of the Purchase Requisition (e.g. GBP, USD, EUR).   ISO
            4217.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operatingCompanyCurrency" type="xs:string" minOccurs="0" sme:label="PurchaseRequisition OperatingCompany Currency">
				<xs:annotation>
					<xs:documentation>
            The currency as used by OperatingCompany, the business operating the
            Sage application (the “base” currency).   ISO 4217.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operatingCompanyCurrencyExchangeRate" type="xs:decimal" nillable="true" minOccurs="0" sme:label="PurchaseRequisition OperatingCompany Currency Exchange Rate">
				<xs:annotation>
					<xs:documentation>
            Rate used to convert from Purchase Requisition Currency into Purchase
            Requisition OperatingCompany Currency, if different.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operatingCompanyCurrencyExchangeRateOperator" type="RateOperator--enum" nillable="true" minOccurs="0" sme:label="PurchaseRequisition OperatingCompany Currency Exchange Rate Operator">
				<xs:annotation>
					<xs:documentation>
            Operator to be used (multiplication or division) to convert from
            Purchase Requisition Currency to Purchase Requisition OperatingCompany Currency.   “*”
            (multiplication) or “/” division. E.g. if the operator is division then it is Purchase
            Requisition Values in Purchase Requisition Currency / Purchase Requisition Operating
            Company Currency Exchange Rate = Purchase Requisition Values in Purchase Requisition
            OperatingCompany currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operatingCompanyCurrencyExchangeRateDate" type="xs:date" nillable="true" minOccurs="0" sme:label="PurchaseRequisition OperatingCompany Currency Exchange Rate Date">
				<xs:annotation>
					<xs:documentation>
            Date of the exchange rate used to convert from Purchase Requisition
            Currency to Purchase Requisition OperatingCompany Currency, if different.   Default =
            System date
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="supplierTradingAccountCurrency" type="xs:string" nillable="true" minOccurs="0" sme:label="PurchaseRequisition Supplier TradingAccount Currency">
