          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customerTradingAccountCurrencyExchangeRate" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesOrderDelivery Customer TradingAccount Currency Exchange Rate">
