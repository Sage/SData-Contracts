				</xs:annotation>
			</xs:element>
			<xs:element name="notes" type="sc:note--list" nillable="true" minOccurs="0" sme:label="PaymentLine Note" sme:relationship="child" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation> Notes associated with the Payment Line.   </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
		<xs:anyAttribute/>
	</xs:complexType>
	<xs:complexType name="paymentLine--list">
