			<xs:element name="carrierTradingAccount" type="tradingAccount--type" nillable="true" minOccurs="0" sme:label="PurchaseCreditLine Carrier TradingAccount" sme:relationship="reference" sme:isCollection="false" sme:isExtension="true">
