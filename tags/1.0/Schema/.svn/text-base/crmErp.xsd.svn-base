				<xs:annotation>
					<xs:documentation>
            Link to the Contact at Customer who placed the Sales Return.   See
            “Contact” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="salespersons" type="salesPerson--list" nillable="true" minOccurs="0" sme:label="SalesReturn Salesperson" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Sales person associated with the Sales Return.   See “SalesPerson”
            definition. Also known as Sales Agent or Manager in some applications. There may be
            multiple and commission may be spread across multiple sales people.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="salesReturnLines" type="salesReturnLine--list" minOccurs="0" sme:label="SalesReturn SalesReturnLine" sme:relationship="child" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Sales Return Line detail (e.g. items on the Sales Return)   See
            “SalesReturnLine” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="lineCount" type="xs:decimal" minOccurs="0" sme:label="SalesReturn Line Count">
				<xs:annotation>
					<xs:documentation>
            The number of lines on the Sales Return.   Should be auto-populated.
            Used as a cross reference to ensure Return integrity.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="returnDiscountType" type="discountType--enum" nillable="true" minOccurs="0" sme:label="SalesReturn Return Discount Type">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate the type of discount applicable to this Sales Return
            (percent or amount)   From the Trading Account.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="returnDiscountAmount" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesReturn Return Discount Amount">
				<xs:annotation>
					<xs:documentation>
            Discount applicable to the Sales Return expressed as an amount.   From
            the Trading Account.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="returnDiscountPercent" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesReturn Return Discount Percent">
				<xs:annotation>
					<xs:documentation>
            Discount applicable to the Sales Return expressed as a percent.   From
            the Trading Account.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="returnAdditionalDiscount1Type" type="discountType--enum" nillable="true" minOccurs="0" sme:label="SalesReturn Return Additional Discount1 Type">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate the type of discount applicable to this Sales Return
            (percent or amount)  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="returnAdditionalDiscount1Amount" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesReturn Return Additional Discount1 Amount">
				<xs:annotation>
					<xs:documentation>
            Discount applicable to the Sales Return expressed as an amount.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="returnAdditionalDiscount1Percent" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesReturn Return Additional Discount1 Percent">
				<xs:annotation>
					<xs:documentation>
            Discount applicable to the Sales Return expressed as a percent.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="returnAdditionalDiscount2" type="discountType--enum" nillable="true" minOccurs="0" sme:label="SalesReturn Return Additional Discount2 Type" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate the type of discount applicable to this Sales Return
            (percent or amount)  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="returnAdditionalDiscount2Amount" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesReturn Return Additional Discount2 Amount" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Discount applicable to the Sales Return expressed as an amount.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="returnAdditionalDiscount2Percent" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesReturn Return Additional Discount2 Percent" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Discount applicable to the Sales Return expressed as a percent.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="text1" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesReturn Text1">
				<xs:annotation>
					<xs:documentation>
            Text/commentary (e.g. special Delivery instructions) for the Sales
            Return   Header text.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="text2" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesReturn Text2">
				<xs:annotation>
					<xs:documentation>
            Further text/commentary for the Sales Return.   Footer text.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="netTotal" type="xs:decimal" minOccurs="0" sme:label="SalesReturn Net Total">
				<xs:annotation>
					<xs:documentation>
            Total value of the Sales Return, including discounts, excluding tax.  
            In Return currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="discountTotal" type="xs:decimal" minOccurs="0" sme:label="SalesReturn Discount Total">
				<xs:annotation>
					<xs:documentation>
            Total discounts applied to the Return   In Return currency
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="chargesTotal" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesReturn Charges Total">
				<xs:annotation>
					<xs:documentation>
            Total value of any (sur)charges applied to the Sales Return (e.g.
            finance charges, interest).   In Return currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxTotal" type="xs:decimal" minOccurs="0" sme:label="SalesReturn Tax Total">
				<xs:annotation>
					<xs:documentation>
            Total tax applicable to the Sales Return.   In Return currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="grossTotal" type="xs:decimal" minOccurs="0" sme:label="SalesReturn Gross Total">
				<xs:annotation>
					<xs:documentation>
            Total value of the Sales Return, including tax &amp; discounts
            &amp; charges.   In Return currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="costTotal" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesReturn Cost Total">
				<xs:annotation>
					<xs:documentation>
            Total cost of the Sales Return.   In Return currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="profitTotal" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesReturn Profit Total">
				<xs:annotation>
					<xs:documentation>
            Total profit on the Sales Return (e.g. net total – cost total).   In
            Return currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="payments" type="payment--list" nillable="true" minOccurs="0" sme:label="SalesReturn Payment" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Link to the Sales Return Payment (e.g. if refund is entered with the
            Return).   See “Payment” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="contract" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesReturn Contract" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            reference to a contract associated with the Sales Return.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="creditTradingAccount" type="tradingAccount--type" nillable="true" minOccurs="0" sme:label="SalesReturn Credit TradingAccount" sme:relationship="reference" sme:isCollection="false" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Link to the Trading Account to be credited for the Sales Return (if
            different from the Customer).   See “TradingAccount” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="returnCountry" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesReturn Return Country">
				<xs:annotation>
					<xs:documentation>
            Country of destination for the Sales Return (e.g. for customs
            purposes).   ISO 3166-1 alpha-2 country codes only Could be derived from Delivery
            address?
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="projects" type="project--list" nillable="true" minOccurs="0" sme:label="SalesReturn Project" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Identifier of the Project this Sales Return is for.   See “Project”
            definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="projectLines" type="projectLine--list" nillable="true" minOccurs="0" sme:label="SalesReturn ProjectLine" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Project Line value for this Sales Return.   See “Project Line”
            definition. Some applications may not have Project Lines.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="cases" type="case--list" nillable="true" minOccurs="0" sme:label="SalesReturn Case reference" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            The reference of the ‘case’ associated with the Sales Return e.g. if
            the Return is for repair/fix services.   A ‘Case’ entity hase been defined to support
            the F2F process.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="salesReturnDeliveries" type="salesReturnDelivery--list" nillable="true" minOccurs="0" sme:label="SalesReturn SalesReturnDelivery" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Link to the Delivery (dispatch) against the Sales Return. If there are
            multiple then these are held against the Sales Return Line.   See
            “SalesReturnDeliveries” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="financialAccounts" type="sc:financialAccount--list" nillable="true" minOccurs="0" sme:label="SalesReturn FinancialAccount" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Financial accounting code applicable to the Sales Return.   See
            “FinancialAccount” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="costCentres" type="sc:costCentre--list" nillable="true" minOccurs="0" sme:label="SalesReturn CostCentres" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="departments" type="sc:department--list" nillable="true" minOccurs="0" sme:label="SalesReturn Departments" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="interactions" type="interaction--list" nillable="true" minOccurs="0" sme:label="SalesReturn Interaction" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Interaction(s) associated with the Sales Return.   See “Interaction”
            definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="user" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesReturn User" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Link to the user who placed the Sales Return.   The ‘user’ resource
            will be defined in the future.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="notes" type="sc:note--list" nillable="true" minOccurs="0" sme:label="SalesReturn Note" sme:relationship="child" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation> Notes associated with a Sales Return.   </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
		<xs:anyAttribute/>
	</xs:complexType>
	<xs:complexType name="salesReturn--list">
		<xs:sequence>
			<xs:element name="salesReturn" type="salesReturn--type" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:anyAttribute/>
	</xs:complexType>
	<xs:element name="salesReturnLine" type="salesReturnLine--type" sme:role="resourceKind" sme:pluralName="salesReturnLines" sme:compliance="should" sme:canGet="true" sme:canPut="true" sme:canPost="true" sme:hasUuid="true" sme:isSyncSource="true"/>
	<xs:complexType name="salesReturnLine--type">
		<xs:all>
			<xs:element name="active" type="xs:boolean" minOccurs="0" sme:label="SalesReturnLine Active Flag">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate if the Sales Return Line is an ‘active’ one .  
            Default = True
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference" type="xs:string" minOccurs="0" sme:label="SalesReturnLine reference" sme:isUniqueKey="true">
				<xs:annotation>
					<xs:documentation> reference for the Sales Return Line.   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference2" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesReturnLine Reference2" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation> Additional reference for the Sales Return Line.   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="number" type="xs:decimal" minOccurs="0" sme:label="SalesReturnLine Number">
				<xs:annotation>
					<xs:documentation>
            The number of the Sales Return Line.   For sequencing and ordering of
            lines.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="status" type="salesReturnStatus--enum" nillable="true" minOccurs="0" sme:label="SalesReturnLine Status">
				<xs:annotation>
					<xs:documentation> Status of the Sales Return Line.   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="type" type="lineType--enum" nillable="true" minOccurs="0" sme:label="SalesReturnLine Type" sme:isMandatory="true">
				<xs:annotation>
					<xs:documentation>
            Enumerator indicating the type of Sales Return Line.   Allowable
            values: Standard Line; Free text (with price); Commentary (without price, e.g. “thank
            you for your business!”); Component (with price but not in total); Discount/charge
            (included price/total)
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reason" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesReturnLine Reason">
				<xs:annotation>
					<xs:documentation>
            Reason for the Sales Return Line.   The return reason may be different
            for each Line.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deliveryTerms" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesReturnLine Delivery Terms">
				<xs:annotation>
					<xs:documentation>
            The terms for Delivery for this Sales Return Line.   Also on Sales
            Return.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierTradingAccount" type="tradingAccount--type" nillable="true" minOccurs="0" sme:label="SalesReturnLine Carrier TradingAccount" sme:relationship="reference" sme:isCollection="false" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Link to the Trading Account associated with the Supplier that wll be
            the carrier / freight forwarder / deliverer of the Sales Return Line.   See
            “TradingAccount” definition. Carriers are not used by all application.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierNetPrice" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesReturnLine Carrier Net Price">
				<xs:annotation>
					<xs:documentation>
            The price of Delivery/carriage.   In Return currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierTaxPrice" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesReturnLine Carrier Tax Price">
				<xs:annotation>
					<xs:documentation> The tax on Delivery/carriage.   In Return currency. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierTotalPrice" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesReturnLine Carrier Gross Price">
				<xs:annotation>
					<xs:documentation>
            The total price of Delivery/carriage (net + VAT).   In Return currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierSalesInvoiceLine" type="salesInvoice--type" nillable="true" minOccurs="0" sme:label="SalesReturnLine Carrier SalesInvoice" sme:relationship="reference" sme:isCollection="false" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            A link to the Sales Invoice generated to recharge the carriage to the
            Customer.   See “SalesInvoice” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierPurchaseInvoiceLine" type="purchaseInvoice--type" nillable="true" minOccurs="0" sme:label="SalesReturnLine Carrier PurchaseInvoice" sme:relationship="reference" sme:isCollection="false" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            A link to the purchase Invoice charged by the carrier Company for the
            Delivery back from the Customer.   See “PurchaseInvoice” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierTaxCodes" type="taxCode--list" nillable="true" minOccurs="0" sme:label="SalesReturnLine Carrier Tax Code" sme:relationship="association" sme:isCollection="true" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Tax code governing the tax regime applicable to the carriage on this
            Sales Return Line.   See “TaxCode” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierReference" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesReturnLine Carrier reference">
				<xs:annotation>
					<xs:documentation>
            reference number as used by the carrier during Delivery e.g. for
            Delivery tracking.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="commodity" type="commodity--type" nillable="true" minOccurs="0" sme:label="SalesReturnLine Commodity" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Link to the Commodity used on the Sales Return Line.   See “Commodity”
            definition. Not mandatory as Line may be a free text or comment Line.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="text" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesReturnLine Text">
				<xs:annotation>
					<xs:documentation>
            Text applicable to the Sales Return Line (e.g. the Line may be a
            comment Line or a free text Line). If the Sales Return Line is for a Commodity then the
            Commodity description should be held here.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="commodityVariant" type="commodityVariant--type" nillable="true" minOccurs="0" sme:label="SalesReturnLine Commodity Variant" sme:relationship="reference" sme:isCollection="false" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            The Variant of the Commodity (e.g. colour or size) on the Sales Return
            Line.   See “Variant” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="commodityDimension" type="commodityDimension--type" nillable="true" minOccurs="0" sme:label="SalesReturnLIne Commodity Dimension" sme:relationship="reference" sme:isCollection="false" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            The Dimension of the Commodity on the Sales Return Line.   See
            “Dimension” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="commodityCondition" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesReturnLine Commodity Condition">
				<xs:annotation>
					<xs:documentation>
            Condition of the Commodity being returned on the Sales Return Line.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fulfillmentLocation" type="location--type" nillable="true" minOccurs="0" sme:label="SalesReturnLine Delivery Location" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            The Location to which the Sales Return Line will be delivered (e.g. the
            warehouse holding the Commodity).   See “Location” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="unitOfMeasure" type="unitOfMeasure--type" minOccurs="0" sme:label="SalesReturnLine UnitOfMeasure" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            A link to the scale to be used in the measurement of physical
            quantities of Commodity. Examples include "Each", "Box of 10", etc. May also be called
            "Pack Size".   See “UnitOfMeasure” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="quantity" type="xs:decimal" minOccurs="0" sme:label="SalesReturnLine Quantity" sme:isMandatory="true">
				<xs:annotation>
					<xs:documentation>
            The quantity of the Commodity Returned on the Sales Return Line.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="initialPrice" type="xs:decimal" minOccurs="0" sme:label="SalesReturnLine Initial Price">
				<xs:annotation>
					<xs:documentation>
            The initial price of the Commodity Returned on the Sales Return Line
            (e.g. before discounts).   In Return currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="pricelist" type="priceList--type" nillable="true" minOccurs="0" sme:label="SalesReturnLine PriceList" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            A reference to the Price List used to derive the price on the Sales
            Return Line.   See “PriceList” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="returnLineDiscountType" type="discountType--enum" nillable="true" minOccurs="0" sme:label="SalesReturnLine Return Line Discount Type">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate the type of discount applicable to this Sales Return
            Line (percent or amount).   From the Sales Return.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="returnLineDiscountAmount" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesReturnLine Return Line Discount Amount">
				<xs:annotation>
					<xs:documentation>
            Discount applicable to the Sales Return Line, expressed as an amount.  
            From the Sales Return.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="returnLineDiscountPercent" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesReturnLine Return Line Discount Percent">
				<xs:annotation>
					<xs:documentation>
            Discount applicable to the Sales Return Line, expressed as a
            percentage.   From the Sales Return.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="returnLineSubtotalDiscountType" type="discountType--enum" nillable="true" minOccurs="0" sme:label="SalesReturnLine Return Line Subtotal Discount Type" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate the type of discount applicable to a subtotal of Sales
            Return Lines (percent or amount) of which this Line is part.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="returnLineSubtotalDiscountAmount" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesReturnLine Return Line Subtotal Discount Amount" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Discount applicable to the Sales Return Line, expressed as an amount.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="returnLineSubtotalDiscountPercent" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesReturnLine Return Line Subtotal Discount Percent" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Discount applicable to the Sales Return Line, expressed as a
            percentage.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="actualPrice" type="xs:decimal" minOccurs="0" sme:label="SalesReturnLine Actual Price">
				<xs:annotation>
					<xs:documentation>
            The actual price of the Commodity Returned on the Sales Return Line
            (e.g. after discounts or adjustments).   In Return currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="netTotal" type="xs:decimal" minOccurs="0" sme:label="SalesReturnLine Net Total">
				<xs:annotation>
					<xs:documentation>
            Total value of the Sales Return Line, including discounts, excluding
            tax (e.g. quantity * actual price)   In Return currency. Could be calculated.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="chargesTotal" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesReturnLine Charges Total">
				<xs:annotation>
					<xs:documentation>
            Total value of any (sur)charges applied to the Sales Return Line (e.g.
            finance charges, interest).   In Return currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="discountTotal" type="xs:decimal" minOccurs="0" sme:label="SalesReturnLine Discount Total">
				<xs:annotation>
					<xs:documentation>
            Total discounts applied to the return line   In return currency
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fOBPoint" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesReturnLine FOB Point">
				<xs:annotation>
					<xs:documentation>
            The FOB point applicable to the Sales Return Line. FOB is an
            abbreviation for Free On Board. The term FOB is used when shipping goods, to indicate
            who pays loading and transportation costs, and/or the point at which the responsibility
            of the goods transfers from shipper to buyer  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxCodes" type="taxCode--list" nillable="true" minOccurs="0" sme:label="SalesReturnLine Tax Code" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Tax code governing the tax regime applicable to this Sales Return Line
            (default value from Sales Return).   See “TaxCode” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="priceTax" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesReturnLine Price Tax">
				<xs:annotation>
					<xs:documentation>
            Tax applicable to the Sales Return Line price e.g. the per unit tax.  
            Both unit-tax and Line-tax values are required due to the way tax is handled differently
            by product/country.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxTotal" type="xs:decimal" minOccurs="0" sme:label="SalesReturnLine Tax Total">
				<xs:annotation>
					<xs:documentation>
            Tax applicable to the Sales Return Line.   In Return currency. Could be
            calculated. Both unit-tax and Line-tax values are required due to the way tax is handled
            differently by product/country.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="grossTotal" type="xs:decimal" minOccurs="0" sme:label="SalesReturnLine Gross Total">
				<xs:annotation>
					<xs:documentation>
            Total value of the Sales Return Line, including discounts &amp; tax
            &amp; charges.   In Return currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="costTotal" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesReturnLine Cost Total">
				<xs:annotation>
					<xs:documentation>
            Total cost of the Sales Return Line.   In Return currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="profitTotal" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesReturnLine Profit Total">
				<xs:annotation>
					<xs:documentation>
            Total profit on the Sales Return Line (e.g. net total – cost total).  
            In Return currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="salesCreditLines" type="salesCreditLine--list" nillable="true" minOccurs="0" sme:label="SalesReturnLine SalesCreditLine" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Sales Credit Line(s) associated with the Sales Return Line.   See
            “SalesCreditLine” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="payments" type="payment--list" nillable="true" minOccurs="0" sme:label="SalesReturnLine Payment" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            A link to the (cash) payment(s) associated with this Sales Return Line
              See “Receipt” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="paymentLines" type="paymentLine--list" nillable="true" minOccurs="0" sme:label="SalesReturnLine PaymentLine" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            A link to the (cash) payment line(s) associated with this Sales Return
            Line   See “ReceiptLine” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="buyerContact" type="contact--type" nillable="true" minOccurs="0" sme:label="SalesReturnLine Buyer Contact" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Link to the Sales Return Line originator (buyer Contact).   See
            “Contact” definition. This could be derived from the Sales Return
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="project" type="project--type" nillable="true" minOccurs="0" sme:label="SalesReturnLine Project" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Identifier of the Project this Sales Return Line is for.   See
            “Project” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="projectLine" type="projectLine--type" nillable="true" minOccurs="0" sme:label="SalesReturnLine ProjectLine" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Project Line value for this Sales Return Line.   See “Project Line”
            definition. Some applications may not have Project Lines.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="salesReturnDeliveryLines" type="salesReturnDeliveryLine--list" nillable="true" minOccurs="0" sme:label="SalesReturnLine SalesReturnDeliveryLines" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Link to the Delivery (dispatch) Line against the Sales Return Line.  
            See “SalesReturnDeliveryLines” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="financialAccounts" type="sc:financialAccount--list" nillable="true" minOccurs="0" sme:label="SalesReturnLine FinancialAccount" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Financial accounting code applicable to the Sales Return Line.   See
            “FinancialAccount” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="costCentres" type="sc:costCentre--list" nillable="true" minOccurs="0" sme:label="SalesReturnLine CostCentres" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="departments" type="sc:department--list" nillable="true" minOccurs="0" sme:label="SalesReturnLine Departments" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="notes" type="sc:note--list" nillable="true" minOccurs="0" sme:label="SalesReturnLine Note" sme:relationship="child" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation> Notes associated with a Sales Return Line.   </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
		<xs:anyAttribute/>
	</xs:complexType>
	<xs:complexType name="salesReturnLine--list">
		<xs:sequence>
			<xs:element name="salesReturnLine" type="salesReturnLine--type" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:anyAttribute/>
	</xs:complexType>
	<xs:element name="salesOrderDelivery" type="salesOrderDelivery--type" sme:role="resourceKind" sme:pluralName="salesOrderDeliveries" sme:compliance="may" sme:canGet="true"/>
	<xs:complexType name="salesOrderDelivery--type">
		<xs:all>
			<xs:element name="active" type="xs:boolean" minOccurs="0" sme:label="SalesOrderDelivery Active Flag">
