          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="quotationDiscountTotal" type="xs:decimal" nillable="true" minOccurs="0" sme:label="Opportunity Quotation Discount Total">
				<xs:annotation>
					<xs:documentation>
