				</xs:annotation>
			</xs:element>
			<xs:element name="financialAccounts" type="sc:financialAccount--list" nillable="true" minOccurs="0" sme:label="PurchaseInvoice FinancialAccount" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Financial accounting code applicable to the Purchase Invoice.   See
            “FinancialAccount” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="costCentres" type="sc:costCentre--list" nillable="true" minOccurs="0" sme:label="PurchaseInvoice CostCentres" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="departments" type="sc:department--list" nillable="true" minOccurs="0" sme:label="PurchaseInvoice Departments" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="interactions" type="interaction--list" nillable="true" minOccurs="0" sme:label="PurchaseInvoice Interaction" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
