	<xs:element name="salesOrder" type="salesOrder--type" sme:role="resourceKind" sme:pluralName="salesOrders" sme:compliance="should" sme:canGet="true" sme:canPut="true" sme:canPost="true" sme:hasUuid="true" sme:isSyncSource="true" sme:isSyncTarget="true"/>
	<xs:complexType name="salesOrder--type">
		<xs:all>
			<xs:element name="active" type="xs:boolean" minOccurs="0" sme:label="SalesOrder Active Flag">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate if the Sales Order is an ‘active’ one .   Default =
            True
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference" type="xs:string" minOccurs="0" sme:label="SalesOrder reference" sme:isUniqueKey="true">
				<xs:annotation>
					<xs:documentation> reference for the Sales Order.   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference2" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesOrder Reference2" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation> Additional reference for the Sales Order.   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="status" type="salesOrderStatus--enum" nillable="true" minOccurs="0" sme:label="SalesOrder Status">
				<xs:annotation>
					<xs:documentation> Status of the Sales Order   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="allocationStatus" type="salesOrderAllocationStatus--enum" nillable="true" minOccurs="0" sme:label="SalesOrder Allocation Status" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Status of Commodity allocations against the order.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deliveryStatus" type="salesOrderDeliveryStatus--enum" nillable="true" minOccurs="0" sme:label="SalesOrder Delivery Status" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation> Delivery status of the order.   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceStatus" type="salesOrderInvoiceStatus--enum" nillable="true" minOccurs="0" sme:label="SalesOrder Invoice Status" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation> Invoice status of the order.   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="statusFlag" type="xs:boolean" nillable="true" minOccurs="0" sme:label="SalesOrder Status Flag">
				<xs:annotation>
					<xs:documentation>
            Additional Boolean status flag (e.g. order on hold (or “blocked”), off
            hold).   True is on hold, False is not on hold. Default value is False.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="statusFlagText" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesOrder Status Flag Text">
				<xs:annotation>
					<xs:documentation> Sales Order status flag text.   Default = “On Hold” </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tradingAccount" type="tradingAccount--type" minOccurs="0" sme:label="SalesOrder Customer TradingAccount" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Link to the Sales Order Customer account.   See “TradingAccount”
            definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customerReference" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesOrder Customer reference">
				<xs:annotation>
					<xs:documentation>
            Customer own reference for the Sales Order (e.g. a PO number).  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="opportunity" type="opportunity--type" nillable="true" minOccurs="0" sme:label="SalesOrder Opportunity" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Opportunity linked to the Sales Order (if any).   See “Opportunity”
            definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="pricelist" type="priceList--type" nillable="true" minOccurs="0" sme:label="SalesOrder PriceList" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Link to the Price List used when generating the Sales Order   See
            “PriceList” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="supplierTradingAccount" type="tradingAccount--type" nillable="true" minOccurs="0" sme:label="SalesOrder Supplier TradingAccount" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Link to the Sales Order Supplier account (e.g. for back to back,
            contremarque, drop-ship ordering).   See “TradingAccount” definition. These are also
            stored on SalesOrderLines
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="supplierReference" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesOrder Supplier reference">
				<xs:annotation>
					<xs:documentation> Supplier own reference for the Sales Order.   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="supplierPurchaseOrders" type="purchaseOrder--list" nillable="true" minOccurs="0" sme:label="SalesOrder Supplier PurchaseOrder" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Link to any Supplier Purchase Orders used to fulfill the Sales Order
            (back to back or drop-ship).   See “PurchaseOrder” definition. These are also stored on
            SalesOrderLines
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="type" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesOrder Type">
				<xs:annotation>
					<xs:documentation> The Sales Order type.   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="copyFlag" type="xs:boolean" minOccurs="0" sme:label="SalesOrder Copy Flag">
				<xs:annotation>
					<xs:documentation>
            Indicates if this Sales Order is a copy or not.   Default is false (no)
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="originatorDocument" type="salesOrder--type" nillable="true" minOccurs="0" sme:label="SalesOrder Originator Document reference" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            reference to another Sales Order if required (e.g. if order is a copy,
            then this will show which order was copied, If this is a replacement order this will
            show the original order).
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="quotation" type="salesQuotation--type" nillable="true" minOccurs="0" sme:label="SalesOrder Quotation" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            A link to the quotation used to create this Sales Order (if any).   See
            “Quotation” definition. Must be a valid quotation.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="postalAddresses" type="sc:postalAddress--list" nillable="true" minOccurs="0" sme:label="SalesOrder PostalAddress" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Link to Postal Address(es) associated with the Sales Order.   See
            “PostalAddress” definition. Could be multiple e.g. shipping, billing, etc These are also
            stored on SalesOrderLines
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deliveryMethod" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesOrder Delivery Method">
				<xs:annotation>
					<xs:documentation>
            Delivery method to be employed for Sales Order (e.g. Courier,
            Collection ,etc)  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deliveryRule" type="xs:boolean" nillable="true" minOccurs="0" sme:label="SalesOrder Delivery Rule">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate if the order has to be delivered in full or can be
            split across partial deliveries.   True = Partial deliveries allowed; False = Partial
            deliveries not allowed. Default = True
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deliveryTerms" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesOrder Delivery Terms">
				<xs:annotation>
					<xs:documentation> The terms for Delivery.   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deliveryDate" type="xs:date" nillable="true" minOccurs="0" sme:label="SalesOrder Delivery Date">
				<xs:annotation>
					<xs:documentation>
            The anticipated date of the Delivery. W3C date time standard for XML
            (ISO 8601). The base timezone is UTC. The offset is included. UTC should be used See the
            reference here: http://www.w3.org/TR/xmlschema-2/#isoformats  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dueDate" type="xs:date" nillable="true" minOccurs="0" sme:label="SalesOrder Due Date">
				<xs:annotation>
					<xs:documentation>
            Date the order is due by the Customer. W3C date time standard for XML
            (ISO 8601). The base timezone is UTC. The offset is included. UTC should be used See the
            reference here: http://www.w3.org/TR/xmlschema-2/#isoformats   Delivery date may be
            different to the Customer due date.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierCompany" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesOrder Carrier Company">
				<xs:annotation>
					<xs:documentation>
            Link to the Company that will be the carrier / freight forwarder/
            deliverer of the Sales Order   See “Company” definition. Carriers are not used by all
            application.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierTradingAccount" type="tradingAccount--list" nillable="true" minOccurs="0" sme:label="SalesOrder Carrier TradingAccount" sme:relationship="association" sme:isCollection="true" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Link to the Trading Account associated with the Supplier that wll be
            the carrier / freight forwarder / deliverer of the Sales Order.   See “TradingAccount”
            definition. Carriers are not used by all application.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierNetPrice" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesOrder Carrier Net Price">
				<xs:annotation>
					<xs:documentation>
            The price of Delivery/carriage.   In Order currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierTaxPrice" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesOrder Carrier Tax Price">
				<xs:annotation>
					<xs:documentation> The tax on Delivery/carriage.   In Order currency. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierTotalPrice" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesOrder Carrier Gross Price">
				<xs:annotation>
					<xs:documentation>
            The total price of Delivery/carriage (net + tax).   In Order currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierSalesInvoice" type="salesInvoice--list" nillable="true" minOccurs="0" sme:label="SalesOrder Carrier SalesInvoice" sme:relationship="association" sme:isCollection="true" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            A link to the Sales Invoice generated to recharge the carriage to the
            Customer.   See “SalesInvoice” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierPurchaseInvoice" type="purchaseInvoice--list" nillable="true" minOccurs="0" sme:label="SalesOrder Carrier PurchaseInvoice" sme:relationship="association" sme:isCollection="true" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            A link to the purchase Invoice charged by the carrier Company for the
            Delivery to the Customer.   See “PurchaseInvoice” defi nition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierTaxCodes" type="taxCode--list" nillable="true" minOccurs="0" sme:label="SalesOrder Carrier Tax Code" sme:relationship="association" sme:isCollection="true" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Tax code governing the tax regime applicable to the carriage on this
            Sales Order.   See “TaxCode” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierReference" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesOrder Carrier reference">
				<xs:annotation>
					<xs:documentation>
            reference number as used by the carrier during Delivery e.g. for
            Delivery tracking.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="currency" type="xs:string" minOccurs="0" sme:label="SalesOrder Currency">
				<xs:annotation>
					<xs:documentation>
            The currency of the Sales Order (e.g. GBP, USD, EUR).   ISO 4217.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operatingCompanyCurrency" type="xs:string" minOccurs="0" sme:label="SalesOrder OperatingCompany Currency">
				<xs:annotation>
					<xs:documentation>
            The currency as used by OperatingCompany, the business operating the
            Sage application (the “base” currency).   ISO 4217.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operatingCompanyCurrencyExchangeRate" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesOrder OperatingCompany Currency Exchange Rate">
				<xs:annotation>
					<xs:documentation>
            Rate used to convert from Sales Order Currency into Sales Order
            OperatingCompany Currency, if different.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operatingCompanyCurrencyExchangeRateOperator" type="RateOperator--enum" nillable="true" minOccurs="0" sme:label="SalesOrder OperatingCompany Currency Exchange Rate Operator">
				<xs:annotation>
					<xs:documentation>
            Operator to be used (multiplication or division) to convert from Sales
            Order Currency to Sales Order OperatingCompany Currency.   “*” (multiplication) or “/”
            division. E.g. if the operator is division then it is Sales Order Values in Sales Order
            Currency / Sales Order Operating Company Currency Exchange Rate = Sales Order Values in
            Sales Order OperatingCompany currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operatingCompanyCurrencyExchangeRateDate" type="xs:date" nillable="true" minOccurs="0" sme:label="SalesOrder OperatingCompany Currency Exchange Rate Date">
				<xs:annotation>
					<xs:documentation>
            Date of the exchange rate used to convert from Sales Order Currency to
            Sales Order OperatingCompany Currency, if different.   Default = System date
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceCurrency" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesOrder Invoice Currency" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            The currency to be used on invoices associated with this order.   ISO
            4217. Could be derived from order currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceCurrencyExchangeRate" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesOrder Invoice Currency Exchange Rate" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Rate used to convert from Sales Order Currency into Sales Order Invoice
            Currency, if different.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceCurrencyExchangeRateOperator" type="RateOperator--enum" nillable="true" minOccurs="0" sme:label="SalesOrder Invoice Currency Exchange Rate Operator" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Operator to be used (multiplication or division) to convert from Sales
            Order Currency to Sales Order Invoice Currency   “*” (multiplication) or “/” division.
            E.g. if the operator is division then it is Sales Order Values in Sales Order Currency /
            Sales Order Invoice Currency Exchange Rate = Sales Order Values in Sales Order Invoice
            Currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceCurrencyExchangeRateDate" type="xs:date" nillable="true" minOccurs="0" sme:label="SalesOrder Invoice Currency Exchange Rate Date" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Date of the exchange rate used to convert from Sales Order Currency to
            Sales Order Invoice Currency, if different.   Default = System date
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customerTradingAccountCurrency" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesOrder Customer TradingAccount Currency">
				<xs:annotation>
					<xs:documentation>
            The currency (e.g. GBP, USD, EUR) used by the Customer Trading Account
            on the Sales Order.   ISO 4217. Could be derived from order currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customerTradingAccountCurrencyExchangeRate" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesOrder Customer TradingAccount Currency Exchange Rate">
				<xs:annotation>
					<xs:documentation>
            Rate used to convert the Sales Order currency into the Customer Trading
            Account currency, if different.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customerTradingAccountCurrencyExchangeRateOperator" type="RateOperator--enum" nillable="true" minOccurs="0" sme:label="SalesOrder Customer TradingAccount Currency Exchange Rate Operator">
				<xs:annotation>
					<xs:documentation>
            Operator to be used (multiplication or division) to convert from Sales
            Order Currency to Sales Order Customer Trading Account Currency   “*” (multiplication)
            or “/” division. E.g. if the operator is division then it is Sales Order Values in Sales
            Order Currency / Sales Order Customer TradingAccount Currency Exchange Rate = Sales
            Order Values in Sales Order Customer TradingAccount Currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customerTradingAccountCurrencyExchangeRateDate" type="xs:date" nillable="true" minOccurs="0" sme:label="SalesOrder Customer TradingAccount Currency Exchange Rate Date">
				<xs:annotation>
					<xs:documentation>
            Date of the exchange rate used to convert from Sales Order Currency to
            Customer Trading Account Currency, if different.   Default = System date
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="date" type="xs:date" minOccurs="0" sme:label="SalesOrder Date">
				<xs:annotation>
					<xs:documentation> Date the Sales Order was placed.   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="time" type="xs:time" nillable="true" minOccurs="0" sme:label="SalesOrder Time">
				<xs:annotation>
					<xs:documentation>
            Time the Sales Order was placed. W3C date time standard for XML (ISO
            8601). The base timezone is UTC. The offset is included. UTC should be used See the
            reference here: http://www.w3.org/TR/xmlschema-2/#isoformats  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="validity" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesOrder Validity">
				<xs:annotation>
					<xs:documentation>
            Period the Sales Order is valid for (days from Order Date).  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="expirationDate" type="xs:date" nillable="true" minOccurs="0" sme:label="SalesOrder Expiration Date">
				<xs:annotation>
					<xs:documentation>
            The expiration date of the Sales Order (SalesOrder Date + Validity).  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxCodes" type="taxCode--list" nillable="true" minOccurs="0" sme:label="SalesOrder Tax Code" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Tax code governing the tax regime applicable to this Sales Order.   See
            “TaxCode” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="buyerContact" type="contact--type" nillable="true" minOccurs="0" sme:label="SalesOrder Buyer Contact" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Link to the Contact at Customer who placed the Sales Order.   See
            “Contact” definition. Default = Primary TradingAccount Contact
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="salesPersons" type="salesPerson--list" nillable="true" minOccurs="0" sme:label="SalesOrder Salesperson" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Sales person associated with the Sales Order.   See “SalesPerson”
            definition. Also known as Sales Agent or Manager in some applications. There may be
            multiple and commission may be spread across multiple sales people.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="user" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesOrder User" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Link to the user who placed the Sales Order.   The ‘user’ resource will
            be defined in the future.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="salesOrderLines" type="salesOrderLine--list" minOccurs="0" sme:label="SalesOrder SalesOrderLine" sme:relationship="child" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Sales order Line detail (e.g. items on the Sales Order)   See
            “SalesOrderLine” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="lineCount" type="xs:decimal" minOccurs="0" sme:label="SalesOrder Line Count">
				<xs:annotation>
					<xs:documentation>
            The number of lines on the Sales Order.   Should be auto-populated.
            Used as a cross reference to ensure order integrity.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderDiscountType" type="discountType--enum" nillable="true" minOccurs="0" sme:label="SalesOrder Order Discount Type">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate the type of discount applicable to this Sales Order
            (percent or amount)   From the Trading Account.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderDiscountAmount" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesOrder Order Discount Amount">
				<xs:annotation>
					<xs:documentation>
            Discount applicable to the Sales Order expressed as an amount.   From
            the Trading Account.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderDiscountPercent" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesOrder Order Discount Percent">
				<xs:annotation>
					<xs:documentation>
            Discount applicable to the Sales Order expressed as a percent.   From
            the Trading Account.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderAdditionalDiscount1Type" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesOrder Order Additional Discount1 Type">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate the type of discount applicable to this Sales Order
            (percent or amount)   Percent Amount Both
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderAdditionalDiscount1Amount" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesOrder Order Additional Discount1 Amount">
				<xs:annotation>
					<xs:documentation>
            Discount applicable to the Sales Order expressed as an amount.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderAdditionalDiscount1Percent" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesOrder Order Additional Discount1 Percent">
				<xs:annotation>
					<xs:documentation>
            Discount applicable to the Sales Order expressed as a percent.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderAdditionalDiscount2" type="discountType--enum" nillable="true" minOccurs="0" sme:label="SalesOrder Order Additional Discount2 Type" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate the type of discount applicable to this Sales Order
            (percent or amount)  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderAdditionalDiscount2Amount" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesOrder Order Additional Discount2 Amount" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Discount applicable to the Sales Order expressed as an amount.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderAdditionalDiscount2Percent" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesOrder Order Additional Discount2 Percent" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Discount applicable to the Sales Order expressed as a percent.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="text1" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesOrder Text1">
				<xs:annotation>
					<xs:documentation>
            Text/commentary (e.g. special Delivery instructions) for the Sales
            Order   Header text.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="text2" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesOrder Text2">
				<xs:annotation>
					<xs:documentation>
            Further text/commentary for the Sales Order.   Footer text.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="netTotal" type="xs:decimal" minOccurs="0" sme:label="SalesOrder Net Total">
				<xs:annotation>
					<xs:documentation>
            Total value of the Sales Order, including discounts, excluding tax.  
            In order currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="discountTotal" type="xs:decimal" minOccurs="0" sme:label="SalesOrder Discount Total">
				<xs:annotation>
					<xs:documentation>
            Total discounts applied to the order   In order currency
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="chargesTotal" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesOrder Charges Total">
				<xs:annotation>
					<xs:documentation>
            Total value of any (sur)charges applied to the Sales Order (e.g.
            finance charges, interest).   In order currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxTotal" type="xs:decimal" minOccurs="0" sme:label="SalesOrder Tax Total">
				<xs:annotation>
					<xs:documentation>
            Total tax applicable to the Sales Order.   In order currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="grossTotal" type="xs:decimal" minOccurs="0" sme:label="SalesOrder Gross Total">
				<xs:annotation>
					<xs:documentation>
            Total value of the Sales Order, including tax &amp; discounts
            &amp; charges.   In order currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="costTotal" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesOrder Cost Total">
				<xs:annotation>
					<xs:documentation> Total cost of the Sales Order.   In order currency. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="profitTotal" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesOrder Profit Total">
				<xs:annotation>
					<xs:documentation>
            Total profit on the Sales Order (e.g. net total – cost total).   In
            order currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="receipts" type="receipt--list" nillable="true" minOccurs="0" sme:label="SalesOrder Receipt" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Link to the Sales Order receipt (e.g. if order is taken with payment or
            if a deposit has been paid).   See “Receipt” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="depositFlag" type="xs:boolean" nillable="true" minOccurs="0" sme:label="SalesOrder Deposit Flag">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate if a deposit has been paid with the order.   Default =
            False (no deposit taken).
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="contract" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesOrder Contract" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            reference to a contract associated with the Sales Order. If Contracts
            aren’t used there may be an estimate instead – this would contain the reference of the
            estimate.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceTradingAccount" type="tradingAccount--type" nillable="true" minOccurs="0" sme:label="SalesOrder Invoice TradingAccount" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Link to the Trading Account to be invoiced for the Sales Order (if
            different from the Customer) e.g. Bill To   See “TradingAccount” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceCountry" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesOrder Invoice Country">
				<xs:annotation>
					<xs:documentation>
            Country to which the Sales Invoice is sent (e.g. for taxation
            purposes).   ISO 3166-1 alpha-2 country codes only
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderCountry" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesOrder Order Country">
				<xs:annotation>
					<xs:documentation>
            Country of destination for the Sales Order (e.g. for customs purposes).
              ISO 3166-1 alpha-2 country codes only
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="projects" type="project--list" nillable="true" minOccurs="0" sme:label="SalesOrder Project" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Identifier of the Project this Sales Order is for.   See “Project”
            definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="projectLines" type="projectLine--list" nillable="true" minOccurs="0" sme:label="SalesOrder ProjectLine" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Project Line value for this Sales Order.   See “Project Line”
            definition. Some applications may not have Project Lines.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="cases" type="case--list" nillable="true" minOccurs="0" sme:label="SalesOrder Case reference" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            The reference of the ‘case’ associated with the Sales Order e.g. if the
            Order is for repair/fix services.   A ‘Case’ entity hase been defined to support the F2F
            process.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="salesOrderDeliveries" type="salesOrderDelivery--list" nillable="true" minOccurs="0" sme:label="SalesOrder SalesOrderDelivery" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Link to the Delivery (dispatch) against the Sales Order. If there are
            multiple then these are held against the Sales Order Line.   See “SalesOrderDelivery”
            definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fulfillmentLocations" type="location--list" nillable="true" minOccurs="0" sme:label="SalesOrder Fulfilment Location" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            The Location from which the Sales Order will be fulfilled (e.g. the
            warehouse holding the Commodity).   See “Location” definition. May be held at Line level
            instead.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="financialAccounts" type="sc:financialAccount--list" nillable="true" minOccurs="0" sme:label="SalesOrder FinancialAccount" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Financial accounting code applicable to the Sales Order.   See
            “FinancialAccount” definition. Could come from the Trading Account.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="costCentres" type="sc:costCentre--list" nillable="true" minOccurs="0" sme:label="Contact CostCentres" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="departments" type="sc:department--list" nillable="true" minOccurs="0" sme:label="Contact Departments" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="interactions" type="interaction--list" nillable="true" minOccurs="0" sme:label="SalesOrder Interaction" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Interaction(s) associated with the Sales Order.   See “Interaction”
            definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="notes" type="sc:note--list" nillable="true" minOccurs="0" sme:label="SalesOrder Note" sme:relationship="child" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation> Notes associated with a Sales Order.   </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
		<xs:anyAttribute/>
	</xs:complexType>
	<xs:complexType name="salesOrder--list">
		<xs:sequence>
			<xs:element name="salesOrder" type="salesOrder--type" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:anyAttribute/>
	</xs:complexType>
	<xs:element name="salesOrderLine" type="salesOrderLine--type" sme:role="resourceKind" sme:pluralName="salesOrderLines" sme:compliance="should" sme:canGet="true" sme:canPut="true" sme:canPost="true" sme:hasUuid="true" sme:isSyncSource="true" sme:isSyncTarget="true"/>
	<xs:complexType name="salesOrderLine--type">
		<xs:all>
			<xs:element name="applicationID" type="xs:string" minOccurs="0" sme:label="SalesOrderLine Application ID" sme:isUniqueKey="true">
				<xs:annotation>
					<xs:documentation> The application specific identifier.   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="salesOrder" type="salesOrder--type" minOccurs="0" sme:label="Sales Order" sme:relationship="parent" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation> the parent SalesOrder </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="active" type="xs:boolean" minOccurs="0" sme:label="SalesOrderLine Active Flag">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate if the Sales Order Line is an ‘active’ one .   Default
            = True
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deleted" type="xs:boolean" minOccurs="0" sme:label="SalesOrderLine Deleted">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate if Sales Order Line is (soft) deleted.   Default =
            False
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference" type="xs:string" minOccurs="0" sme:label="SalesOrderLine reference" sme:isUniqueKey="true">
				<xs:annotation>
					<xs:documentation> reference for the Sales Order Line.   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference2" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesOrderLine Reference2" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation> Additional reference for the Sales Order Line.   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="number" type="xs:decimal" minOccurs="0" sme:label="SalesOrderLine Number">
				<xs:annotation>
					<xs:documentation>
            The number of the Sales Order Line.   For sequencing and ordering of
            lines.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="status" type="salesOrderStatus--enum" nillable="true" minOccurs="0" sme:label="SalesOrderLine Status">
				<xs:annotation>
					<xs:documentation> Status of the Sales Order Line.   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="type" type="lineType--enum" nillable="true" sme:label="SalesOrderLine Type" sme:isMandatory="true">
				<xs:annotation>
					<xs:documentation>
            Enumerator indicating the type of Sales Order Line.   Allowable values:
            Standard Line; Free text (with price); Commentary (without price, e.g. “thank you for
            your business!”); Component (with price but not in total); Discount/charge (included
            price/total)
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deliveryTerms" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesOrderLine Delivery Terms">
				<xs:annotation>
					<xs:documentation>
            The terms for Delivery for this Sales Order Line.   Also on Sales Order
            but may be different per Line.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="commodity" type="commodity--type" nillable="true" minOccurs="0" sme:label="SalesOrderLine Commodity" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Link to the Commodity used on the Sales Order Line.   See “Commodity”
            definition. Not mandatory as Line may be a free text or comment Line.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="text" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesOrderLine Text">
				<xs:annotation>
					<xs:documentation>
            Text applicable to the Sales Order Line (e.g. the Line may be a comment
            Line or a free text Line). If the Sales Order Line is for a Commodity then the Commodity
            description should be held here.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="commodityVariant" type="commodityVariant--type" nillable="true" minOccurs="0" sme:label="SalesOrderLine Commodity Variant" sme:relationship="reference" sme:isCollection="false" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            The Variant of the Commodity (e.g. colour or size) on the Sales Order
            Line.   See “Variant” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="commodityDimension" type="commodityDimension--type" nillable="true" minOccurs="0" sme:label="SalesOrderLine Commodity Dimension" sme:relationship="reference" sme:isCollection="false" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            The Dimension of the Commodity on the Sales Order Line.   See
            “Dimension” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="associatedCommodity" type="commodity--type" nillable="true" minOccurs="0" sme:label="SalesOrderLine Associated Commodity" sme:relationship="reference" sme:isCollection="false" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Link to the Commodity associated with the Sales Order Line, e.g. if
            Line is for service to another Commodity.   See “Commodity” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fulfillmentLocation" type="location--type" nillable="true" minOccurs="0" sme:label="SalesOrderLine Fulfillment Location" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            The Location from which the Sales Order Line will be fulfilled (e.g.
            the warehouse holding the Commodity).   See “Location” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deliveryDate" type="xs:date" nillable="true" minOccurs="0" sme:label="SalesOrderLine Delivery Date">
				<xs:annotation>
					<xs:documentation> The anticipated date of the Delivery.   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dueDate" type="xs:date" nillable="true" minOccurs="0" sme:label="SalesOrderLine Due Date">
				<xs:annotation>
					<xs:documentation>
            Date the Sales Order Line is due by the Customer.   Delivery date may
            be different to the Customer due date.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierTradingAccount" type="tradingAccount--type" nillable="true" minOccurs="0" sme:label="SalesOrderLine Carrier TradingAccount" sme:relationship="reference" sme:isCollection="false" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Link to the Trading Account associated with the Supplier that wll be
            the carrier / freight forwarder / deliverer of the Sales Order Line.   See
            “TradingAccount” definition. Carriers are not used by all application.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierNetPrice" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesOrderLine Carrier Net Price">
				<xs:annotation>
					<xs:documentation>
            The price of Delivery/carriage.   In Order currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierTaxPrice" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesOrderLine Carrier Tax Price">
				<xs:annotation>
					<xs:documentation> The tax on Delivery/carriage.   In Order currency. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierTotalPrice" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesOrderLine Carrier Gross Price">
				<xs:annotation>
					<xs:documentation>
            The total price of Delivery/carriage (net + tax).   In Order currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierSalesInvoiceLine" type="salesInvoiceLine--type" nillable="true" minOccurs="0" sme:label="SalesOrderLine Carrier SalesInvoiceLine" sme:relationship="reference" sme:isCollection="false" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            A link to the Sales Invoice generated to recharge the carriage to the
            Customer.   See “SalesInvoice” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierPurchaseInvoiceLine" type="purchaseInvoiceLine--type" nillable="true" minOccurs="0" sme:label="SalesOrderLine Carrier PurchaseInvoiceLine" sme:relationship="reference" sme:isCollection="false" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            A link to the purchase Invoice charged by the carrier Company for the
            Delivery to the Customer.   See “PurchaseInvoice” defi nition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierTaxCodes" type="taxCode--list" nillable="true" minOccurs="0" sme:label="SalesOrderLine Carrier Tax Code" sme:relationship="association" sme:isCollection="true" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Tax code governing the tax regime applicable to the carriage on this
            Sales Order Line.   See “TaxCode” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierReference" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesOrderLine Carrier reference">
				<xs:annotation>
					<xs:documentation>
            reference number as used by the carrier during Delivery e.g. for
            Delivery tracking.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="unitOfMeasure" type="unitOfMeasure--type" minOccurs="0" sme:label="SalesOrderLine UnitOfMeasure" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            A link to the scale to be used in the measurement of physical
            quantities of Commodity. Examples include "Each", "Box of 10", etc. May also be called
            "Pack Size".   See “UnitOfMeasure” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="quantity" type="xs:decimal" minOccurs="0" sme:label="SalesOrderLine Quantity" sme:isMandatory="true">
				<xs:annotation>
					<xs:documentation>
            The quantity of the Commodity ordered on the Sales Order Line.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="initialPrice" type="xs:decimal" minOccurs="0" sme:label="SalesOrderLine Initial Price">
				<xs:annotation>
					<xs:documentation>
            The initial price of the Commodity ordered on the Sales Order Line
            (e.g. before discounts).   In order currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="pricelist" type="priceList--type" nillable="true" minOccurs="0" sme:label="SalesOrderLine PriceList" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            A reference to the Price List used to derive the price on the Sales
            Order Line.   See “PriceList” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="supplierTradingAccount" type="tradingAccount--type" nillable="true" minOccurs="0" sme:label="SalesOrderLine Supplier TradingAccount" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Link to the Sales Order Line Supplier account (e.g. for back to back,
            contremarque, drop-ship ordering).   See “TradingAccount” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="supplierReference" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesOrderLine Supplier reference">
				<xs:annotation>
					<xs:documentation> Supplier own reference for the Sales Order Line.   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="supplierPurchaseOrder" type="purchaseOrder--list" nillable="true" minOccurs="0" sme:label="SalesOrderLine Supplier PurchaseOrder" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Link to any Supplier Purchase Orders used to fulfill the Sales Order
            Line (back to back or drop-ship).   See “PurchaseOrder” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="supplierPurchaseOrderLine" type="purchaseOrderLine--list" nillable="true" minOccurs="0" sme:label="SalesOrderLine Supplier PurchaseOrderLine" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Link to any Supplier Purchase Order Lines used to fulfill the Sales
            Order Line (back to back or drop-ship).   See “PurchaseOrderLine” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="quotationLine" type="salesQuotationLine--type" nillable="true" minOccurs="0" sme:label="SalesOrderLine QuotationLine" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            A link to the Quotation Line used to create this Sales Order Line (if
            any).   See “QuotationLine” definition. Must be a valid quotation.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="postalAddresses" type="sc:postalAddress--list" nillable="true" minOccurs="0" sme:label="SalesOrderLine PostalAddress" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Link to Postal Address(es) associated with the Sales Order Line (ship
            to).  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderLineDiscountType" type="discountType--enum" nillable="true" minOccurs="0" sme:label="SalesOrderLine Order Line Discount Type" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate the type of discount applicable to this Sales Order
            Line (percent or amount)   From the Trading Account.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderLineDiscountAmount" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesOrderLine Order Line Discount Amount" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Discount applicable to the Sales Order Line, expressed as an amount.  
            From the Trading Account.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderLineDiscountPercent" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesOrderLine Order Line Discount Percent" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Discount applicable to the Sales Order Line, expressed as a percentage.
              From the Trading Account.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderLineSubtotalDiscountType" type="discountType--enum" nillable="true" minOccurs="0" sme:label="SalesOrderLine Order Line Subtotal Discount Type" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate the type of discount applicable to a subtotal of Sales
            Order lines (percent or amount) of which this Line is part.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderLineSubtotalDiscountAmount" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesOrderLine Order Line Subtotal Discount Amount" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Discount applicable to the Sales Order Line, expressed as an amount.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderLineSubtotalDiscountPercent" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesOrderLine Order Line Subtotal Discount Percent" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Discount applicable to the Sales Order Line, expressed as a percentage.
             
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FOBPoint" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesOrderLine FOB Point">
				<xs:annotation>
					<xs:documentation>
            The FOB point applicable to the Sales Order Line. FOB is an
            abbreviation for Free On Board. The term FOB is used when shipping goods, to indicate
            who pays loading and transportation costs, and/or the point at which the responsibility
            of the goods transfers from shipper to buyer  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="actualPrice" type="xs:decimal" minOccurs="0" sme:label="SalesOrderLine Actual Price">
				<xs:annotation>
					<xs:documentation>
            The actual price of the Commodity ordered on the Sales Order Line (e.g.
            after discounts or adjustments).   In order currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="netTotal" type="xs:decimal" minOccurs="0" sme:label="SalesOrderLine Net Total">
				<xs:annotation>
					<xs:documentation>
            Total value of the Sales Order Line, including discounts and charges,
            excluding tax (e.g. quantity * actual price)   In order currency. Could be calculated.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="chargesTotal" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesOrderLine Charges Total">
				<xs:annotation>
					<xs:documentation>
            Total value of any (sur)charges applied to the Sales Order Line (e.g.
            finance charges, interest).   In order currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="discountTotal" type="xs:decimal" minOccurs="0" sme:label="SalesOrderLine Discount Total">
				<xs:annotation>
					<xs:documentation>
            Total discounts applied to the order line   In order currency
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxCodes" type="taxCode--list" nillable="true" minOccurs="0" sme:label="SalesOrderLine Tax Code" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Tax code governing the tax regime applicable to this Sales Order Line
            (default value from Sales Order).   See “TaxCode” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="priceTax" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesOrderLine Price Tax">
				<xs:annotation>
					<xs:documentation>
            Tax applicable to the Sales Order Line price e.g. the per unit tax.  
            Both unit-tax and Line-tax values are required due to the way tax is handled differently
            by product/country.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxTotal" type="xs:decimal" minOccurs="0" sme:label="SalesOrderLine Tax Total">
				<xs:annotation>
					<xs:documentation>
            Tax applicable to the Sales Order Line.   In order currency. Could be
            calculated. Both unit-tax and Line-tax values are required due to the way tax is handled
            differently by product/country.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="grossTotal" type="xs:decimal" minOccurs="0" sme:label="SalesOrderLine Gross Total">
				<xs:annotation>
					<xs:documentation>
            Total value of the Sales Order Line, including discounts &amp; tax
            &amp; charges.   In order currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="costTotal" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesOrderLine Cost Total">
				<xs:annotation>
					<xs:documentation>
            Total cost of the Sales Order Line.   In order currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="profitTotal" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesOrderLine Profit Total">
				<xs:annotation>
					<xs:documentation>
            Total profit on the Sales Order Line (e.g. net total – cost total).  
            In order currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="salesInvoiceLines" type="salesInvoiceLine--list" nillable="true" minOccurs="0" sme:label="SalesOrderLine Sales Invoice Line" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Sales Invoice Line(s) associated with the Sales Order Line.   See
            “SalesInvoiceLine” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="receipts" type="receipt--list" nillable="true" minOccurs="0" sme:label="SalesOrderLine Receipt" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            A link to the (cash) receipt(s) associated with this Sales Order Line  
            See “Receipt” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="receiptLines" type="receiptLine--list" nillable="true" minOccurs="0" sme:label="SalesOrderLine ReceiptLine" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            A link to the (cash) receipt line(s) associated with this Sales Order
            Line   See “ReceiptLine” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="buyerContact" type="contact--type" nillable="true" minOccurs="0" sme:label="SalesOrderLine Buyer Contact" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Link to the Sales Order Line originator (buyer Contact).   See
            “Contact” definition. This could be derived from the Sales Order
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="project" type="project--type" nillable="true" minOccurs="0" sme:label="SalesOrderLine Project" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Identifier of the Project this Sales Order Line is for.   See “Project”
            definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="projectLine" type="projectLine--type" nillable="true" minOccurs="0" sme:label="SalesOrderLine ProjectLine" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Project Line value for this Sales Order Line.   See “Project Line”
            definition. Some applications may not have Project Lines.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="salesOrderDeliveryLines" type="salesOrderDeliveryLine--list" nillable="true" minOccurs="0" sme:label="SalesOrderLine SalesOrderDeliveryLine" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Link to the Delivery (dispatch) Line against the Sales Order Line.  
            See “SalesOrderDeliveryLine” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BOMLine" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesOrderLine BOM Line" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            BOM Line associated with the Sales Order Line   The ‘BOM Line’ resource
            will be defined in the future. Not all applications support this feature.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="financialAccounts" type="sc:financialAccount--list" nillable="true" minOccurs="0" sme:label="SalesOrderLine FinancialAccount" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Financial accounting code applicable to the Sales Order Line.   See
            “FinancialAccount” definition. This could be derived from the Sales Order
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="costCentres" type="sc:costCentre--list" nillable="true" minOccurs="0" sme:label="SalesOrderLine CostCentres" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="departments" type="sc:department--list" nillable="true" minOccurs="0" sme:label="SalesOrderLine Departments" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="notes" type="sc:note--list" nillable="true" minOccurs="0" sme:label="SalesOrderLine Note" sme:relationship="child" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation> Notes associated with a Sales Order Line.   </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
		<xs:anyAttribute/>
	</xs:complexType>
	<xs:complexType name="salesOrderLine--list">
		<xs:sequence>
			<xs:element name="salesOrderLine" type="salesOrderLine--type" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:anyAttribute/>
	</xs:complexType>
	<xs:element name="salesQuotation" type="salesQuotation--type" sme:role="resourceKind" sme:pluralName="salesQuotations" sme:compliance="should" sme:canGet="true" sme:canPut="true" sme:canPost="true" sme:hasUuid="true" sme:isSyncSource="true" sme:isSyncTarget="true"/>
	<xs:complexType name="salesQuotation--type">
		<xs:all>
			<xs:element name="active" type="xs:boolean" minOccurs="0" sme:label="SalesQuotation Active Flag">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate if the Sales Quotation is an ‘active’ one .   Default = True
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference" type="xs:string" minOccurs="0" sme:label="SalesQuotation reference" sme:isUniqueKey="true">
				<xs:annotation>
					<xs:documentation> reference for the Sales Quotation.   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference2" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesQuotation Reference2" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation> Additional reference for the Sales Quotation.   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="status" type="salesQuotationStatus--enum" nillable="true" minOccurs="0" sme:label="SalesQuotation Status">
				<xs:annotation>
					<xs:documentation> Status of the Sales Quotation   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="statusFlag" type="xs:boolean" nillable="true" minOccurs="0" sme:label="SalesQuotation Status Flag">
				<xs:annotation>
					<xs:documentation>
            Additional Boolean status flag (e.g. Quotation on hold (or “blocked”),
            off hold).   True is on hold, False is not on hold. Default value is False.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="statusFlagText" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesQuotation Status Flag Text">
				<xs:annotation>
					<xs:documentation>
            Sales Quotation status flag text.   Default = “On Hold”
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tradingAccount" type="tradingAccount--type" nillable="true" minOccurs="0" sme:label="SalesQuotation Customer TradingAccount" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Link to the Sales Quotation Customer account.   See “TradingAccount”
            definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customerReference" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesQuotation Customer reference">
				<xs:annotation>
					<xs:documentation>
            Customer own reference for the Sales Quotation (e.g. a PO number).  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="opportunity" type="opportunity--type" nillable="true" minOccurs="0" sme:label="SalesQuotation Opportunity" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Opportunity linked to the Sales Quotation (if any).   See “Opportunity”
            definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="pricelist" type="priceList--type" nillable="true" minOccurs="0" sme:label="SalesQuotation PriceList" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Link to the Price List used when generating the Sales Quotation   See
            “PriceList” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="type" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesQuotation Type">
				<xs:annotation>
					<xs:documentation> The Sales Quotation type.   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="copyFlag" type="xs:boolean" minOccurs="0" sme:label="SalesQuotation Copy Flag">
				<xs:annotation>
					<xs:documentation>
            Indicates if this Sales Quotation is a copy or not.   Default is false
            (no)
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="originatorDocument" type="salesQuotation--type" nillable="true" minOccurs="0" sme:label="SalesQuotation Originator Document reference" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            reference to another Sales Quotation if required (e.g. if Quotation is
            a copy, then this will show which Quotation was copied, If this is a replacement
            Quotation this will show the original Quotation).  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="postalAddresses" type="sc:postalAddress--list" nillable="true" minOccurs="0" sme:label="SalesQuotation PostalAddress" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Link to Postal Address(es) associated with the Sales Quotation.   See
            “PostalAddress” definition. Could be multiple e.g. shipping, billing, etc These are also
            stored on SalesQuotationLines
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deliveryMethod" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesQuotation Delivery Method">
				<xs:annotation>
					<xs:documentation>
            Delivery method to be employed for Sales Quotation (e.g. Courier,
            Collection ,etc)  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deliveryRule" type="xs:boolean" nillable="true" minOccurs="0" sme:label="SalesQuotation Delivery Rule">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate if the Quotation has to be delivered in full or can be
            split across partial deliveries.   True = Partial deliveries allowed; False = Partial
            deliveries not allowed. Default = True
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deliveryTerms" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesQuotation Delivery Terms">
				<xs:annotation>
					<xs:documentation> The terms for Delivery.   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deliveryDate" type="xs:date" nillable="true" minOccurs="0" sme:label="SalesQuotation Delivery Date">
				<xs:annotation>
					<xs:documentation> The anticipated date of the Delivery.   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dueDate" type="xs:date" nillable="true" minOccurs="0" sme:label="SalesQuotation Due Date">
				<xs:annotation>
					<xs:documentation>
            Date the Quotation is due by the Customer.   Delivery date may be
            different to the Customer due date.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="currency" type="xs:string" minOccurs="0" sme:label="SalesQuotation Currency">
				<xs:annotation>
					<xs:documentation>
            The currency of the Sales Quotation (e.g. GBP, USD, EUR).   ISO 4217.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operatingCompanyCurrency" type="xs:string" minOccurs="0" sme:label="SalesQuotation OperatingCompany Currency">
				<xs:annotation>
					<xs:documentation>
            The currency as used by OperatingCompany, the business operating the
            Sage application (the “base” currency).   ISO 4217.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operatingCompanyCurrencyExchangeRate" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesQuotation OperatingCompany Currency Exchange Rate">
				<xs:annotation>
					<xs:documentation>
            Rate used to convert from Sales Quotation Currency into Sales Quotation
            OperatingCompany Currency, if different.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operatingCompanyCurrencyExchangeRateOperator" type="RateOperator--enum" nillable="true" minOccurs="0" sme:label="SalesQuotation OperatingCompany Currency Exchange Rate Operator">
				<xs:annotation>
					<xs:documentation>
            Operator to be used (multiplication or division) to convert from Sales
            Quotation Currency to Sales Quotation OperatingCompany Currency.   “*” (multiplication)
            or “/” division. E.g. if the operator is division then it is Sales Quotation Values in
            Sales Quotation Currency / Sales Quotation Operating Company Currency Exchange Rate =
            Sales Quotation Values in Sales Quotation OperatingCompany currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operatingCompanyCurrencyExchangeRateDate" type="xs:date" nillable="true" minOccurs="0" sme:label="SalesQuotation OperatingCompany Currency Exchange Rate Date">
				<xs:annotation>
					<xs:documentation>
            Date of the exchange rate used to convert from Sales Quotation Currency
            to Sales Quotation OperatingCompany Currency, if different.   Default = System date
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customerTradingAccountCurrency" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesQuotation Customer TradingAccount Currency">
				<xs:annotation>
					<xs:documentation>
            The currency (e.g. GBP, USD, EUR) used by the Customer Trading Account
            on the Sales Quotation.   ISO 4217. Could be derived from Quotation currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customerTradingAccountCurrencyExchangeRate" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesQuotation Customer TradingAccount Currency Exchange Rate">
				<xs:annotation>
					<xs:documentation>
            Rate used to convert the Sales Quotation currency into the Customer
            Trading Account currency, if different.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customerTradingAccountCurrencyExchangeRateOperator" type="RateOperator--enum" nillable="true" minOccurs="0" sme:label="SalesQuotation Customer TradingAccount Currency Exchange Rate Operator">
				<xs:annotation>
					<xs:documentation>
            Operator to be used (multiplication or division) to convert from Sales
            Quotation Currency to Sales Quotation Customer Trading Account Currency   “*”
            (multiplication) or “/” division. E.g. if the operator is division then it is Sales
            Quotation Values in Sales Quotation Currency / Sales Quotation Customer TradingAccount
            Currency Exchange Rate = Sales Quotation Values in Sales Quotation Customer
            TradingAccount Currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customerTradingAccountCurrencyExchangeRateDate" type="xs:date" nillable="true" minOccurs="0" sme:label="SalesQuotation Customer TradingAccount Currency Exchange Rate Date">
				<xs:annotation>
					<xs:documentation>
            Date of the exchange rate used to convert from Sales Quotation Currency
            to Customer Trading Account Currency, if different.   Default = System date
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="date" type="xs:date" minOccurs="0" sme:label="SalesQuotation Date">
				<xs:annotation>
					<xs:documentation> Date the Sales Quotation was placed.   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="time" type="xs:time" nillable="true" minOccurs="0" sme:label="SalesQuotation Time">
				<xs:annotation>
					<xs:documentation>
            Time the Sales Quotation was placed. W3C date time standard for XML
            (ISO 8601). The base timezone is UTC. The offset is included. UTC should be used See the
            reference here: http://www.w3.org/TR/xmlschema-2/#isoformats  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="validity" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesQuotation Validity">
				<xs:annotation>
					<xs:documentation>
            Period the Sales Quotation is valid for (days from Quotation Date).  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="expirationDate" type="xs:date" nillable="true" minOccurs="0" sme:label="SalesQuotation Expiration Date">
				<xs:annotation>
					<xs:documentation>
            The expiration date of the Sales Quotation (SalesQuotation Date +
            Validity).  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxCodes" type="taxCode--list" nillable="true" minOccurs="0" sme:label="SalesQuotation Tax Code" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Tax code governing the tax regime applicable to this Sales Quotation.  
            See “TaxCode” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="buyerContact" type="contact--type" nillable="true" minOccurs="0" sme:label="SalesQuotation Buyer Contact" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Link to the Contact at Customer who placed the Sales Quotation.   See
            “Contact” definition. Default = Primary TradingAccount Contact
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="salespersons" type="salesPerson--list" nillable="true" minOccurs="0" sme:label="SalesQuotation Salesperson" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Sales person associated with the Sales Quotation.   See “SalesPerson”
            definition. Also known as Sales Agent or Manager in some applications. There may be
            multiple and commission may be spread across multiple sales people.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="user" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesQuotation User" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Link to the user who placed the Sales Quotation.   The ‘user’ resource
            will be defined in the future.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="salesQuotationLines" type="salesQuotationLine--list" minOccurs="0" sme:label="SalesQuotation SalesQuotationLine" sme:relationship="child" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Sales Quotation Line detail (e.g. items on the Sales Quotation)   See
            “SalesQuotationLine” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="lineCount" type="xs:decimal" minOccurs="0" sme:label="SalesQuotation Line Count">
				<xs:annotation>
					<xs:documentation>
            The number of lines on the Sales Quotation.   Should be auto-populated.
            Used as a cross reference to ensure Quotation integrity.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderDiscountType" type="discountType--enum" nillable="true" minOccurs="0" sme:label="SalesQuotation Order Discount Type">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate the type of discount applicable to this Sales
            Quotation (percent or amount)   From the Trading Account.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderDiscountAmount" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesQuotation Order Discount Amount">
				<xs:annotation>
					<xs:documentation>
            Discount applicable to the Sales Quotation expressed as an amount.  
            From the Trading Account.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderDiscountPercent" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesQuotation Order Discount Percent">
				<xs:annotation>
					<xs:documentation>
            Discount applicable to the Sales Quotation expressed as a percent.  
            From the Trading Account.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderAdditionalDiscount1Type" type="discountType--enum" nillable="true" minOccurs="0" sme:label="SalesQuotation Order Additional Discount1 Type">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate the type of discount applicable to this Sales
            Quotation (percent or amount)  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderAdditionalDiscount1Amount" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesQuotation Order Additional Discount1 Amount">
				<xs:annotation>
					<xs:documentation>
            Discount applicable to the Sales Quotation expressed as an amount.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderAdditionalDiscount1Percent" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesQuotation Order Additional Discount1 Percent">
				<xs:annotation>
					<xs:documentation>
            Discount applicable to the Sales Quotation expressed as a percent.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderAdditionalDiscount2" type="discountType--enum" nillable="true" minOccurs="0" sme:label="SalesQuotation Order Additional Discount2 Type" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate the type of discount applicable to this Sales
            Quotation (percent or amount)  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderAdditionalDiscount2Amount" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesQuotation Order Additional Discount2 Amount" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Discount applicable to the Sales Quotation expressed as an amount.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderAdditionalDiscount2Percent" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesQuotation Order Additional Discount2 Percent" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Discount applicable to the Sales Quotation expressed as a percent.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="text1" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesQuotation Text1">
				<xs:annotation>
					<xs:documentation>
            Text/commentary (e.g. special Delivery instructions) for the Sales
            Quotation   Header text.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="text2" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesQuotation Text2">
				<xs:annotation>
					<xs:documentation>
            Further text/commentary for the Sales Quotation.   Footer text.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="netTotal" type="xs:decimal" minOccurs="0" sme:label="SalesQuotation Net Total">
				<xs:annotation>
					<xs:documentation>
            Total value of the Sales Quotation, including discounts, excluding tax.
              In Quotation currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="discountTotal" type="xs:decimal" minOccurs="0" sme:label="SalesQuotation Discount Total">
				<xs:annotation>
					<xs:documentation>
            Total discounts applied to the Quotation   In Quotation currency
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="chargesTotal" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesQuotation Charges Total">
				<xs:annotation>
					<xs:documentation>
            Total value of any (sur)charges applied to the Sales Quotation (e.g.
            finance charges, interest).   In Quotation currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxTotal" type="xs:decimal" minOccurs="0" sme:label="SalesQuotation Tax Total">
				<xs:annotation>
					<xs:documentation>
            Total tax applicable to the Sales Quotation.   In Quotation currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="grossTotal" type="xs:decimal" minOccurs="0" sme:label="SalesQuotation Gross Total">
				<xs:annotation>
					<xs:documentation>
            Total value of the Sales Quotation, including tax &amp; discounts
            &amp; charges.   In Quotation currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="costTotal" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesQuotation Cost Total">
				<xs:annotation>
					<xs:documentation>
            Total cost of the Sales Quotation.   In Quotation currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="profitTotal" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesQuotation Profit Total">
				<xs:annotation>
					<xs:documentation>
            Total profit on the Sales Quotation (e.g. net total – cost total).   In
            Quotation currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="contract" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesQuotation Contract" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            reference to a contract associated with the Sales Quotation. If
            Contracts aren’t used there may be an estimate instead – this would contain the
            reference of the estimate.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="projects" type="project--list" nillable="true" minOccurs="0" sme:label="SalesQuotation Project" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Identifier of the Project this Sales Quotation is for.   See “Project”
            definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="projectLines" type="projectLine--list" nillable="true" minOccurs="0" sme:label="SalesQuotation ProjectLine" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Project Line value for this Sales Quotation.   See “Project Line”
            definition. Some applications may not have Project Lines.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="cases" type="case--list" nillable="true" minOccurs="0" sme:label="SalesQuotation Case reference" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            The reference of the ‘case’ associated with the Sales Quotation e.g. if
            the Quotation is for repair/fix services.   A ‘Case’ entity hase been defined to support
            the F2F process.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fulfillmentLocations" type="location--list" nillable="true" minOccurs="0" sme:label="SalesQuotation Fulfilment Location" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            The Location from which the Sales Quotation will be fulfilled (e.g. the
            warehouse holding the Commodity).   See “Location” definition. May be held at Line level
            instead.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="financialAccounts" type="sc:financialAccount--list" nillable="true" minOccurs="0" sme:label="SalesQuotation FinancialAccount" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Financial accounting code applicable to the Sales Quotation.   See
            “FinancialAccount” definition. Could come from the Trading Account.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="costCentres" type="sc:costCentre--list" nillable="true" minOccurs="0" sme:label="SalesQuotation CostCentres" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="departments" type="sc:department--list" nillable="true" minOccurs="0" sme:label="SalesQuotation Departments" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="interactions" type="interaction--list" nillable="true" minOccurs="0" sme:label="SalesQuotation Interaction" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Interaction(s) associated with the Sales Quotation.   See “Interaction”
            definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="notes" type="sc:note--list" nillable="true" minOccurs="0" sme:label="SalesQuotation Note" sme:relationship="child" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation> Notes associated with a Sales Quotation.   </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
		<xs:anyAttribute/>
	</xs:complexType>
	<xs:complexType name="salesQuotation--list">
		<xs:sequence>
			<xs:element name="salesQuotation" type="salesQuotation--type" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="salesQuotationLine" type="salesQuotationLine--type" sme:role="resourceKind" sme:pluralName="salesQuotationLines" sme:compliance="should" sme:canGet="true" sme:canPut="true" sme:canPost="true" sme:hasUuid="true" sme:isSyncSource="true" sme:isSyncTarget="true"/>
	<xs:complexType name="salesQuotationLine--type">
		<xs:all>
			<xs:element name="salesQuotation" type="salesQuotation--type" minOccurs="0" sme:label="Sales Quotation" sme:relationship="parent" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation> The parent Sales Quotation </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="active" type="xs:boolean" minOccurs="0" sme:label="SalesQuotationLine Active Flag">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate if the Sales Quotation Line is an ‘active’ one .  
            Default = True
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference" type="xs:string" minOccurs="0" sme:label="SalesQuotationLine reference" sme:isUniqueKey="true">
				<xs:annotation>
					<xs:documentation> reference for the Sales Quotation Line.   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference2" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Reference2" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Additional reference for the Sales Quotation Line.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="number" type="xs:decimal" minOccurs="0" sme:label="SalesQuotationLine Number">
				<xs:annotation>
					<xs:documentation>
            The number of the Sales Quotation Line.   For sequencing and ordering
            of lines.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="status" type="salesQuotationStatus--enum" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Status">
				<xs:annotation>
					<xs:documentation> Status of the Sales Quotation Line.   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="type" type="lineType--enum" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Type" sme:isMandatory="true">
				<xs:annotation>
					<xs:documentation>
            Enumerator indicating the type of Sales Quotation Line.   Allowable
            values: Standard Line; Free text (with price); Commentary (without price, e.g. “thank
            you for your business!”); Component (with price but not in total); Discount/charge
            (included price/total)
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deliveryTerms" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Delivery Terms">
				<xs:annotation>
					<xs:documentation>
            The terms for Delivery for this Sales Quotation Line.   Also on Sales
            Quotation but may be different per Line.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="commodity" type="commodity--type" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Commodity" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Link to the Commodity used on the Sales Quotation Line.   See
            “Commodity” definition. Not mandatory as Line may be a free text or comment Line.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="text" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Text">
				<xs:annotation>
					<xs:documentation>
            Text applicable to the Sales Quotation Line (e.g. the Line may be a
            comment Line or a free text Line). If the Sales Quotation Line is for a Commodity then
            the Commodity description should be held here.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="commodityVariant" type="commodityVariant--type" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Commodity Variant" sme:relationship="reference" sme:isCollection="false" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            The Variant of the Commodity (e.g. colour or size) on the Sales
            Quotation Line.   See “Variant” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="commodityDimension" type="commodityDimension--type" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Commodity Dimension" sme:relationship="reference" sme:isCollection="false" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            The Dimension of the Commodity on the Sales Quotation Line.   See
            “Dimension” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="associatedCommodity" type="commodity--type" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Associated Commodity" sme:relationship="reference" sme:isCollection="false" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Link to the Commodity associated with the Sales Quotation Line, e.g. if
            Line is for service to another Commodity.   See “Commodity” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fulfillmentLocation" type="location--type" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Fulfillment Location" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            The Location from which the Sales Quotation Line will be fulfilled
            (e.g. the warehouse holding the Commodity).   See “Location” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deliveryDate" type="xs:date" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Delivery Date">
				<xs:annotation>
					<xs:documentation> The anticipated date of the Delivery.   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dueDate" type="xs:date" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Due Date">
				<xs:annotation>
					<xs:documentation>
            Date the Sales Quotation Line is due by the Customer.   Delivery date
            may be different to the Customer due date.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="unitOfMeasure" type="unitOfMeasure--type" minOccurs="0" sme:label="SalesQuotationLine UnitOfMeasure" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            A link to the scale to be used in the measurement of physical
            quantities of Commodity. Examples include "Each", "Box of 10", etc. May also be called
            "Pack Size".   See “UnitOfMeasure” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="quantity" type="xs:decimal" minOccurs="0" sme:label="SalesQuotationLine Quantity" sme:isMandatory="true">
				<xs:annotation>
					<xs:documentation>
            The quantity of the Commodity ordered on the Sales Quotation Line.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="initialPrice" type="xs:decimal" minOccurs="0" sme:label="SalesQuotationLine Initial Price">
				<xs:annotation>
					<xs:documentation>
            The initial price of the Commodity ordered on the Sales Quotation Line
            (e.g. before discounts).   In order currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="pricelist" type="priceList--type" nillable="true" minOccurs="0" sme:label="SalesQuotationLine PriceList" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            A reference to the Price List used to derive the price on the Sales
            Quotation Line.   See “PriceList” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="postalAddresses" type="sc:postalAddress--list" nillable="true" minOccurs="0" sme:label="SalesQuotationLine PostalAddress" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Link to Postal Address(es) associated with the Sales Quotation Line
            (ship to).  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderLineDiscountType" type="discountType--enum" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Order Line Discount Type" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate the type of discount applicable to this Sales
            Quotation Line (percent or amount)   From the Trading Account.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderLineDiscountAmount" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Order Line Discount Amount" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Discount applicable to the Sales Quotation Line, expressed as an
            amount.   From the Trading Account.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderLineDiscountPercent" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Order Line Discount Percent" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Discount applicable to the Sales Quotation Line, expressed as a
            percentage.   From the Trading Account.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderLineSubtotalDiscountType" type="discountType--enum" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Order Line Subtotal Discount Type" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate the type of discount applicable to a subtotal of Sales
            Quotation lines (percent or amount) of which this Line is part.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderLineSubtotalDiscountAmount" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Order Line Subtotal Discount Amount" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Discount applicable to the Sales Quotation Line, expressed as an
            amount.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderLineSubtotalDiscountPercent" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Order Line Subtotal Discount Percent" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Discount applicable to the Sales Quotation Line, expressed as a
            percentage.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="actualPrice" type="xs:decimal" minOccurs="0" sme:label="SalesQuotationLine Actual Price">
				<xs:annotation>
					<xs:documentation>
            The actual price of the Commodity ordered on the Sales Quotation Line
            (e.g. after discounts or adjustments).   In Quotation currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="netTotal" type="xs:decimal" minOccurs="0" sme:label="SalesQuotationLine Net Total">
				<xs:annotation>
					<xs:documentation>
            Total value of the Sales Quotation Line, including discounts, excluding
            tax (e.g. quantity * actual price)   In Quotation currency. Could be calculated.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="chargesTotal" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Charges Total">
				<xs:annotation>
					<xs:documentation>
            Total value of any (sur)charges applied to the Sales Quotation Line
            (e.g. finance charges, interest).   In Quotation currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="discountTotal" type="xs:decimal" minOccurs="0" sme:label="SalesQuotationLine Discount Total">
				<xs:annotation>
					<xs:documentation>
            Total discounts applied to the quotation line   In quotation currency
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxCodes" type="taxCode--list" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Tax Code" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Tax code governing the tax regime applicable to this Sales Quotation
            Line (default value from Sales Quotation).   See “TaxCode” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="priceTax" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Price Tax">
				<xs:annotation>
					<xs:documentation>
            Tax applicable to the Sales Quotation Line price e.g. the per unit tax.
              Both unit-tax and Line-tax values are required due to the way tax is handled
            differently by product/country.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxTotal" type="xs:decimal" minOccurs="0" sme:label="SalesQuotationLine Tax Total">
				<xs:annotation>
					<xs:documentation>
            Tax applicable to the Sales Quotation Line.   In Quotation currency.
            Could be calculated. Both unit-tax and Line-tax values are required due to the way tax
            is handled differently by product/country.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="grossTotal" type="xs:decimal" minOccurs="0" sme:label="SalesQuotationLine Gross Total">
				<xs:annotation>
					<xs:documentation>
            Total value of the Sales Quotation Line, including discounts &amp;
            tax &amp; charges.   In Quotation currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="costTotal" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Cost Total">
				<xs:annotation>
					<xs:documentation>
            Total cost of the Sales Quotation Line.   In Quotation currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="profitTotal" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Profit Total">
				<xs:annotation>
					<xs:documentation>
            Total profit on the Sales Quotation Line (e.g. net total – cost total).
              In Quotation currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="buyerContact" type="contact--type" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Buyer Contact" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Link to the Sales Quotation Line originator (buyer Contact).   See
            “Contact” definition. This could be derived from the Sales Quotation
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="project" type="project--type" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Project" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Identifier of the Project this Sales Quotation Line is for.   See
            “Project” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="projectLine" type="projectLine--type" nillable="true" minOccurs="0" sme:label="SalesQuotationLine ProjectLine" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Project Line value for this Sales Quotation Line.   See “Project Line”
            definition. Some applications may not have Project Lines.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BOMLine" type="xs:string" nillable="true" minOccurs="0" sme:label="SalesQuotationLine BOM Line" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            BOM Line associated with the Sales Quotation Line   The ‘BOM Line’
            resource will be defined in the future. Not all applications support this feature.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="financialAccounts" type="sc:financialAccount--list" nillable="true" minOccurs="0" sme:label="SalesQuotationLine FinancialAccount" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Financial accounting code applicable to the Sales Quotation Line.   See
            “FinancialAccount” definition. This could be derived from the Sales Quotation
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="costCentres" type="sc:costCentre--list" nillable="true" minOccurs="0" sme:label="SalesQuotationLine CostCentres" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="departments" type="sc:department--list" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Departments" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="notes" type="sc:note--list" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Note" sme:relationship="child" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation> Notes associated with a Sales Quotation Line.   </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
		<xs:anyAttribute/>
	</xs:complexType>
	<xs:complexType name="salesQuotationLine--list">
		<xs:sequence>
			<xs:element name="salesQuotationLine" type="salesQuotationLine--type" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:anyAttribute/>
	</xs:complexType>
	<xs:element name="commodity" type="commodity--type" sme:role="resourceKind" sme:pluralName="commodities" sme:compliance="must" sme:canGet="true" sme:hasUuid="true" sme:isSyncSource="true"/>
	<xs:complexType name="commodity--type">
		<xs:all>
			<xs:element name="active" type="xs:boolean" minOccurs="0" sme:label="Commodity Active Flag">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate if the Commodity is an ‘active’ one .   Default = True
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="commodityGroup" type="commodityGroup--type" minOccurs="0" sme:label="Commodity CommodityGroup" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Identifier of the Commodity group this Commodity belongs to.   See
            “CommodityGroup” definition. Commodities can only belong to one Commodity Group.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="commodityIdentifierType" type="commodityCommodityIdentifierType--enum" nillable="true" minOccurs="0" sme:label="Commodity CommodityIdentifier Type">
				<xs:annotation>
					<xs:documentation>
            Type of the Commodity Identifier associated with this Commodity (if
            any) (e.g. Total, Batch (lot), Serial Number).   Total Batch/Lot Serial Serial from
            Batch/Lot Unknown Default = “Total”
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="commodityIdentifiers" type="commodityIdentifier--list" nillable="true" minOccurs="0" sme:label="Commodity CommodityIdentifier" sme:relationship="child" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Link to the Commodity Identifier(s) for this Commodity, if it is traced
            to that level of detail.   See “CommodityIdentifier” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference" type="xs:string" minOccurs="0" sme:label="Commodity reference" sme:isUniqueKey="true">
				<xs:annotation>
					<xs:documentation>
            reference for the Commodity (typically the one used by the buying
            organization).  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference2" type="xs:string" nillable="true" minOccurs="0" sme:label="Commodity Reference2" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation> Another reference for the Commodity.   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="status" type="xs:string" nillable="true" minOccurs="0" sme:label="Commodity Status" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation> Status of the Commodity.   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="name" type="xs:string" minOccurs="0" sme:label="Commodity Name" sme:isMandatory="true">
				<xs:annotation>
					<xs:documentation> Short name for the Commodity.   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="shortName" type="xs:string" nillable="true" minOccurs="0" sme:label="Commodity Short Name">
				<xs:annotation>
					<xs:documentation> A shorter name for the Commodity.   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="description" type="xs:string" nillable="true" minOccurs="0" sme:label="Commodity Description">
				<xs:annotation>
					<xs:documentation> The Commodity description.   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="alternativeDescription1" type="xs:string" nillable="true" minOccurs="0" sme:label="Commodity Alternative Description 1" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            An alternative Commodity description (e.g. buying or selling
            descriptions may be different).  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="alternativeDescription2" type="xs:string" nillable="true" minOccurs="0" sme:label="Commodity Alternative Description 2" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Another alternative Commodity description (e.g. could be used for
            special Delivery instructions, handling instructions, or alternative language
            descriptions, etc).  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="type" type="commodityType--enum" minOccurs="0" sme:label="Commodity Type" sme:isMandatory="true">
				<xs:annotation>
					<xs:documentation>
            The Commodity type (e.g. product, service).   Default value should come
            from CommodityGroup. Product Stock (e.g. finished goods and/or raw materials) Product
            Non Stock (e.g. Misc or expense). Service (contracts). BillOfMaterial (Manufactured
            Item) (e.g. assembled Kit). (Built to Order) Kit. Subcontract Description (e.g.
            comment). User defined. Unknown. Default = “Unknown”
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="unitOfMeasure" type="unitOfMeasure--type" minOccurs="0" sme:label="Commodity UnitOfMeasure" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Link to the Unit of Measure (pack size / quantity) for the Commodity
            (e.g. each, six-pack, case, etc). Also known as stocking Unit of Measure.   See
            “UnitOfMeasure” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="buyingAllowedFlag" type="xs:boolean" nillable="true" minOccurs="0" sme:label="Commodity Buying Allowed Flag">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate that this Commodity can be ‘bought’ i.e. can be used
            on purchase orders/requisitions.   Default = Yes
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="buyingUnitOfMeasure" type="unitOfMeasure--type" nillable="true" minOccurs="0" sme:label="Commodity Buying UnitOfMeasure" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Link to the Unit of Measure (pack size / quantity) for the Commodity
            used during buying (e.g. each, six-pack, case, etc)   See “UnitOfMeasure” definition.
            Default = Commodity UnitOfMeasure
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="buyingUnitOfMeasureNumber" type="xs:decimal" nillable="true" minOccurs="0" sme:label="Commodity Buying UnitOfMeasure Number">
				<xs:annotation>
					<xs:documentation>
            The number (quantity) of items in a pack of the Commodity (e.g. 1, 6,
            24, etc).   Default = 1
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="sellingAllowedFlag" type="xs:boolean" nillable="true" minOccurs="0" sme:label="Commodity Selling Allowed Flag">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate that this Commodity can be ‘sold’ i.e. can be used on
            sales orders/quotations.   Default = Yes
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="sellingUnitOfMeasure" type="unitOfMeasure--type" nillable="true" minOccurs="0" sme:label="Commodity Selling UnitOfMeasure" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Link to the Unit of Measure (pack size / quantity) for the Commodity
            used during selling/retail (e.g. each, six-pack, case, etc)   See “UnitOfMeasure”
            definition. Default = Commodity UnitOfMeasure
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="sellingUnitOfMeasureNumber" type="xs:decimal" nillable="true" minOccurs="0" sme:label="Commodity Selling UnitOfMeasure Number">
				<xs:annotation>
					<xs:documentation>
            The number (quantity) of items in a pack of the Commodity (e.g. 1, 6,
            24, etc).   1
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="packingUnitOfMeasure" type="unitOfMeasure--type" nillable="true" minOccurs="0" sme:label="Commodity Packing UnitOfMeasure" sme:relationship="reference" sme:isCollection="false" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Link to the Unit of Measure (pack size / quantity) for the Commodity
            used during packing/dispatch (e.g. each, six-pack, case, etc)   See “UnitOfMeasure”
            definition. This could be the default or buying Unit of Measure.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="packingUnitOfMeasureNumber" type="xs:decimal" nillable="true" minOccurs="0" sme:label="Commodity Packing UnitOfMeasure Number" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            The number (quantity) of items in a pack of the Commodity (e.g. 1, 6,
            24, etc).  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="catalogueNumber" type="xs:string" nillable="true" minOccurs="0" sme:label="Commodity Catalogue Number">
				<xs:annotation>
					<xs:documentation>
            reference for the Commodity in a Supplier catalogue.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="supplierTradingAccount" type="tradingAccount--list" nillable="true" minOccurs="0" sme:label="Commodity Supplier TradingAccount" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Link to the Commodity Supplier trading account.   See “TradingAccount”
            definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="supplierReference" type="xs:string" nillable="true" minOccurs="0" sme:label="Commodity Supplier reference">
				<xs:annotation>
					<xs:documentation>
            reference for the Commodity as used by the Supplier.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="manufacturerCompany" type="xs:string" nillable="true" minOccurs="0" sme:label="Commodity Manufacturer Company" sme:isCollection="false" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Link to the Company associated with manufacturing the Commodity.   See
            “Company” definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="manufacturerReference" type="xs:string" nillable="true" minOccurs="0" sme:label="Commodity Manufacturer reference" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            reference for the Commodity as used by the manufacturer.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="material" type="xs:string" nillable="true" minOccurs="0" sme:label="Commodity Material" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation> Material from which the Commodity is made.   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="countryOfOrigin" type="xs:string" nillable="true" minOccurs="0" sme:label="Commodity Country Of Origin">
				<xs:annotation>
					<xs:documentation>
            The country where the Commodity originated.   ISO 3166-1 alpha-2
            country codes only Could default from CommodityGroup.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="weight" type="xs:decimal" nillable="true" minOccurs="0" sme:label="Commodity Weight">
				<xs:annotation>
