          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="salesQuotations" type="salesQuotation--list" nillable="true" minOccurs="0" sme:label="Opportunity SalesQuotation" sme:relationship="association" sme:isCollection="true">
