          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="receiptLines" type="receiptLine--list" nillable="true" minOccurs="0" sme:label="PurchaseCreditLine ReceiptLine" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
