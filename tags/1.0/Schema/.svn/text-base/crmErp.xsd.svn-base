          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="financialAccounts" type="sc:financialAccount--list" nillable="true" minOccurs="0" sme:label="PurchaseCreditLine FinancialAccount" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
