			<xs:element name="date" type="xs:date" nillable="true" minOccurs="0" sme:label="Opportunity Date">
				<xs:annotation>
					<xs:documentation> The date the Opportunity was generated.   </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="closeDate" type="xs:date" nillable="true" minOccurs="0" sme:label="Opportunity Close Date">
