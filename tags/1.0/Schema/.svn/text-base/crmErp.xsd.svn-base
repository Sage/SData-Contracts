				</xs:annotation>
			</xs:element>
			<xs:element name="costCentres" type="sc:costCentre--list" nillable="true" minOccurs="0" sme:label="SalesCreditLine CostCentres" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="departments" type="sc:department--list" nillable="true" minOccurs="0" sme:label="SalesCreditLine Departments" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="notes" type="sc:note--list" nillable="true" minOccurs="0" sme:label="SalesCreditLine Note" sme:relationship="child" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
