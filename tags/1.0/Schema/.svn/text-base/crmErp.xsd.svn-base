					<xs:documentation>
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="interactions" type="interaction--list" nillable="true" minOccurs="0" sme:label="PurchaseRequisition Interaction" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
