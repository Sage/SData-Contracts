				<xs:annotation>
					<xs:documentation>
            Pricing Document Discount Type	Flag to indicate the type of discount applicable to this document (percent or amount)

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="discountAmount" type="xs:decimal" sme:label="Discount Amount">
				<xs:annotation>
					<xs:documentation>
            Pricing Document Discount Amount	Discount applicable to this document expressed as an amount.

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="discountPercent" type="xs:decimal" sme:label="Discount Percent">
				<xs:annotation>
					<xs:documentation>
            Pricing Document Discount Percent	Discount applicable to this document expressed as a percent.

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="settlementDiscountType" type="discountType--enum" sme:label="Settlement Discount Type">
				<xs:annotation>
					<xs:documentation>
            Pricing Settlement Discount Type	Flag to indicate the type of settlement discount applicable to the Trading Account (percent or amount)
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="settlementDiscountPercent" type="xs:decimal" sme:label="Settlement Discount Percent">
				<xs:annotation>
					<xs:documentation>
            Pricing Settlement Discount Percent	Settlement discount applicable to the Trading Account expressed as a percent.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="additionalDiscount1Type" type="discountType--enum" sme:label="Order Additional Discount1 Type">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate the type of additional discount 1 applicable to this Pricing Document  (percent or amount)

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="additionalDiscount1Amount" type="xs:decimal" sme:label="Order Additional Discount1 Amount">
				<xs:annotation>
					<xs:documentation>
            Additional discount 1 applicable to the Pricing Document expressed as an amount.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="additionalDiscount1Percent" type="xs:decimal" sme:label="Order Additional Discount1 Percent">
				<xs:annotation>
					<xs:documentation>
            Additional discount 1 applicable to the Pricing Document expressed as a percent.

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="additionalDiscount2Type" type="discountType--enum" sme:label="Order Additional Discount2 Type">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate the type of additional discount 2 applicable to this Pricing Document  (percent or amount)

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="additionalDiscount2Amount" type="xs:decimal" sme:label="Order Additional Discount2 Amount">
				<xs:annotation>
					<xs:documentation>
            Additional discount 2 applicable to the Pricing Document expressed as an amount.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="additionalDiscount2Percent" type="xs:decimal" sme:label="Order Additional Discount2 Percent">
				<xs:annotation>
					<xs:documentation>
            Additional discount 2 applicable to the Pricing Document expressed as a percent.

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="netTotal" type="xs:decimal" sme:label="Net Total">
				<xs:annotation>
					<xs:documentation>
            Total value of the associated document, including discounts, excluding tax.

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="discountTotal" type="xs:decimal" sme:label="Discount Total">
				<xs:annotation>
					<xs:documentation>
            Pricing Document Discount Total	Total value of discounts applied to the associated document (all lines).

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxTotal" type="xs:decimal" sme:label="Tax Total">
				<xs:annotation>
					<xs:documentation>
            Total tax applicable to the associated document (all lines).
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="grossTotal" type="xs:decimal" sme:label="Gross Total">
				<xs:annotation>
					<xs:documentation>
            Gross Total	Total value of the associated document (all lines) including tax and discounts and charges.

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="costTotal" type="xs:decimal" sme:label="Cost Total">
				<xs:annotation>
					<xs:documentation>
            Cost Total	Total cost of the Document.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="profitTotal" type="xs:decimal" sme:label="Profit Total">
				<xs:annotation>
					<xs:documentation>
            Profit Total	Total profit on the Document(e.g. net total – cost total).
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="additionalText" type="xs:string" sme:label="Additional Text">
				<xs:annotation>
					<xs:documentation>
            Additional text returned by the pricing request.  This is intended to be a short message for display on the same screen as the document. This could display for example textual information on the discount applied.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="pricingDocumentLines" type="pricingDocumentLine--list" sme:label="Pricing Lines" sme:isCollection="true" sme:relationship="child">
