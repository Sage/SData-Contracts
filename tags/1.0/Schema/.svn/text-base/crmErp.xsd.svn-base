          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customerTradingAccountCurrencyExchangeRateOperator" type="RateOperator--enum" nillable="true" minOccurs="0" sme:label="SalesOrderDelivery Customer TradingAccount Currency Exchange Rate Operator">
