          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierTradingAccount" type="tradingAccount--type" nillable="true" minOccurs="0" sme:label="PurchaseOrderDelivery Carrier TradingAccount" sme:relationship="reference" sme:isCollection="false" sme:isExtension="true">
