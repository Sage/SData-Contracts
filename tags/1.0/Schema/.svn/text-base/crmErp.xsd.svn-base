			<xs:element name="invoiceDiscountType" type="discountType--enum" nillable="true" minOccurs="0" sme:label="PurchaseInvoice Invoice Discount Type">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate the type of discount applicable to the Purchase
            Invoice (percent or amount)   From the Trading Account.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceDiscountAmount" type="xs:decimal" nillable="true" minOccurs="0" sme:label="PurchaseInvoice Invoice Discount Amount">
				<xs:annotation>
					<xs:documentation>
            Any discount that may be applicable to the Purchase Invoice, expressed
            as an amount.   From the Trading Account.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceDiscountPercent" type="xs:decimal" nillable="true" minOccurs="0" sme:label="PurchaseInvoice Invoice Discount Percent">
				<xs:annotation>
					<xs:documentation>
            Any discount that may be applicable to the Purchase Invoice, expressed
            as a percentage.   From the Trading Account.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceAdditionalDiscount1Type" type="discountType--enum" nillable="true" minOccurs="0" sme:label="PurchaseInvoice Invoice Additional Discount1 Type">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate the type of discount applicable to this Purchase
            Invoice (percent or amount)  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceAdditionalDiscount1Amount" type="xs:decimal" nillable="true" minOccurs="0" sme:label="PurchaseInvoice Invoice Additional Discount1 Amount">
				<xs:annotation>
					<xs:documentation>
            Discount applicable to the Purchase Invoice expressed as an amount.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceAdditionalDiscount1Percent" type="xs:decimal" nillable="true" minOccurs="0" sme:label="PurchaseInvoice Invoice Additional Discount1 Percent">
				<xs:annotation>
					<xs:documentation>
            Discount applicable to the Purchase Invoice expressed as a percent.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceAdditionalDiscount2" type="discountType--enum" nillable="true" minOccurs="0" sme:label="PurchaseInvoice Invoice Additional Discount2 Type" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate the type of discount applicable to this Purchase
            Invoice (percent or amount)  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceAdditionalDiscount2Amount" type="xs:decimal" nillable="true" minOccurs="0" sme:label="PurchaseInvoice Invoice Additional Discount2 Amount" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Discount applicable to the Purchase Invoice expressed as an amount.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceAdditionalDiscount2Percent" type="xs:decimal" nillable="true" minOccurs="0" sme:label="PurchaseInvoice Invoice Additional Discount2 Percent" sme:isExtension="true">
				<xs:annotation>
					<xs:documentation>
            Discount applicable to the Purchase Invoice expressed as a percent.  
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="netTotal" type="xs:decimal" minOccurs="0" sme:label="PurchaseInvoice Net Total">
				<xs:annotation>
					<xs:documentation>
            Total value of the Purchase Invoice, including discounts, excluding
            tax.   In Purchase Invoice currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="discountTotal" type="xs:decimal" minOccurs="0" sme:label="PurchaseInvoice Discount Total">
				<xs:annotation>
					<xs:documentation>
            Total discounts applied to the Purchase Invoice.   In Purchase Invoice
            currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="chargesTotal" type="xs:decimal" nillable="true" minOccurs="0" sme:label="PurchaseInvoice Charges Total">
				<xs:annotation>
					<xs:documentation>
            Total value of any (sur)charges applied to the Purchase Invoice (e.g.
            finance charges, interest, landed costs).   In Purchase Invoice currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxTotal" type="xs:decimal" minOccurs="0" sme:label="PurchaseInvoice Tax Total">
				<xs:annotation>
					<xs:documentation>
            Total tax applicable to the Purchase Invoice.   In Purchase Invoice
            currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
