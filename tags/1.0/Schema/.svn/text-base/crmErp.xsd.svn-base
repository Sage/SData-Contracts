	</xs:complexType>
	<xs:element name="paymentLine" type="paymentLine--type" sme:role="resourceKind" sme:pluralName="paymentLines" sme:compliance="may" sme:canGet="true"/>
	<xs:complexType name="paymentLine--type">
		<xs:all>
