<?xml version="1.0" encoding="utf-8"?>
<?xml-stylesheet type="text/xsl" href="../Tools/XsdToHtml.xslt"?>
<xs:schema id="hrmErp" elementFormDefault="qualified" targetNamespace="http://schemas.sage.com/hrmErp/2009" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:sme="http://schemas.sage.com/sdata/sme/2007" xmlns="http://schemas.sage.com/hrmErp/2009" xmlns:hrmErp="http://schemas.sage.com/hrmErp/2009" xmlns:sc="http://schemas.sage.com/sc/2009" xmlns:crmErp="http://schemas.sage.com/crmErp/2008" xmlns:famErp="http://schemas.sage.com/famErp/2009">
  <xs:import namespace="http://schemas.sage.com/sc/2009" schemaLocation="common.xsd"/>
  <xs:import namespace="http://schemas.sage.com/crmErp/2008" schemaLocation="crmErp.xsd" />
  <xs:import namespace="http://schemas.sage.com/famErp/2009" schemaLocation="famErp.xsd"/>
  <xs:element name="organization" type="organization--type" sme:role="resourceKind" sme:pluralName="organizations" sme:compliance="may" sme:canGet="true" sme:canPut="true" sme:canPost="true" sme:canDelete="true" />
  <xs:complexType name="organization--type">
    <xs:all>
      <xs:element name="active" sme:label="Organization Active Flag" minOccurs="0" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
            Flag to indicate if the Organization is an ‘active’ Organization .
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="operatingCompany" sme:label="Organization OperatingCompany" minOccurs="0" type="sc:operatingCompany--type" sme:relationship="reference" sme:isCollection="false">
        <xs:annotation>
          <xs:documentation>
            Link to the OperatingCompany associated with the Organization.
             
            See “OperatingCompany” definition (Company where type = “Operating Company”).
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="reference" sme:label="Organization Reference" minOccurs="0" type="xs:string" sme:isUniqueKey="true">
        <xs:annotation>
          <xs:documentation>
            Reference for the Organization (e.g. “R&amp;D 14”).
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="reference2" sme:label="Organization Reference2" minOccurs="0" type="xs:string" sme:isExtension="true" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Additional reference for the Organization.
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="status" sme:label="Organization Status" minOccurs="0" type="xs:string" sme:isExtension="true" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Status of the Organization.
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="name" sme:label="Organization Name" minOccurs="0" type="xs:string" sme:isMandatory="true">
        <xs:annotation>
          <xs:documentation>
            The Organization name (e.g. “Career Family”, “R&amp;D”, “Sales”)
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="type" sme:label="Organization Type" minOccurs="0" type="xs:string">
        <xs:annotation>
          <xs:documentation>
            The Organization type (e.g. “Job Family”, “Unit”)
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="positions" sme:label="Organization Position" minOccurs="0" type="position--list" sme:relationship="association" sme:isCollection="true" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Link to the Position(s) associated with the Organization.
             
            See “Position” definition.

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="levelNumber" sme:label="Organization Level Number" minOccurs="0" type="xs:decimal" sme:isMandatory="true">
        <xs:annotation>
          <xs:documentation>
            The level associated with the Position within the Organization in numeric format (e.g. 1-15 in the Sage UK Career Framework or Sorting Level of Organizational Units).
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="levelText" sme:label="Organization Level Text" minOccurs="0" type="xs:string" nillable="true">
        <xs:annotation>
          <xs:documentation>
            The level associated with the Position within the Organization – in textual format.
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="bandNumber" sme:label="Organization Band Number" minOccurs="0" type="xs:decimal" nillable="true">
        <xs:annotation>
          <xs:documentation>
            The band associated with the Position within the Organization in numeric format.
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="bandText" sme:label="Organization Band Text" minOccurs="0" type="xs:string" nillable="true">
        <xs:annotation>
          <xs:documentation>
            The band associated with the Position within the Organization – in textual format (e.g. A-D in the Sage UK Career Framework).
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="associatedParentOrganization" sme:label="Organization Associated Parent Organization" minOccurs="0" type="organization--type" sme:relationship="parent" sme:isCollection="false" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Link to Parent Organization associated with this Organization e.g. into which an Employee would be promoted.
             
            Organization are usually hierarchical to represent a career path.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="associatedChildOrganizations" sme:label="Organization Associated Child Organization" minOccurs="0" type="organization--list" sme:relationship="child" sme:isCollection="true" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Link to Child Organization associated with this Organization e.g. from which an Employee has been promoted or could be demoted.
             
            Organization are usually hierarchical to represent a career path.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="postalAddresses" sme:label="Organization PostalAddress " minOccurs="0" type="sc:postalAddress--list" sme:relationship="association" sme:isCollection="true" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Link to the main Organization address(es)
             
            See “PostalAddress” definition.

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="phones" sme:label="Organization Phone" minOccurs="0" type="sc:phoneNumber--list" sme:relationship="association" sme:isCollection="true" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Organization telephone number(s).
             
            See “PhoneNumber” definition.

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="defaultWorkplace" sme:label="Organization Default Workplace" minOccurs="0" type="workplace--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
        <xs:annotation>
          <xs:documentation>
            The default Workplace associated with the Organization.
             
            See “Workplace” definition.
            .
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="financialAccounts" sme:label="Organization FinancialAccount" minOccurs="0" type="financialAccount--list" sme:relationship="association" sme:isCollection="true" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Financial (e.g. cost centre) account associated with the Organization.
             
            See “FinancialAccount” definition.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="costCentres" type="sc:costCentre--list" nillable="true" minOccurs="0" sme:label="Organization CostCentre" sme:relationship="association" sme:isCollection="true">
        <xs:annotation>
          <xs:documentation></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="departments" type="sc:department--list" nillable="true" minOccurs="0" sme:label="Organization Department" sme:relationship="association" sme:isCollection="true">
        <xs:annotation>
          <xs:documentation></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="text1" sme:label="Organization Text1" minOccurs="0" type="xs:string" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Textual information associated with the Organization.
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="text2" sme:label="Organization Text2" minOccurs="0" type="xs:string" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Textual information associated with the Organization.
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="startDate" sme:label="Organization Start Date" minOccurs="0" type="xs:date" nillable="true">
        <xs:annotation>
          <xs:documentation>
            A start date associated with the Organization (if applicable).
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="endDate" sme:label="Organization End Date" minOccurs="0" type="xs:date" nillable="true">
        <xs:annotation>
          <xs:documentation>
            A start date associated with the Organization (if applicable) – e,g, to support restructuring of an Organization.
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="notes" sme:label="Organization Note" minOccurs="0" type="sc:note--list" sme:relationship="child" sme:isCollection="true" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Notes associated with an Organization.
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>
    <xs:anyAttribute />
  </xs:complexType>
  <xs:complexType name="organization--list">
    <xs:sequence minOccurs="0" maxOccurs="unbounded">
      <xs:element name="organization" type="organization--type" />
    </xs:sequence>
    <xs:anyAttribute />
  </xs:complexType>
  <xs:element name="position" type="position--type" sme:role="resourceKind" sme:pluralName="positions" sme:compliance="may" sme:canGet="true" sme:canPut="true" sme:canPost="true" sme:canDelete="true" />
  <xs:complexType name="position--type">
    <xs:all>
      <xs:element name="active" sme:label="Position Active Flag" minOccurs="0" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
            Flag to indicate if the Position is an ‘active’ Position.
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="operatingCompany" sme:label="Position OperatingCompany" minOccurs="0" type="sc:operatingCompany--type" sme:relationship="reference" sme:isCollection="false">
        <xs:annotation>
          <xs:documentation>
            Link to the OperatingCompany associated with the Position.
             
            See “OperatingCompany” definition (Company where type = “Operating Company”).
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="department" sme:label="Position Department" minOccurs="0" type="xs:string" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Department within the Operating Company the Position is for.
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="organization" sme:label="Position Organization" minOccurs="0" type="organization--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Link to the Organization associated with this Position.
             
            See “Organization” definition.

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="reference" sme:label="Position Reference" minOccurs="0" type="xs:string" sme:isUniqueKey="true">
        <xs:annotation>
          <xs:documentation>
            Reference for the Position (e.g. “JOB001”).
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="reference2" sme:label="Position Reference2" minOccurs="0" type="xs:string" sme:isExtension="true" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Additional reference for the Position.
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="status" sme:label="Position Status" minOccurs="0" type="positionStatus--enum">
        <xs:annotation>
          <xs:documentation>
            Status of the Position
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="name" sme:label="Position Name" minOccurs="0" type="xs:string" sme:isMandatory="true">
        <xs:annotation>
          <xs:documentation>
            The Position name (typically the Job name, e.g. “R&amp;D Product Development Manager”
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="shortName" sme:label="Position Short Name" minOccurs="0" type="xs:string" nillable="true">
        <xs:annotation>
          <xs:documentation>
            A shorter name for the Position (e.g. “R&amp;D PDM”).
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="type" sme:label="Position Type" minOccurs="0" type="positionType--enum">
        <xs:annotation>
          <xs:documentation>
            The Position type (e.g. Permanent, Part-Time, etc).
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="employment" sme:label="Position Employment" minOccurs="0" type="employment--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Employment (contract) associated with the Position. This may be prospective terms if the Position is Vacant, or actual terms if the Position is filled.
             
            See “Employment” definition.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="workplace" sme:label="Position Workplace" minOccurs="0" type="workplace--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
        <xs:annotation>
          <xs:documentation>
            The Workplace associated with the Position.
             
            See “Workplace” definition.
            .
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="postalAddresses" sme:label="Position PostalAddress " minOccurs="0" type="sc:postalAddress--list" sme:relationship="association" sme:isCollection="true" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Link to the current Position address(es)
             
            See “PostalAddress” definition.

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="phones" sme:label="Position Phone" minOccurs="0" type="sc:phoneNumber--list" sme:relationship="association" sme:isCollection="true" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Position telephone number(s).
             
            See “PhoneNumber” definition.

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="emails" sme:label="Position Email" minOccurs="0" type="sc:email--list" sme:relationship="association" sme:isCollection="true" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Position email address(es).
             
            See “Email” definition.

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="employees" sme:label="Position Employee" minOccurs="0" type="employee--list" sme:relationship="association" sme:isCollection="true" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Link to the Employee(s) associated with the Position (if any – may be Vacant).
             
            See “Employee” definition.

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="managerEmployee" sme:label="Position Manager Employee" minOccurs="0" type="employee--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
        <xs:annotation>
          <xs:documentation>
            A link to manager’s Employee record.
             
            See “Employee” definition.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="text1" sme:label="Position Text1" minOccurs="0" type="xs:string" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Textual information associated with the Position.
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="text2" sme:label="Position Text2" minOccurs="0" type="xs:string" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Textual information associated with the Position.
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="startedDate" sme:label="Position Started Date" minOccurs="0" type="xs:date" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Date the Position was (or is to be) originated (e.g. Secondment).
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="endeDate" sme:label="Position Ended Date" minOccurs="0" type="xs:date" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Date the Position was (or is to be) closed.
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="competenciesRequired" sme:label="Position Competencies Required" minOccurs="0" type="competency--list" sme:relationship="association" sme:isCollection="true" sme:isExtension="true" nillable="true">
        <xs:annotation>
          <xs:documentation>
            A link to the Competencies required of the Position.
             
            See “Competency” definition.

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="primacyIndicator" sme:label="Position Primacy Indicator" minOccurs="0" type="xs:boolean" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Flag to indicate if this Position is the primary one (e.g. for the associated Employee).
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="externalPositionFlag" sme:label="Position External Position Flag" minOccurs="0" type="xs:boolean" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Flag to indicate if this position is held externally to the employing company (e.g. where a record is being kept of voluntary, governmental, supplementary positions).
             
            True = External position

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="notes" sme:label="Position Note" minOccurs="0" type="sc:note--list" sme:relationship="child" sme:isCollection="true" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Notes associated with a Position.
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>
    <xs:anyAttribute />
  </xs:complexType>
  <xs:complexType name="position--list">
    <xs:sequence minOccurs="0" maxOccurs="unbounded">
      <xs:element name="position" type="position--type" />
    </xs:sequence>
    <xs:anyAttribute />
  </xs:complexType>
  <xs:element name="vacancy" type="vacancy--type" sme:role="resourceKind" sme:pluralName="vacancies" sme:compliance="may" sme:canGet="true" sme:canPut="true" sme:canPost="true" sme:canDelete="true" />
  <xs:complexType name="vacancy--type">
    <xs:all>
      <xs:element name="active" sme:label="Vacancy Active Flag" minOccurs="0" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
            Flag to indicate if the Vacancy is an ‘active’ Vacancy.
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="operatingCompany" sme:label="Vacancy OperatingCompany" minOccurs="0" type="sc:operatingCompany--type" sme:relationship="reference" sme:isCollection="false">
        <xs:annotation>
          <xs:documentation>
            Link to the OperatingCompany associated with the Vacancy.
             
            See “OperatingCompany” definition (Company where type = “Operating Company”).
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="organization" sme:label="Vacancy Organization" minOccurs="0" type="organization--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Link to the Organization associated with this Vacancy.
             
            See “Organization” definition.

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="position" sme:label="Vacancy Position" minOccurs="0" type="position--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Link to the Position the Vacancy is for.
             
            See “Position” definition.

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="reference" sme:label="Vacancy Reference" minOccurs="0" type="xs:string" sme:isUniqueKey="true">
        <xs:annotation>
          <xs:documentation>
            Reference for the Vacancy (e.g. “000456”).
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="reference2" sme:label="Vacancy Reference2" minOccurs="0" type="xs:string" sme:isExtension="true" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Additional reference for the Vacancy.
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="status" sme:label="Vacancy Status" minOccurs="0" type="vacancyStatus--enum">
        <xs:annotation>
          <xs:documentation>
            Status of the Vacancy
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="name" sme:label="Vacancy Name" minOccurs="0" type="xs:string" sme:isMandatory="true">
        <xs:annotation>
          <xs:documentation>
            The Vacancy name (typically the Job name, e.g. “R&amp;D Product Development Manager”
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="shortName" sme:label="Vacancy Short Name" minOccurs="0" type="xs:string" nillable="true">
        <xs:annotation>
          <xs:documentation>
            A shorter name for the Vacancy (e.g. “R&amp;D PDM”).
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="type" sme:label="Vacancy Type" minOccurs="0" type="vacancyType--enum">
        <xs:annotation>
          <xs:documentation>
            The Vacancy type (e.g. Permanent, Part-Time, etc).
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="positionsAvailable" sme:label="Vacancy Positions Available" minOccurs="0" type="xs:decimal" sme:isMandatory="true" nillable="true">
        <xs:annotation>
          <xs:documentation>
            A numeric value indicating the number of Vacancy Positions that are available.
             
            Default = 1
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="createdDate" sme:label="Vacancy Created Date" minOccurs="0" type="xs:date" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Date the Vacancy was originated.
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="targetStartDate" sme:label="Vacancy Target Start Date" minOccurs="0" type="xs:date" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Target start date for the Vacancy (i.e. when Employee expected to start Employment).
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="applicationClosingDate" sme:label="Vacancy Application Closing Date" minOccurs="0" type="xs:date" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Application closing date, after which no new Candidates will be accepted.
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="applicationMethod" sme:label="Vacancy Application Method" minOccurs="0" type="xs:string" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Value indicating the preferred application method for the Vacancy (e.g. Web, Post, Email, Agency, etc).
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="assessmentStartingDate" sme:label="Vacancy Assessment Starting Date" minOccurs="0" type="xs:date" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Date when Candidates will be assessed (e.g. when interviews, etc commence)
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="assessingEmployees" sme:label="Vacancy Assessing Employee" minOccurs="0" type="employee--list" sme:relationship="association" sme:isCollection="true" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Details of the Employee doing the Candidate assessment. Could be many as this could be a recruiting panel.
             
            See “Employee” definition.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="administratingEmployees" sme:label="Vacancy Administrating Employee" minOccurs="0" type="employee--list" sme:relationship="association" sme:isCollection="true" nillable="true">
        <xs:annotation>
          <xs:documentation>
            A link to the Employee who administers the Vacancy.
             
            See “Employee” definition.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
