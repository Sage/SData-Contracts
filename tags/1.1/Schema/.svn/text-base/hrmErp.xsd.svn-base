          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="clawbackAmount" sme:label="Training Clawback Amount" minOccurs="0" type="xs:decimal" nillable="true">
        <xs:annotation>
          <xs:documentation>
            Amount that could be reclaimed from an Employee if a claw back clause is invoked.
             

          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="notes" sme:label="TrainingEvent Note" minOccurs="0" type="sc:note--list" sme:relationship="child" sme:isCollection="true" nillable="true">
        <xs:annotation>
          <xs:documentation>
