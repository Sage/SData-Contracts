			<xs:element name="notes" sme:label="Supplier Note" minOccurs="0" type="note--list" sme:relationship="child" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Notes associated with a Supplier.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
	</xs:complexType>
	<xs:complexType name="supplier--list">
		<xs:sequence minOccurs="0" maxOccurs="unbounded">
			<xs:element name="supplier" type="supplier--type" />
		</xs:sequence>
	</xs:complexType>
	<xs:element name="tradingAccount" type="tradingAccount--type" sme:role="resourceKind" sme:pluralName="tradingAccounts" sme:compliance="must" sme:canGet="true" sme:canPut="true" sme:canPost="true" sme:canDelete="true" />
	<xs:complexType name="tradingAccount--type">
		<xs:all>
			<xs:element name="id" sme:label="TradingAccount Application ID" minOccurs="0" type="xs:string" sme:isUniqueKey="true">
				<xs:annotation>
					<xs:documentation>
						The application specific identifier.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="active" sme:label="TradingAccount Active Flag" minOccurs="0" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>
						Flag to indicate if the Trading Account is an ‘active’ account .
						 
						Default = True
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deleted" sme:label="Trading Account Deleted" minOccurs="0" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>
						Flag to indicate if Trading Account is (soft) deleted.
						 
						Default = False
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="label" sme:label="TradingAccount Label" minOccurs="0" type="xs:string">
				<xs:annotation>
					<xs:documentation>
						The label for the resource to be presented in the user interface. To support application-specific or language-specific descriptions of resources.
						 
						“Trading Account”
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customerSupplierFlag" sme:label="TradingAccount Customer Supplier Flag" minOccurs="0" type="xs:string">
				<xs:annotation>
					<xs:documentation>
						A flag to indicate if the Trading Account is a Customer or Supplier.
						 
						This can be derived from the entity linked to this Trading Account (either “Customer” or “Supplier”).
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customer" sme:label="TradingAccount Customer" minOccurs="0" type="customer--type" sme:relationship="parent" sme:isCollection="false" nillable="true">
				<xs:annotation>
					<xs:documentation>
						A link to the Customer associated with this Trading Account (if Customer Supplier Flag = “Customer”).
						 
						See “Customer” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="supplier" sme:label="TradingAccount Supplier" minOccurs="0" type="supplier--type" sme:relationship="parent" sme:isCollection="false" nillable="true">
				<xs:annotation>
					<xs:documentation>
						A link to the Supplier associated with this Trading Account (if Customer Supplier Flag = “Supplier”).
						 
						See “Supplier” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceTradingAccount" sme:label="TradingAccount Invoice TradingAccount" minOccurs="0" type="tradingAccount--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to the Trading Account to be invoiced for transactions (e.g Sales Orders, Purchase Orders) if different from this TradingAccount.
						 
						See “TradingAccount” definition.

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="openedDate" sme:label="TradingAccount Opened Date" minOccurs="0" type="xs:date">
				<xs:annotation>
					<xs:documentation>
						Date the Trading Account was opened.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference" sme:label="TradingAccount reference" minOccurs="0" type="xs:string" sme:isUniqueKey="true">
				<xs:annotation>
					<xs:documentation>
						reference for the Trading Account.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference2" sme:label="TradingAccount Reference2" minOccurs="0" type="xs:string" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Additional reference for the Trading Account.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operatingCompany" sme:label="TradingAccount OperatingCompany" minOccurs="0" type="company--type" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
						Link to the OperatingCompany associated with the Trading Account.
						 
						See “Company” definition (where Company Type = “Operating Company”).
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="status" sme:label="TradingAccount Status" minOccurs="0" type="xs:string" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Status of the Trading Account.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="name" sme:label="TradingAccount Name" minOccurs="0" type="xs:string" sme:mandatory="true">
				<xs:annotation>
					<xs:documentation>
						The Trading Account name.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="shortName" sme:label="TradingAccount Short Name" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						A shorter name for the Trading Account.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="type" sme:label="TradingAccount Type" minOccurs="0" type="tradingAccountType--enum" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The Trading Account type.
						 

						Default = “Unknown”

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="postalAddresses" sme:label="TradingAccount PostalAddress" minOccurs="0" type="postalAddress--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to the Trading Account address(es) (e.g. Invoice/billing address).
						 
						See “PostalAddress” definition.

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="shippingAddresses" sme:label="TradingAccount ShippingAddress" minOccurs="0" type="postalAddress--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to the Trading Account shipping address.
						 
						See “PostalAddress” definition.

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="billingAddresses" sme:label="TradingAccount BillingAddress" minOccurs="0" type="postalAddress--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to the Trading Account billing address.
						 
						See “PostalAddress” definition.

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="phones" sme:label="TradingAccount Phone" minOccurs="0" type="phoneNumber--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Trading account telephone number(s).
						 
						See “PhoneNumber” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="emails" sme:label="TradingAccount Email" minOccurs="0" type="email--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Trading account email address(es).
						 
						See “Email” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="website" sme:label="TradingAccount Web site" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Trading Account web site address.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="contacts" sme:label="TradingAccount Contact " minOccurs="0" type="contact--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to the accounting Contact(s) for this Customer.
						 
						See “Contact” definition.

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deliveryContact" sme:label="TradingAccount Delivery Contact" minOccurs="0" type="contact--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to the Contact for deliveries to this Trading Account.
						 
						Default = Primary Contact
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="salesPersons" sme:label="TradingAccount Salesperson" minOccurs="0" type="salesPerson--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Sales person(s) associated with the Trading Account.
						 
						See “SalesPerson” definition.
						Also known as Sales Agent or Manager in some applications.
						There may be multiple and commission may be spread across multiple sales people.

						Could default from Customer (or Supplier).
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deliveryMethod" sme:label="TradingAccount Delivery Method" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Preferred Delivery method to be used for the Trading Account (e.g. Courier, Collection ,etc)
						 
						Could default from Customer (or Supplier).
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deliveryRule" sme:label="TradingAccount Delivery Rule" minOccurs="0" type="xs:boolean" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Does the Trading Account accept full or partial deliveries.
						 
						True = Accepts partial deliveries;
						False = Does not accept partial deliveries.

						Default = True

						Could default from Customer (or Supplier).
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="currency" sme:label="TradingAccount Currency" minOccurs="0" type="xs:string">
				<xs:annotation>
					<xs:documentation>
						The Trading Account currency (e.g. GBP, USD, EUR).
						 
						ISO 4217.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxCode" sme:label="TradingAccount Tax Code" minOccurs="0" type="taxCode--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Tax code governing the tax regime applicable to this Trading Account.
						 
						See “TaxCode” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxReference" sme:label="TradingAccount Tax reference" minOccurs="0" type="xs:string" sme:isUniqueKey="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Tax reference associated with the Trading Account.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxationCountry" sme:label="TradingAccount Taxation Country" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The country whose tax regime applies to this Trading Account.
						 
						ISO 3166-1 alpha-2 country codes only.

						Could default from PostalAddress.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="useTaxInclusivePrices" sme:label="TradingAccount Use Tax Inclusive Prices" minOccurs="0" type="xs:boolean" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Flag to indicate if prices associated with this Trading Account are inclusive of tax, or not.
						 
						Default = No (prices are not inclusive of tax)
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="financeBalance" sme:label="TradingAccount Finance Balance" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Trading account financial balance.
						 
						In TradingAccount currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="financeLimit" sme:label="TradingAccount Finance Limit" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Trading account financial (credit) limit.
						 
						In TradingAccount currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="financeBalanceDate" sme:label="TradingAccount Finance Balance Date" minOccurs="0" type="xs:date" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The date upon which the finance balance was based.
						 
						Default = System date
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="financeStatusFlag" sme:label="TradingAccount Finance Status Flag" minOccurs="0" type="xs:boolean" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Trading account financial status (e.g. on hold (or “blocked”), off hold).
						 
						True is on hold, False is not on hold.

						Default = False
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="financeStatusText" sme:label="TradingAccount Finance Status Text" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Trading account financial status text. Please note: this is the label used to describe the Finance Status Flag above – it is not a status in its own right.
						 
						Default = “On Hold”
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="settlementDiscountType" sme:label="TradingAccount Settlement Discount Type" minOccurs="0" type="discountType--enum" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Flag to indicate the type of settlement discount applicable to the Trading Account (percent or amount)
						 

						Default = “Percent”
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="settlementDiscountAmount" sme:label="TradingAccount Settlement Discount Amount" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Settlement discount applicable to the Trading Account expressed as an amount.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="settlementDiscountPercent" sme:label="TradingAccount Settlement Discount Percent" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Settlement discount applicable to the Trading Account expressed as a percent.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="settlementDiscountTerms" sme:label="TradingAccount Settlement Discount Terms" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Terms applicable to the settlement discount, often expressed as a number of days.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="settlementDiscountTermsCommencement" sme:label="TradingAccount Settlement Discount Terms Commencement" minOccurs="0" type="xs:string" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						For some types of settlement discount it is necessary to know when the terms start, e.g. if terms are 30 days from end of month.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="settlementDiscountIncludedInTotal" sme:label="TradingAccount Settlement Discount Included In Total" minOccurs="0" type="xs:boolean" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Flag to indicate if settlement discount is included in document totals (e.g. Invoices, etc).
						 
						Default = False
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="paymentTerms" sme:label="TradingAccount Payment Terms" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Period in which outstanding amounts must be paid (e.g. 30 days, by end of month “EOM”). Also after this period additional (sur)charges may apply.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="paymentTermsCommencement" sme:label="TradingAccount Payment Terms Commencement" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						For some types of payment terms it is necessary to know when the terms start, e.g. if terms are 30 days from end of month.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderDiscountType" sme:label="TradingAccount Order Discount Type" minOccurs="0" type="discountType--enum" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Flag to indicate the type of order discount applicable to the Trading Account (percent or amount)
						 
						Percent
						Amount
						Both

						Default = “Percent”
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderDiscountAmount" sme:label="TradingAccount Order Discount Amount" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Any discount that may be applicable to orders (or quotations) raised against the account, expressed as an amount.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderDiscountPercent" sme:label="TradingAccount Order Discount Percent" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Any discount that may be applicable to orders (or quotations) raised against the account, expressed as a percent.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderText1" sme:label="TradingAccount Order Text1" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Text to be shown by default on orders raised against this Trading Account.
						 
						Header text.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderText2" sme:label="TradingAccount Order Text2" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Text to be shown by default on orders raised against this Trading Account.
						 
						Footer text.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderLineDiscountType" sme:label="TradingAccount Order Line Discount Type" minOccurs="0" type="discountType--enum" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Flag to indicate the type of order Line discount applicable to the Trading Account (percent or amount)
						 
						Percent
						Amount
						Both

						Default = “Percent”
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderLineDiscountAmount" sme:label="TradingAccount Order Line Discount Amount" minOccurs="0" type="xs:decimal" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Any discount that may be applicable to order (or quotation) lines raised against the account, expressed as an amount.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderLineDiscountPercent" sme:label="TradingAccount Order Line Discount Percent" minOccurs="0" type="xs:decimal" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Any discount that may be applicable to order (or quotation) lines raised against the account, expressed as a percent.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderLineText1" sme:label="TradingAccount OrderLine Text1" minOccurs="0" type="xs:string" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Text to be shown by default on order lines raised against this Trading Account.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderLineText2" sme:label="TradingAccount OrderLine Text2" minOccurs="0" type="xs:string" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Text to be shown by default on order lines raised against this Trading Account.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceDiscountType" sme:label="TradingAccount Invoice Discount Type" minOccurs="0" type="discountType--enum" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Flag to indicate the type of discount applicable to invoices raised against the Trading Account (percent or amount)
						 
						Percent
						Amount
						Both

						Default = “Percent”
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceDiscountAmount" sme:label="TradingAccount Invoice Discount Amount" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Any discount that may be applicable to invoices raised against the Trading Account, expressed as an amount.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceDiscountPercent" sme:label="TradingAccount Invoice Discount Percent" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Any discount that may be applicable to invoices raised against the Trading Account, expressed as a percentage.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceText1" sme:label="TradingAccount Invoice Text1" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Text to be shown by default on Invoice raised against this Trading Account.
						 
						Header text
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceText2" sme:label="TradingAccount Invoice Text2" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Text to be shown by default on Invoice raised against this Trading Account.
						 
						Footer text
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="bankAccounts" sme:label="TradingAccount BankAccount " minOccurs="0" type="bankAccount--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Bank account associated with the Trading Account e.g. for electronic Payment, direct debits, bank transfers, etc.
						 
						See “BankAccount” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tenderType" sme:label="TradingAccount Default Tender Type" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Tender Type used by Trading Account (e.g. if this Trading Account is associated with a Customer then this is the tender type used on Receipts/Payments associated with the Customer, e.g. BACS, check, Cash, Commercial Papers, etc).
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="defaultFulfillmentLocation" sme:label="TradingAccount Default Fulfillment Location" minOccurs="0" type="location--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The default Location to be used for fulfillment (e.g. the warehouse to take deliveries of purchase orders or from which to deliver sales orders).
						 
						See “Location” definition.

						Could default from Customer (or Supplier).
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="pricelists" sme:label="TradingAccount PriceList" minOccurs="0" type="priceList--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
