					<xs:documentation>
						Supplier own reference for the Purchase Credit (e.g. SO number).
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="pricelist" sme:label="PurchaseCredit PriceList" minOccurs="0" type="priceList--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
