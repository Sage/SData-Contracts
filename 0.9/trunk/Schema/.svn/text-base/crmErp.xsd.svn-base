					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="supplierReference" sme:label="PurchaseRequisition Supplier reference" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Supplier own reference for the Purchase Requisition (e.g. a SO number).
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="pricelist" sme:label="PurchaseRequisition PriceList" minOccurs="0" type="priceList--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
