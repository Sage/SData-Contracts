				<xs:annotation>
					<xs:documentation>
						Customer associated with the Sales Order.
						 
						See “Customer” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tradingAccount" sme:label="SalesOrder Customer TradingAccount" minOccurs="0" type="tradingAccount--type" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
						Link to the Sales Order Customer account.
						 
						See “TradingAccount” definition.

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customerReference" sme:label="SalesOrder Customer reference" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Customer own reference for the Sales Order (e.g. a PO number).
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="opportunity" sme:label="SalesOrder Opportunity" minOccurs="0" type="opportunity--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Opportunity linked to the Sales Order (if any).
						 
						See “Opportunity” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="pricelist" sme:label="SalesOrder PriceList" minOccurs="0" type="priceList--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to the Price List used when generating the Sales Order
						 
						See “PriceList” definition.

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="supplierTradingAccount" sme:label="SalesOrder Supplier TradingAccount" minOccurs="0" type="tradingAccount--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to the Sales Order Supplier account (e.g. for back to back, contremarque, drop-ship ordering).
						 
						See “TradingAccount” definition.
						These are also stored on SalesOrderLines

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="supplierReference" sme:label="SalesOrder Supplier reference" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Supplier own reference for the Sales Order.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="supplierPurchaseOrders" sme:label="SalesOrder Supplier PurchaseOrder" minOccurs="0" type="purchaseOrder--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to any Supplier Purchase Orders used to fulfill the Sales Order (back to back or drop-ship).
						 
						See “PurchaseOrder” definition.
						These are also stored on SalesOrderLines

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="type" sme:label="SalesOrder Type" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The Sales Order type.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="copyFlag" sme:label="SalesOrder Copy Flag" minOccurs="0" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>
						Indicates if this Sales Order is a copy or not.
						 
						Default is false (no)

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="originatorDocument" sme:label="SalesOrder Originator Document reference" minOccurs="0" type="salesOrder--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
				<xs:annotation>
					<xs:documentation>
						reference to another Sales Order if required (e.g. if order is a copy, then this will show which order was copied, If this is a replacement order this will show the original order).
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="quotation" sme:label="SalesOrder Quotation" minOccurs="0" type="salesQuotation--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
				<xs:annotation>
					<xs:documentation>
						A link to the quotation used to create this Sales Order (if any).
						 
						See “Quotation” definition.
						Must be a valid quotation.

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="postalAddresses" sme:label="SalesOrder PostalAddress" minOccurs="0" type="postalAddress--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to Postal Address(es) associated with the Sales Order.
						 
						See “PostalAddress” definition.
						Could be multiple e.g. shipping, billing, etc
						These are also stored on SalesOrderLines
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deliveryMethod" sme:label="SalesOrder Delivery Method" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Delivery method to be employed for Sales Order (e.g. Courier, Collection ,etc)
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deliveryRule" sme:label="SalesOrder Delivery Rule" minOccurs="0" type="xs:boolean" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Flag to indicate if the order has to be delivered in full or can be split across partial deliveries.
						 
						True = Partial deliveries allowed;
						False = Partial deliveries not allowed.

						Default = True
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deliveryTerms" sme:label="SalesOrder Delivery Terms" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The terms for Delivery.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deliveryDate" sme:label="SalesOrder Delivery Date" minOccurs="0" type="xs:date" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The anticipated date of the Delivery.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dueDate" sme:label="SalesOrder Due Date" minOccurs="0" type="xs:date" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Date the order is due by the Customer.
						 
						Delivery date may be different to the Customer due date.

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierCompany" sme:label="SalesOrder Carrier Company" minOccurs="0" type="company--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to the Company that will be the carrier / freight forwarder/ deliverer of the Sales Order
						 
						See “Company” definition.
						Carriers are not used by all application.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierSupplier" sme:label="SalesOrder Carrier Supplier" minOccurs="0" type="supplier--list" sme:relationship="association" sme:isCollection="true" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to the Supplier associated with the Company that wll be the carrier / freight forwarder / deliverer of the Sales Order.
						 
						See “Supplier” definition.
						Carriers are not used by all application.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierTradingAccount" sme:label="SalesOrder Carrier TradingAccount" minOccurs="0" type="tradingAccount--list" sme:relationship="association" sme:isCollection="true" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to the Trading Account associated with the Supplier that wll be the carrier / freight forwarder / deliverer of the Sales Order.
						 
						See “TradingAccount” definition.
						Carriers are not used by all application.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierNetPrice" sme:label="SalesOrder Carrier Net Price" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The price of Delivery/carriage.
						 
						In Order currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierTaxPrice" sme:label="SalesOrder Carrier Tax Price" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The tax on Delivery/carriage.
						 
						In Order currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierTotalPrice" sme:label="SalesOrder Carrier Gross Price" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The total price of Delivery/carriage (net + tax).
						 
						In Order currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierSalesInvoice" sme:label="SalesOrder Carrier SalesInvoice" minOccurs="0" type="salesInvoice--list" sme:relationship="association" sme:isCollection="true" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						A link to the Sales Invoice generated to recharge the carriage to the Customer.
						 
						See “SalesInvoice” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierPurchaseInvoice" sme:label="SalesOrder Carrier PurchaseInvoice" minOccurs="0" type="purchaseInvoice--list" sme:relationship="association" sme:isCollection="true" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						A link to the purchase Invoice charged by the carrier Company for the Delivery to the Customer.
						 
						See “PurchaseInvoice” defi nition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierTaxCodes" sme:label="SalesOrder Carrier Tax Code" minOccurs="0" type="taxCode--list" sme:relationship="association" sme:isCollection="true" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Tax code governing the tax regime applicable to the carriage on this Sales Order.
						 
						See “TaxCode” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierReference" sme:label="SalesOrder Carrier reference" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						reference number as used by the carrier during Delivery e.g. for Delivery tracking.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="currency" sme:label="SalesOrder Currency" minOccurs="0" type="xs:string">
				<xs:annotation>
					<xs:documentation>
						The currency of the Sales Order (e.g. GBP, USD, EUR).
						 
						ISO 4217.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operatingCompanyCurrency" sme:label="SalesOrder OperatingCompany Currency" minOccurs="0" type="xs:string">
				<xs:annotation>
					<xs:documentation>
						The currency as used by OperatingCompany, the business operating the Sage application (the “base” currency).
						 
						ISO 4217.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operatingCompanyCurrencyExchangeRate" sme:label="SalesOrder OperatingCompany Currency Exchange Rate" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Rate used to convert from Sales Order Currency into Sales Order OperatingCompany Currency, if different.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operatingCompanyCurrencyExchangeRateOperator" sme:label="SalesOrder OperatingCompany Currency Exchange Rate Operator" minOccurs="0" type="RateOperator--enum" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Operator to be used (multiplication or division) to convert from Sales Order Currency to Sales Order OperatingCompany Currency.
						 
						“*” (multiplication) or “/” division.

						E.g. if the operator is division then it is Sales Order Values in Sales Order Currency / Sales Order Operating Company Currency Exchange Rate = Sales Order Values in Sales Order OperatingCompany currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operatingCompanyCurrencyExchangeRateDate" sme:label="SalesOrder OperatingCompany Currency Exchange Rate Date" minOccurs="0" type="xs:date" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Date of the exchange rate used to convert from Sales Order Currency to Sales Order OperatingCompany Currency, if different.
						 
						Default = System date
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceCurrency" sme:label="SalesOrder Invoice Currency" minOccurs="0" type="xs:string" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The currency to be used on invoices associated with this order.
						 
						ISO 4217.
						Could be derived from order currency.

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceCurrencyExchangeRate" sme:label="SalesOrder Invoice Currency Exchange Rate" minOccurs="0" type="xs:decimal" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Rate used to convert from Sales Order Currency into Sales Order Invoice Currency, if different.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceCurrencyExchangeRateOperator" sme:label="SalesOrder Invoice Currency Exchange Rate Operator" minOccurs="0" type="RateOperator--enum" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Operator to be used (multiplication or division) to convert from Sales Order Currency to Sales Order Invoice Currency
						 
						“*” (multiplication) or “/” division.

						E.g. if the operator is division then it is Sales Order Values in Sales Order Currency / Sales Order Invoice Currency Exchange Rate = Sales Order Values in Sales Order Invoice Currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceCurrencyExchangeRateDate" sme:label="SalesOrder Invoice Currency Exchange Rate Date" minOccurs="0" type="xs:date" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Date of the exchange rate used to convert from Sales Order Currency to Sales Order Invoice Currency, if different.
						 
						Default = System date
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customerTradingAccountCurrency" sme:label="SalesOrder Customer TradingAccount Currency" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The currency (e.g. GBP, USD, EUR) used by the Customer Trading Account on the Sales Order.
						 
						ISO 4217.
						Could be derived from order currency.

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customerTradingAccountCurrencyExchangeRate" sme:label="SalesOrder Customer TradingAccount Currency Exchange Rate" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Rate used to convert the Sales Order currency into the Customer Trading Account currency, if different.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customerTradingAccountCurrencyExchangeRateOperator" sme:label="SalesOrder Customer TradingAccount Currency Exchange Rate Operator" minOccurs="0" type="RateOperator--enum" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Operator to be used (multiplication or division) to convert from Sales Order Currency to Sales Order Customer Trading Account Currency
						 
						“*” (multiplication) or “/” division.

						E.g. if the operator is division then it is Sales Order Values in Sales Order Currency / Sales Order Customer TradingAccount Currency Exchange Rate = Sales Order Values in Sales Order Customer TradingAccount Currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customerTradingAccountCurrencyExchangeRateDate" sme:label="SalesOrder Customer TradingAccount Currency Exchange Rate Date" minOccurs="0" type="xs:date" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Date of the exchange rate used to convert from Sales Order Currency to Customer Trading Account Currency, if different.
						 
						Default = System date
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="date" sme:label="SalesOrder Date" minOccurs="0" type="xs:date">
				<xs:annotation>
					<xs:documentation>
						Date the Sales Order was placed.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="time" sme:label="SalesOrder Time" minOccurs="0" type="xs:time" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Time the Sales Order was placed.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="validity" sme:label="SalesOrder Validity" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Period the Sales Order is valid for (days from Order Date).
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="expirationDate" sme:label="SalesOrder Expiration Date" minOccurs="0" type="xs:date" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The expiration date of the Sales Order (SalesOrder Date + Validity).
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxCodes" sme:label="SalesOrder Tax Code" minOccurs="0" type="taxCode--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Tax code governing the tax regime applicable to this Sales Order.
						 
						See “TaxCode” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="buyerContact" sme:label="SalesOrder Buyer Contact" minOccurs="0" type="contact--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to the Contact at Customer who placed the Sales Order.
						 
						See “Contact” definition.

						Default = Primary TradingAccount Contact
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="salesPersons" sme:label="SalesOrder Salesperson" minOccurs="0" type="salesPerson--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Sales person associated with the Sales Order.
						 
						See “SalesPerson” definition.
						Also known as Sales Agent or Manager in some applications.
						There may be multiple and commission may be spread across multiple sales people.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="user" sme:label="SalesOrder User" minOccurs="0" type="xs:string" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to the user who placed the Sales Order.
						 
						The ‘user’ resource will be defined in the future.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="salesOrderLines" sme:label="SalesOrder SalesOrderLine" minOccurs="0" type="salesOrderLine--list" sme:relationship="child" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
						Sales order Line detail (e.g. items on the Sales Order)
						 
						See “SalesOrderLine” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="lineCount" sme:label="SalesOrder Line Count" minOccurs="0" type="xs:decimal">
				<xs:annotation>
					<xs:documentation>
						The number of lines on the Sales Order.
						 
						Should be auto-populated. Used as a cross reference to ensure order integrity.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderDiscountType" sme:label="SalesOrder Order Discount Type" minOccurs="0" type="discountType--enum" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Flag to indicate the type of discount applicable to this Sales Order (percent or amount)
						 
						From the Trading Account.

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderDiscountAmount" sme:label="SalesOrder Order Discount Amount" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Discount applicable to the Sales Order expressed as an amount.
						 
						From the Trading Account.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderDiscountPercent" sme:label="SalesOrder Order Discount Percent" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Discount applicable to the Sales Order expressed as a percent.
						 
						From the Trading Account.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderAdditionalDiscount1Type" sme:label="SalesOrder Order Additional Discount1 Type" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Flag to indicate the type of discount applicable to this Sales Order (percent or amount)
						 
						Percent
						Amount
						Both
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderAdditionalDiscount1Amount" sme:label="SalesOrder Order Additional Discount1 Amount" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Discount applicable to the Sales Order expressed as an amount.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderAdditionalDiscount1Percent" sme:label="SalesOrder Order Additional Discount1 Percent" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Discount applicable to the Sales Order expressed as a percent.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderAdditionalDiscount2" sme:label="SalesOrder Order Additional Discount2 Type" minOccurs="0" type="discountType--enum" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Flag to indicate the type of discount applicable to this Sales Order (percent or amount)
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderAdditionalDiscount2Amount" sme:label="SalesOrder Order Additional Discount2 Amount" minOccurs="0" type="xs:decimal" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Discount applicable to the Sales Order expressed as an amount.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderAdditionalDiscount2Percent" sme:label="SalesOrder Order Additional Discount2 Percent" minOccurs="0" type="xs:decimal" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Discount applicable to the Sales Order expressed as a percent.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="text1" sme:label="SalesOrder Text1" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Text/commentary (e.g. special Delivery instructions) for the Sales Order
						 
						Header text.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="text2" sme:label="SalesOrder Text2" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Further text/commentary for the Sales Order.
						 
						Footer text.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="netTotal" sme:label="SalesOrder Net Total" minOccurs="0" type="xs:decimal">
				<xs:annotation>
					<xs:documentation>
						Total value of the Sales Order, including discounts, excluding tax.
						 
						In order currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="discountTotal" sme:label="SalesOrder Discount Total" minOccurs="0" type="xs:decimal">
				<xs:annotation>
					<xs:documentation>
						Total discounts applied to the order
						 
						In order currency
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="chargesTotal" sme:label="SalesOrder Charges Total" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Total value of any (sur)charges applied to the Sales Order (e.g. finance charges, interest).
						 
						In order currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxTotal" sme:label="SalesOrder Tax Total" minOccurs="0" type="xs:decimal">
				<xs:annotation>
					<xs:documentation>
						Total tax applicable to the Sales Order.
						 
						In order currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="grossTotal" sme:label="SalesOrder Gross Total" minOccurs="0" type="xs:decimal">
				<xs:annotation>
					<xs:documentation>
						Total value of the Sales Order, including tax &amp; discounts &amp; charges.
						 
						In order currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="costTotal" sme:label="SalesOrder Cost Total" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Total cost of the Sales Order.
						 
						In order currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="profitTotal" sme:label="SalesOrder Profit Total" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Total profit on the Sales Order (e.g. net total – cost total).
						 
						In order currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="receipts" sme:label="SalesOrder Receipt" minOccurs="0" type="receipt--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to the Sales Order receipt (e.g. if order is taken with payment or if a deposit has been paid).
						 
						See “Receipt” definition.

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="depositFlag" sme:label="SalesOrder Deposit Flag" minOccurs="0" type="xs:boolean" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Flag to indicate if a deposit has been paid with the order.
						 
						Default = False (no deposit taken).
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="contract" sme:label="SalesOrder Contract" minOccurs="0" type="xs:string" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						reference to a contract associated with the Sales Order. If Contracts aren’t used there may be an estimate instead – this would contain the reference of the estimate.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceTradingAccount" sme:label="SalesOrder Invoice TradingAccount" minOccurs="0" type="tradingAccount--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to the Trading Account to be invoiced for the Sales Order (if different from the Customer) e.g. Bill To
						 
						See “TradingAccount” definition.

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceCountry" sme:label="SalesOrder Invoice Country" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Country to which the Sales Invoice is sent (e.g. for taxation purposes).
						 
						ISO 3166-1 alpha-2 country codes only
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderCountry" sme:label="SalesOrder Order Country" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Country of destination for the Sales Order (e.g. for customs purposes).
						 
						ISO 3166-1 alpha-2 country codes only
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="projects" sme:label="SalesOrder Project" minOccurs="0" type="project--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Identifier of the Project this Sales Order is for.
						 
						See “Project” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="projectLines" sme:label="SalesOrder ProjectLine" minOccurs="0" type="projectLine--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Project Line value for this Sales Order.
						 
						See “Project Line” definition.
						Some applications may not have Project Lines.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="cases" sme:label="SalesOrder Case reference" minOccurs="0" type="case--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The reference of the ‘case’ associated with the Sales Order e.g. if the Order is for repair/fix services.
						 
						A ‘Case’ entity hase been defined to support the F2F process.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="salesOrderDeliveries" sme:label="SalesOrder SalesOrderDelivery" minOccurs="0" type="salesOrderDelivery--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to the Delivery (dispatch) against the Sales Order. If there are multiple then these are held against the Sales Order Line.
						 
						See “SalesOrderDelivery” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fulfillmentLocations" sme:label="SalesOrder Fulfilment Location" minOccurs="0" type="location--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The Location from which the Sales Order will be fulfilled (e.g. the warehouse holding the Commodity).
						 
						See “Location” definition.
						May be held at Line level instead.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="financialAccounts" sme:label="SalesOrder FinancialAccount" minOccurs="0" type="financialAccount--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Financial accounting code applicable to the Sales Order.
						 
						See “FinancialAccount” definition.
						Could come from the Trading Account.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="interactions" sme:label="SalesOrder Interaction" minOccurs="0" type="interaction--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Interaction(s) associated with the Sales Order.
						 
						See “Interaction” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="notes" sme:label="SalesOrder Note" minOccurs="0" type="note--list" sme:relationship="child" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Notes associated with a Sales Order.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
	</xs:complexType>
	<xs:complexType name="salesOrder--list">
		<xs:sequence minOccurs="0" maxOccurs="unbounded">
			<xs:element name="salesOrder" type="salesOrder--type" />
		</xs:sequence>
	</xs:complexType>
	<xs:element name="salesOrderLine" type="salesOrderLine--type" sme:role="resourceKind" sme:pluralName="salesOrderLines" sme:compliance="should" sme:canGet="true" sme:canPut="true" sme:canPost="true" />
	<xs:complexType name="salesOrderLine--type">
		<xs:all>
			<xs:element name="uuid" sme:label="SalesOrderLine Global ID" minOccurs="0" type="uuid--type" sme:isUniqueKey="true">
				<xs:annotation>
					<xs:documentation>
						The Sales Order Line unique identifier.
						 
						Globally unique identifier
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="id" sme:label="SalesOrderLine Application ID" minOccurs="0" type="xs:string" sme:isUniqueKey="true">
