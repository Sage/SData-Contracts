					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="date" sme:label="PurchaseCredit Date" minOccurs="0" type="xs:date">
				<xs:annotation>
					<xs:documentation>
						Date the Purchase Credit was raised.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="time" sme:label="PurchaseCredit Time" minOccurs="0" type="xs:time" nillable="true">
