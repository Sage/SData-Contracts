					<xs:documentation>
						Supplier associated with the PurchaseRequisition
						 
						See “Supplier” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tradingAccount" sme:label="PurchaseRequisition Supplier TradingAccount" minOccurs="0" type="tradingAccount--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
