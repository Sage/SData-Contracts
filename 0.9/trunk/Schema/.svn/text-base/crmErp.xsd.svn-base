<?xml version="1.0" encoding="utf-8"?>
<?xml-stylesheet type="text/xsl" href="http://gforge.sageinternal.com/gf/project/gcrmcontract/scmsvn/?action=browse&path=%2F*checkout*%2Ftrunk%2FTools%2FXsdToHtml.xslt"?>
<xs:schema id="crmErp" elementFormDefault="qualified" targetNamespace="http://schemas.sage.com/crmErp/2008" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:sme="http://schemas.sage.com/sdata/sme/2007" xmlns="http://schemas.sage.com/crmErp/2008" xmlns:hrmErp="http://schemas.sage.com/hrmErp/2008">
	<xs:import namespace="http://schemas.sage.com/hrmErp/2008" />
	<xs:element name="company" type="company--type" sme:role="resourceKind" sme:pluralName="companies" sme:compliance="must" sme:canGet="true" sme:canPut="true" sme:canPost="true" sme:canDelete="true" />
	<xs:complexType name="company--type">
		<xs:all>
			<xs:element name="id" sme:label="Company Application ID" minOccurs="0" type="xs:string" sme:isUniqueKey="true">
				<xs:annotation>
					<xs:documentation>
						The application specific identifier.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="active" sme:label="Company Active Flag" minOccurs="0" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>
						Flag to indicate if the Company is an ‘active’ Company .
						 
						Default = True
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deleted" sme:label="Company Deleted" minOccurs="0" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>
						Flag to indicate if Company is (soft) deleted.
						 
						Default = False
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="label" sme:label="Company Label" minOccurs="0" type="xs:string">
				<xs:annotation>
					<xs:documentation>
						The label for the resource to be presented in the user interface. To support application-specific or language-specific descriptions of resources.
						 
						“Company”
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customer" sme:label="Company Customer" minOccurs="0" type="customer--type" sme:relationship="child" sme:isCollection="false" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to the Customer (if any) associated with this Company.
						 
						See “Customer” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="useCustomerData" sme:label="Company Use Customer Data" minOccurs="0" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>
						Flag to indicate that Company and Customer are the same – properties that are the same on both resources should come from the Customer if this is set to True.
						 
						Default = “False”

						If “Use Customer Data” is set to True then “Use Supplier Data” MUST be False.

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="supplier" sme:label="Company Supplier" minOccurs="0" type="supplier--type" sme:relationship="child" sme:isCollection="false" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to the Supplier (if any) associated with this Company.
						 
						See “Supplier” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="useSupplierData" sme:label="Company Use Supplier Data" minOccurs="0" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>
						Flag to indicate that Company and Supplier are the same– properties that are the same on both resources should come from the Supplier if this is set to True.
						 
						Default = “False”

						“Use Supplier Data” can only be set to True when “Use Customer Data” is False.

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference" sme:label="Company reference" minOccurs="0" type="xs:string" sme:isUniqueKey="true">
				<xs:annotation>
					<xs:documentation>
						reference for the Company.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference2" sme:label="Company Reference2" minOccurs="0" type="xs:string" sme:isUniqueKey="true" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Additional reference for the Company.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operatingCompany" sme:label="Company OperatingCompany" minOccurs="0" type="company--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to the OperatingCompany associated with the Company.
						 
						See “Company” definition (where Company Type = “Operating Company”).
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="status" sme:label="Company Status" minOccurs="0" type="xs:string" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Status of the Company.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="name" sme:label="Company Name" minOccurs="0" type="xs:string" sme:mandatory="true">
				<xs:annotation>
					<xs:documentation>
						The entity name (e.g. “Homestyle Kitchens”).
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="shortName" sme:label="Company Short Name" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						A shorter name for the Company.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="type" sme:label="Company Type" minOccurs="0" type="companyType--enum" sme:mandatory="true">
				<xs:annotation>
					<xs:documentation>
						The Company type.
						 

						Default = “Unknown”

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="currency" sme:label="Company Currency" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The Company currency (e.g. GBP, USD, EUR).
						 
						ISO 4217.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="postalAddresses" sme:label="Company PostalAddress" minOccurs="0" type="postalAddress--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to the Company address(es).
						 
						See “PostalAddress” definition.

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="phones" sme:label="Company Phone" minOccurs="0" type="phoneNumber--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Company telephone number(s).
						 
						See “PhoneNumber” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="emails" sme:label="Company Email" minOccurs="0" type="email--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Company email address(es).
						 
						See “Email” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="website" sme:label="Company Web site" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Company web site address.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="contacts" sme:label="Company Contact" minOccurs="0" type="contact--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to the Contact(s) associated with the Company
						 
						See “Contact” definition.

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="identifier" sme:label="Company Identifier" minOccurs="0" type="xs:string" sme:isUniqueKey="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Identifier(s) of the Company as used by the third parties (e.g. Company Registration Number, DUNS number, 1099 type, CIS number, siret number, Trading Number, etc).
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxLocale" sme:label="Company Tax Locale" minOccurs="0" type="xs:string">
				<xs:annotation>
					<xs:documentation>
						The tax-locale applicable to the Operating Company (e.g. District)
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxReference" sme:label="Company Tax reference" minOccurs="0" type="xs:string" sme:isUniqueKey="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Tax reference associated with the Company (e.g. VAT number, numero intracommnunautaire, FEIN)
						 
						NIF (Sp)
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="bankAccounts" sme:label="Company BankAccount " minOccurs="0" type="bankAccount--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Bank account associated with the Company e.g. for electronic Payment, direct debits, bank transfers, etc.
						 
						See “BankAccount” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="industry" sme:label="Company Industry" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The industry code of the Company.
						 
						SIC code (UK) or NAF (Fr).
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="language" sme:label="Company Language" minOccurs="0" type="xs:string" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Language used by the Company (e.g. EN-GB)
						 
						ISO 639-1
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="associatedChildCompanies" sme:label="Associated child Company" minOccurs="0" type="company--list" sme:relationship="child" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to child Company associated with this Company.
						 
						Companies can be ‘nested’ to create organizational hierarchies.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="associatedParentCompany" sme:label="Company Associated parent Company" minOccurs="0" type="company--type" sme:relationship="parent" sme:isCollection="false" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to child Company associated with this Company.
						 
						Companies can be ‘nested’ to create organizational hierarchies.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="interactions" sme:label="Company Interaction" minOccurs="0" type="interaction--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Interaction(s) associated with the Company.
						 
						See “Interaction” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="opportunities" sme:label="Company Opportunity" minOccurs="0" type="opportunity--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Opportunities associated with the Company.
						 
						See “Opportunity” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="salesQuotations" sme:label="Company Sales Quotation" minOccurs="0" type="salesQuotation--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Sales Quotation(s) associated with the Company.
						 
						See “SalesQuotation” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="salesOrders" sme:label="Company Sales Order" minOccurs="0" type="salesOrder--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Sales Order(s) associated with the Company.
						 
						See “SalesOrder” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="salesOrderDeliveries" sme:label="Company Sales Order Delivery" minOccurs="0" type="salesOrderDelivery--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Sales Order Delivery(s) associated with the Company.
						 
						See “SalesOrderDelivery” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="salesInvoices" sme:label="Company Sales Invoice" minOccurs="0" type="salesInvoice--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Sales Invoice(s) associated with the Company.
						 
						See “SalesInvoice” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="salesReturns" sme:label="Company Sales Return" minOccurs="0" type="salesReturn--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Sales Return(s) associated with the Company.
						 
						See “SalesReturn” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="salesReturnDeliveries" sme:label="Company Sales Return Delivery" minOccurs="0" type="salesReturnDelivery--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Sales Return Delivery(s) associated with the Company.
						 
						See “SalesReturnDelivery” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="salesCredits" sme:label="Company Sales Credit" minOccurs="0" type="salesCredit--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Sales Credit(s) associated with the Company.
						 
						See “SalesCredit” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="purchaseRequistions" sme:label="Company Purchase Requisition" minOccurs="0" type="purchaseRequisition--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Purchase Requisition(s) associated with the Company.
						 
						See “PurchaseRequisition” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="purchaseOrders" sme:label="Company Purchase Order" minOccurs="0" type="purchaseOrder--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Purchase Order(s) associated with the Company.
						 
						See “PurchaseOrder” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="purchaseOrderDeliveries" sme:label="Company Purchase Order Delivery" minOccurs="0" type="purchaseOrderDelivery--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Purchase Order Delivery(s) associated with the Company.
						 
						See “PurchaseOrderDelivery” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="purchaseInvoices" sme:label="Company Purchase Invoice" minOccurs="0" type="purchaseInvoice--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Purchase Invoice(s) associated with the Company.
						 
						See “PurchaseInvoice” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="purchaseReturns" sme:label="Company Purchase Return" minOccurs="0" type="purchaseReturn--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Purchase Return(s) associated with the Company.
						 
						See “PurchaseReturn” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="purchaseReturnDeliveries" sme:label="Company Purchase Return Delivery" minOccurs="0" type="purchaseReturnDelivery--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Purchase Return Delivery(s) associated with the Company.
						 
						See “PurchaseReturnDelivery” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="purchaseCredits" sme:label="Company Purchase Credit" minOccurs="0" type="purchaseCredit--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Purchase Credit(s) associated with the Company.
						 
						See “PurchaseCredit” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="receipts" sme:label="Company Receipt" minOccurs="0" type="receipt--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Receipt(s) associated with the Company.
						 
						See “Receipt” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="payments" sme:label="Company Payment" minOccurs="0" type="payment--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Payment(s) associated with the Company.
						 
						See “Payment” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="projects" sme:label="Company Project" minOccurs="0" type="project--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Project(s) associated with the Company.
						 
						See “Project” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="cases" sme:label="Company Case" minOccurs="0" type="case--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Case(s) associated with the Company.
						 
						See “Case” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="notes" sme:label="Company Note" minOccurs="0" type="note--list" sme:relationship="child" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Notes associated with a Company.
						 
						See “Note” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
	</xs:complexType>
	<xs:complexType name="company--list">
		<xs:sequence minOccurs="0" maxOccurs="unbounded">
			<xs:element name="company" type="company--type" />
		</xs:sequence>
	</xs:complexType>
	<xs:element name="customer" type="customer--type" sme:role="resourceKind" sme:pluralName="customers" sme:compliance="must" sme:canGet="true" sme:canPut="true" sme:canPost="true" sme:canDelete="true" />
	<xs:complexType name="customer--type">
		<xs:all>
			<xs:element name="id" sme:label="Customer Application ID" minOccurs="0" type="xs:string" sme:isUniqueKey="true">
				<xs:annotation>
					<xs:documentation>
						The application specific identifier.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="active" sme:label="Customer Active Flag" minOccurs="0" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>
						Flag to indicate if the Customer is an ‘active’ Customer .
						 
						Default = True
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deleted" sme:label="Customer Deleted" minOccurs="0" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>
						Flag to indicate if the Customer is (soft) deleted.
						 
						Default = False
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="label" sme:label="Customer Label" minOccurs="0" type="xs:string">
				<xs:annotation>
					<xs:documentation>
						The label for the resource to be presented in the user interface. To support application-specific or language-specific descriptions of resources.
						 
						“Customer”
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="company" sme:label="Customer Company" minOccurs="0" type="company--type" sme:relationship="parent" sme:isCollection="false" sme:mandatory="true">
				<xs:annotation>
					<xs:documentation>
						Link to the Company associated with the Customer.
						 
						See “Company” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tradingAccounts" sme:label="Customer TradingAccount" minOccurs="0" type="tradingAccount--list" sme:relationship="child" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to the trading account associated with this Customer.
						 
						See “TradingAccount” definition.
						A Customer can have multiple trading accounts to support e.g. different currencies.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="useTradingAccountData" sme:label="Customer Use Trading Account Data" minOccurs="0" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>
						Flag to indicate that Customer and Trading Account are the same – properties that are the same on both resources should come from the Trading Account if this is set to True.
						 
						Default = “False”

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference" sme:label="Customer reference" minOccurs="0" type="xs:string" sme:isUniqueKey="true">
				<xs:annotation>
					<xs:documentation>
						reference for the Customer (e.g. “ABB001”).
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference2" sme:label="Customer Reference2" minOccurs="0" type="xs:string" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Additional reference for the Customer.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operatingCompany" sme:label="Customer OperatingCompany" minOccurs="0" type="company--type" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
						Link to the OperatingCompany associated with the Customer.
						 
						See “Company” definition (where Company Type = “Operating Company”).
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="status" sme:label="Customer Status" minOccurs="0" type="xs:string" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Status of the Customer.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ownReference" sme:label="Customer Own reference" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The reference for the Customer as defined by the Customer themselves (e.g. an open PO number).
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="name" sme:label="Customer Name" minOccurs="0" type="xs:string" sme:mandatory="true">
				<xs:annotation>
					<xs:documentation>
						The Customer name. This may be different to the Company name (e.g. to handle T/A – Trading As – scenario).
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="shortName" sme:label="Customer Short Name" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						A shorter name for the Customer.
						 

					</xs:documentation>
