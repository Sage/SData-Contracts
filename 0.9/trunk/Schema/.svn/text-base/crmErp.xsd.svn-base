				<xs:annotation>
					<xs:documentation>
						Additional reference for the Sales Return.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operatingCompany" sme:label="SalesReturn OperatingCompany" minOccurs="0" type="company--type" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
						Link to the OperatingCompany associated with the Sales Return.
						 
						See “Company” definition (where Company Type = “Operating Company”).
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="status" sme:label="SalesReturn Status" minOccurs="0" type="salesReturnStatus--enum" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Status of the Sales Return (e.g. Live, Completed).
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="statusFlag" sme:label="SalesReturn Status Flag" minOccurs="0" type="xs:boolean" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Additional Boolean status flag (e.g. Return on hold (or “blocked”), off hold).
						 
						True is on hold, False is not on hold.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="statusFlagText" sme:label="SalesReturn Status Flag Text" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Sales return Status Flag text.
						 
						Default = “On Hold”
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="company" sme:label="SalesReturn Company" minOccurs="0" type="company--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Company associated with the Sales Return
						 
						See “Company” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customer" sme:label="SalesReturn Customer" minOccurs="0" type="customer--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Customer associated with the Sales Return
						 
						See “Customer” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tradingAccount" sme:label="SalesReturn Customer TradingAccount" minOccurs="0" type="tradingAccount--type" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
						Link to the Sales Return Customer account.
						 
						See “TradingAccount” definition.

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customerReference" sme:label="SalesReturn Customer reference" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Customer own reference for the Sales Return (e.g. a PO number).
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reason" sme:label="SalesReturn Reason" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The reason for the Sales Return.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="pricelist" sme:label="SalesReturn PriceList" minOccurs="0" type="priceList--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to the Price List used when generating the Sales Return
						 
						See “PriceList” definition.

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="supplierTradingAccount" sme:label="SalesReturn Supplier TradingAccount" minOccurs="0" type="tradingAccount--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to the Sales Return Supplier account (e.g. for direct returns to Supplier).
						 
						See “TradingAccount” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="supplierReference" sme:label="SalesReturn Supplier reference" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Supplier own reference for the Sales Return.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="type" sme:label="SalesReturn Type" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The Sales Return type.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="copyFlag" sme:label="SalesReturn Copy Flag" minOccurs="0" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>
						Indicates if this Sales Return is a copy or not.
						 
						Default is false (no)
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="originatorDocument" sme:label="SalesReturn Originator Document reference" minOccurs="0" type="salesOrder--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
				<xs:annotation>
					<xs:documentation>
						reference to another Sales Return if required (e.g. if Return is a copy, then this will show which Return was copied, if return is for a Sales Order, this will show which Sales Order was returned).
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="postalAddresses" sme:label="SalesReturn PostalAddress" minOccurs="0" type="postalAddress--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to Postal Address(es) associated with the Sales Return.
						 
						See “PostalAddress” definition.
						Could be multiple e.g. shipping, billing, etc

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deliveryMethod" sme:label="SalesReturn Delivery Method" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Delivery method to be employed for Sales Return (e.g. Courier, Collection ,etc)
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deliveryTerms" sme:label="SalesReturn Delivery Terms" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The terms for Delivery.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deliveryDate" sme:label="SalesReturn Delivery Date" minOccurs="0" type="xs:date" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The anticipated date of the Delivery.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierCompany" sme:label="SalesReturn Carrier Company" minOccurs="0" type="company--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to the Company that will be the carrier / freight forwarder/ deliverer of the Sales Return
						 
						See “Company” definition.
						Carriers are not used by all applications.

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierSupplier" sme:label="SalesReturn Carrier Supplier" minOccurs="0" type="supplier--list" sme:relationship="association" sme:isCollection="true" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to the Supplier associated with the Company that wll be the carrier / freight forwarder / deliverer of the Sales Return.
						 
						See “Supplier” definition.
						Carriers are not used by all application.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierTradingAccount" sme:label="SalesReturn Carrier TradingAccount" minOccurs="0" type="tradingAccount--list" sme:relationship="association" sme:isCollection="true" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to the Trading Account associated with the Supplier that wll be the carrier / freight forwarder / deliverer of the Sales Return.
						 
						See “TradingAccount” definition.
						Carriers are not used by all application.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierNetPrice" sme:label="SalesReturn Carrier Net Price" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The price of Delivery/carriage.
						 
						In Return currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierTaxPrice" sme:label="SalesReturn Carrier Tax Price" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The tax on Delivery/carriage.
						 
						In Return currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierTotalPrice" sme:label="SalesReturn Carrier Gross Price" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The total price of Delivery/carriage (net + VAT).
						 
						In Return currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierSalesInvoice" sme:label="SalesReturn Carrier SalesInvoice" minOccurs="0" type="salesInvoice--list" sme:relationship="association" sme:isCollection="true" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						A link to the Sales Invoice generated to recharge the carriage to the Customer.
						 
						See “SalesInvoice” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierPurchaseInvoice" sme:label="SalesReturn Carrier PurchaseInvoice" minOccurs="0" type="purchaseInvoice--list" sme:relationship="association" sme:isCollection="true" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						A link to the purchase Invoice charged by the carrier Company for the Delivery back from the Customer.
						 
						See “PurchaseInvoice” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierTaxCodes" sme:label="SalesReturn Carrier Tax Code" minOccurs="0" type="taxCode--list" sme:relationship="association" sme:isCollection="true" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Tax code governing the tax regime applicable to the carriage on this Sales Return.
						 
						See “TaxCode” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierReference" sme:label="SalesReturn Carrier reference" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						reference number as used by the carrier during Delivery e.g. for Delivery tracking.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="currency" sme:label="SalesReturn Currency" minOccurs="0" type="xs:string">
				<xs:annotation>
					<xs:documentation>
						The currency of the Sales Return (e.g. GBP, USD, EUR).
						 
						ISO 4217.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operatingCompanyCurrency" sme:label="SalesReturn OperatingCompany Currency" minOccurs="0" type="xs:string">
				<xs:annotation>
					<xs:documentation>
						The currency as used by OperatingCompany, the business operating the Sage application (the “base” currency).
						 
						ISO 4217.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operatingCompanyCurrencyExchangeRate" sme:label="SalesReturn OperatingCompany Currency Exchange Rate" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Rate used to convert from Sales Return Currency into Sales Return OperatingCompany Currency, if different.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operatingCompanyCurrencyExchangeRateOperator" sme:label="SalesReturn OperatingCompany Currency Exchange Rate Operator" minOccurs="0" type="RateOperator--enum" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Operator to be used (multiplication or division) to convert from Sales Return Currency to Sales Return OperatingCompany Currency.
						 
						“*” (multiplication) or “/” division.

						E.g. if the operator is division then it is Sales Return Values in Sales Return Currency / Sales Return Operating Company Currency Exchange Rate = Sales Return Values in Sales Return OperatingCompany currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operatingCompanyCurrencyExchangeRateDate" sme:label="SalesReturn OperatingCompany Currency Exchange Rate Date" minOccurs="0" type="xs:date" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Date of the exchange rate used to convert from Sales Return Currency to Sales Return OperatingCompany Currency, if different.
						 
						Default = System date
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="creditCurrency" sme:label="SalesReturn Credit Currency" minOccurs="0" type="xs:string" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The currency to be used on credit notes associated with this Return.
						 
						ISO 4217.
						Could be derived from Return currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="creditCurrencyExchangeRate" sme:label="SalesReturn Credit Currency Exchange Rate" minOccurs="0" type="xs:decimal" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Rate used to convert from Sales Return Currency into Sales Return Credit Currency, if different.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="creditCurrencyExchangeRateOperator" sme:label="SalesReturn Credit Currency Exchange Rate Operator" minOccurs="0" type="RateOperator--enum" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Operator to be used (multiplication or division) to convert from Sales Return Currency to Sales Return Credit Currency
						 
						“*” (multiplication) or “/” division.


						E.g. if the operator is division then it is Sales Return Values in Sales Return Currency / Sales Return Credit Currency Exchange Rate = Sales Return Values in Sales Return Credit Currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="creditCurrencyExchangeRateDate" sme:label="SalesReturn Credit Currency Exchange Rate Date" minOccurs="0" type="xs:date" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Date of the exchange rate used to convert from Sales Return Currency to Sales Return Credit Currency, if different.
						 
						Default = System date
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customerTradingAccountCurrency" sme:label="SalesReturn Customer TradingAccount Currency" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The currency (e.g. GBP, USD, EUR) used by the Customer Trading Account on the Sales Return.
						 
						ISO 4217.
						Could be derived from order currency.

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customerTradingAccountCurrencyExchangeRate" sme:label="SalesReturn Customer TradingAccount Currency Exchange Rate" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Rate used to convert the Sales Return currency into the Customer Trading Account currency, if different.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customerTradingAccountCurrencyExchangeRateOperator" sme:label="SalesReturn Customer TradingAccount Currency Exchange Rate Operator" minOccurs="0" type="RateOperator--enum" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Operator to be used (multiplication or division) to convert from Sales Return Currency to Sales Return Customer Trading Account Currency
						 
						“*” (multiplication) or “/” division.

						E.g. if the operator is division then it is Sales Return Values in Sales Return Currency / Sales Return Customer TradingAccount Currency Exchange Rate = Sales Return Values in Sales Return Customer TradingAccount Currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customerTradingAccountCurrencyExchangeRateDate" sme:label="SalesReturn Customer TradingAccount Currency Exchange Rate Date" minOccurs="0" type="xs:date" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Date of the exchange rate used to convert from Sales Return Currency to Customer Trading Account Currency, if different.
						 
						Default = System date
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="date" sme:label="SalesReturn Date" minOccurs="0" type="xs:date">
				<xs:annotation>
					<xs:documentation>
						Date the Sales Return was placed.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="time" sme:label="SalesReturn Time" minOccurs="0" type="xs:time" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Time the Sales Return was placed.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="validity" sme:label="SalesReturn Validity" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Period the Sales Return is valid for (days from Return Date).
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="expirationDate" sme:label="SalesReturn Expiration Date" minOccurs="0" type="xs:date" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The expiration date of the Sales Return (SalesReturn Date + Validity).
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxCodes" sme:label="SalesReturn Tax Code" minOccurs="0" type="taxCode--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Tax code governing the tax regime applicable to this Sales Return.
						 
						See “TaxCode” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="buyerContact" sme:label="SalesReturn Buyer Contact" minOccurs="0" type="contact--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to the Contact at Customer who placed the Sales Return.
						 
						See “Contact” definition.

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="salespersons" sme:label="SalesReturn Salesperson" minOccurs="0" type="salesPerson--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Sales person associated with the Sales Return.
						 
						See “SalesPerson” definition.
						Also known as Sales Agent or Manager in some applications.
						There may be multiple and commission may be spread across multiple sales people.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="salesReturnLines" sme:label="SalesReturn SalesReturnLine" minOccurs="0" type="salesReturnLine--list" sme:relationship="child" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
						Sales Return Line detail (e.g. items on the Sales Return)
						 
						See “SalesReturnLine” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="lineCount" sme:label="SalesReturn Line Count" minOccurs="0" type="xs:decimal">
				<xs:annotation>
					<xs:documentation>
						The number of lines on the Sales Return.
						 
						Should be auto-populated. Used as a cross reference to ensure Return integrity.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="returnDiscountType" sme:label="SalesReturn Return Discount Type" minOccurs="0" type="discountType--enum" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Flag to indicate the type of discount applicable to this Sales Return (percent or amount)
						 
						From the Trading Account.

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="returnDiscountAmount" sme:label="SalesReturn Return Discount Amount" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Discount applicable to the Sales Return expressed as an amount.
						 
						From the Trading Account.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="returnDiscountPercent" sme:label="SalesReturn Return Discount Percent" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Discount applicable to the Sales Return expressed as a percent.
						 
						From the Trading Account.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="returnAdditionalDiscount1Type" sme:label="SalesReturn Return Additional Discount1 Type" minOccurs="0" type="discountType--enum" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Flag to indicate the type of discount applicable to this Sales Return (percent or amount)
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="returnAdditionalDiscount1Amount" sme:label="SalesReturn Return Additional Discount1 Amount" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Discount applicable to the Sales Return expressed as an amount.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="returnAdditionalDiscount1Percent" sme:label="SalesReturn Return Additional Discount1 Percent" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Discount applicable to the Sales Return expressed as a percent.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="returnAdditionalDiscount2" sme:label="SalesReturn Return Additional Discount2 Type" minOccurs="0" type="discountType--enum" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Flag to indicate the type of discount applicable to this Sales Return (percent or amount)
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="returnAdditionalDiscount2Amount" sme:label="SalesReturn Return Additional Discount2 Amount" minOccurs="0" type="xs:decimal" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Discount applicable to the Sales Return expressed as an amount.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="returnAdditionalDiscount2Percent" sme:label="SalesReturn Return Additional Discount2 Percent" minOccurs="0" type="xs:decimal" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Discount applicable to the Sales Return expressed as a percent.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="text1" sme:label="SalesReturn Text1" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Text/commentary (e.g. special Delivery instructions) for the Sales Return
						 
						Header text.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="text2" sme:label="SalesReturn Text2" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Further text/commentary for the Sales Return.
						 
						Footer text.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="netTotal" sme:label="SalesReturn Net Total" minOccurs="0" type="xs:decimal">
				<xs:annotation>
					<xs:documentation>
						Total value of the Sales Return, including discounts, excluding tax.
						 
						In Return currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="discountTotal" sme:label="SalesReturn Discount Total" minOccurs="0" type="xs:decimal">
				<xs:annotation>
					<xs:documentation>
						Total discounts applied to the Return
						 
						In Return currency
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="chargesTotal" sme:label="SalesReturn Charges Total" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Total value of any (sur)charges applied to the Sales Return (e.g. finance charges, interest).
						 
						In Return currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxTotal" sme:label="SalesReturn Tax Total" minOccurs="0" type="xs:decimal">
				<xs:annotation>
