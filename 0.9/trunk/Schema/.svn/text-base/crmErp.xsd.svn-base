			<xs:element name="id" sme:label="PurchaseOrder Application ID" minOccurs="0" type="xs:string" sme:isUniqueKey="true">
				<xs:annotation>
					<xs:documentation>
						The application specific identifier.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
