			<xs:element name="carrierTaxCodes" sme:label="SalesInvoice Carrier Tax Code" minOccurs="0" type="taxCode--list" sme:relationship="association" sme:isCollection="true" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Tax code governing the tax regime applicable to the carriage on this Sales Invoice.
						 
						See “TaxCode” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierReference" sme:label="SalesInvoice Carrier reference" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						reference number as used by the carrier during Delivery e.g. for Delivery tracking.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="currency" sme:label="SalesInvoice Currency" minOccurs="0" type="xs:string">
				<xs:annotation>
					<xs:documentation>
						The currency (e.g. GBP, USD, EUR) used on the Sales Invoice.
						 
						ISO 4217.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operatingCompanyCurrency" sme:label="SalesInvoice OperatingCompany Currency" minOccurs="0" type="xs:string">
				<xs:annotation>
					<xs:documentation>
						The currency as used by OperatingCompany, the business operating the Sage application (the “base” currency).
						 
						ISO 4217.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operatingCompanyCurrencyExchangeRate" sme:label="SalesInvoice OperatingCompany Currency Exchange Rate" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Rate used to convert from Sales Invoice Currency into Sales Invoice OperatingCompany Currency, if different.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operatingCompanyCurrencyExchangeRateOperator" sme:label="SalesInvoice OperatingCompany Currency Exchange Rate Operator" minOccurs="0" type="RateOperator--enum" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Operator to be used (multiplication or division) to convert from Sales Invoice Currency to Sales Invoice OperatingCompany Currency.
						 
						“*” (multiplication) or “/” division.

						E.g. if the operator is division then it is Sales Invoice Values in Sales Invoice Currency / Sales Invoice Operating Company Currency Exchange Rate = Sales Invoice Values in Sales Invoice OperatingCompany currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operatingCompanyCurrencyExchangeRateDate" sme:label="SalesInvoice OperatingCompany Currency Exchange Rate Date" minOccurs="0" type="xs:date" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Date of the exchange rate used to convert from Sales Invoice Currency to Sales Invoice OperatingCompany Currency, if different.
						 
						Default = System date
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customerTradingAccountCurrency" sme:label="SalesInvoice Customer TradingAccount Currency" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The currency (e.g. GBP, USD, EUR) used by the Trading Account on the Sales Invoice.
						 
						ISO 4217.
						Could be same as Invoice currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customerTradingAccountCurrencyExchangeRate" sme:label="SalesInvoice Customer TradingAccount Currency Exchange Rate" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Rate used to convert the Sales Invoice currency into the Trading Account currency, if different.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customerTradingAccountCurrencyExchangeRateOperator" sme:label="SalesInvoice Customer TradingAccount Currency Exchange Rate Operator" minOccurs="0" type="RateOperator--enum" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Operator to be used (multiplication or division) to convert from Sales Invoice Currency to Sales Invoice Customer Trading Account Currency
						 
						“*” (multiplication) or “/” division.

						E.g. if the operator is division then it is Sales Invoice Values in Sales Invoice Currency / Sales Invoice Customer TradingAccount Currency Exchange Rate = Sales Invoice Values in Sales Invoice Customer TradingAccount Currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customerTradingAccountCurrencyExchangeRateDate" sme:label="SalesInvoice Customer TradingAccount Currency Exchange Rate Date" minOccurs="0" type="xs:date" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Date of the exchange rate used to convert from Sales Invoice Currency to Trading Account Currency, if different.
						 
						Default = System date
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="buyerContact" sme:label="SalesInvoice Buyer Contact" minOccurs="0" type="contact--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to the Contact at Customer who the Sales Invoice is for.
						 
						See “Contact” definition.

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="salespersons" sme:label="SalesInvoice Salesperson" minOccurs="0" type="salesPerson--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Sales person associated with the Sales Invoice.
						 
						See “SalesPerson” definition.
						Also known as Sales Agent or Manager in some applications.
						There may be multiple and commission may be spread across multiple sales people.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="salesInvoiceLines" sme:label="SalesInvoice SalesInvoiceLine" minOccurs="0" type="salesInvoiceLine--list" sme:relationship="child" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
						Sales Invoice Line detail.
						 
						See “SalesInvoiceLine” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="lineCount" sme:label="SalesInvoice Line Count" minOccurs="0" type="xs:decimal">
				<xs:annotation>
					<xs:documentation>
						The number of lines on the Sales Invoice.
						 
						Should be auto-populated. Used as a cross reference to ensure Invoice integrity.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="originatorDocument" sme:label="SalesInvoice Originator Document reference" minOccurs="0" type="salesOrder--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
				<xs:annotation>
					<xs:documentation>
						reference to another document (e.g. the sales order associated with the Invoice).
						 
						Could also be a link to the original Sales Invoice if an Invoice is cancelled (reversed).
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="salesOrderDeliveries" sme:label="SalesInvoice SalesOrderDelivery" minOccurs="0" type="salesOrderDelivery--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						A link to the Sales Order Delivery associated with this Invoice.
						 
						See “SalesOrderDelivery” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="receipts" sme:label="SalesInvoice Receipt" minOccurs="0" type="receipt--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						A link to the (cash) receipt(s) associated with this Sales Invoice
						 
						See “Receipt” definition.

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="contract" sme:label="SalesInvoice Contract" minOccurs="0" type="xs:string" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						reference to a contract associated with the Sales Invoice. If Contracts aren’t used there may be an estimate instead – this would contain the reference of the estimate.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="settlementDiscountType" sme:label="SalesInvoice Settlement Discount Type" minOccurs="0" type="discountType--enum" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Flag to indicate the type of settlement discount applicable to this Sales Invoice (percent or amount)
						 
						From the Trading Account.

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="settlementDiscountAmount" sme:label="SalesInvoice Settlement Discount Amount" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Discount applicable to the Sales Invoice if the Sales Invoice is paid within a certain timeframe, expressed as an amount.
						 
						From the Trading Account.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="settlementDiscountPercent" sme:label="SalesInvoice Settlement Discount Percent" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Discount applicable to the Sales Invoice if the Sales Invoice is paid within a certain timeframe, expressed as a percentage.
						 
						From the Trading Account.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="settlementDiscountTerms" sme:label="SalesInvoice Settlement Discount Terms" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Terms applicable to the settlement discount, often expressed as a number of days.
						 
						From the Trading Account.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="settlementDiscountIncludedInTotal" sme:label="SalesInvoice Settlement Discount Included In Total" minOccurs="0" type="xs:boolean" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Flag to indicate if settlement discount is included in Invoice Totals
						 
						Default = False
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceDiscountType" sme:label="SalesInvoice Invoice Discount Type" minOccurs="0" type="discountType--enum" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Flag to indicate the type of discount applicable to the Sales Invoice (percent or amount)
						 
						From the Trading Account.

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceDiscountAmount" sme:label="SalesInvoice Invoice Discount Amount" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Any discount that may be applicable to the Sales Invoice, expressed as an amount.
						 
						From the Trading Account.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceDiscountPercent" sme:label="SalesInvoice Invoice Discount Percent" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Any discount that may be applicable to the Sales Invoice, expressed as a percentage.
						 
						From the Trading Account.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceAdditionalDiscount1Type" sme:label="SalesInvoice Invoice Additional Discount1 Type" minOccurs="0" type="discountType--enum" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Flag to indicate the type of discount applicable to this Sales Invoice (percent or amount)
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceAdditionalDiscount1Amount" sme:label="SalesInvoice Invoice Additional Discount1 Amount" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Discount applicable to the Sales Invoice expressed as an amount.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceAdditionalDiscount1Percent" sme:label="SalesInvoice Invoice Additional Discount1 Percent" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Discount applicable to the Sales Invoice expressed as a percent.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceAdditionalDiscount2" sme:label="SalesInvoice Invoice Additional Discount2 Type" minOccurs="0" type="discountType--enum" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Flag to indicate the type of discount applicable to this Sales Invoice (percent or amount)
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceAdditionalDiscount2Amount" sme:label="SalesInvoice Invoice Additional Discount2 Amount" minOccurs="0" type="xs:decimal" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Discount applicable to the Sales Invoice expressed as an amount.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceAdditionalDiscount2Percent" sme:label="SalesInvoice Invoice Additional Discount2 Percent" minOccurs="0" type="xs:decimal" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Discount applicable to the Sales Invoice expressed as a percent.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="netTotal" sme:label="SalesInvoice Net Total" minOccurs="0" type="xs:decimal">
				<xs:annotation>
					<xs:documentation>
						Total value of the Sales Invoice, including discounts, excluding tax.
						 
						In Sales Invoice currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="discountTotal" sme:label="SalesInvoice Discount Total" minOccurs="0" type="xs:decimal">
				<xs:annotation>
					<xs:documentation>
						Total discounts applied to the Sales Invoice.
						 
						In Sales Invoice currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="chargesTotal" sme:label="SalesInvoice Charges Total" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Total value of any (sur)charges applied to the Sales Invoice (e.g. finance charges, interest).
						 
						In Sales Invoice currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxTotal" sme:label="SalesInvoice Tax Total" minOccurs="0" type="xs:decimal">
				<xs:annotation>
					<xs:documentation>
						Total tax applicable to the Sales Invoice.
						 
						In Sales Invoice currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="grossTotal" sme:label="SalesInvoice Gross Total" minOccurs="0" type="xs:decimal">
				<xs:annotation>
					<xs:documentation>
						Total value of the Sales Invoice, including tax &amp; discounts &amp; charges.
						 
						In Sales Invoice currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="costTotal" sme:label="SalesInvoice Cost Total" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Total cost of the Sales Invoice.
						 
						In Sales Invoice currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="profitTotal" sme:label="SalesInvoice Profit Total" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Total profit on the sales Sales Invoice (e.g. net total – cost total).
						 
						In Sales Invoice currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="text1" sme:label="SalesInvoice Text1" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Text/commentary (e.g. special Delivery instructions) for the Sales Invoice
						 
						Header text
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="text2" sme:label="SalesInvoice Text2" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Text/commentary (e.g. special Delivery instructions) for the Sales Invoice
						 
						Footer text
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="projects" sme:label="SalesInvoice Project" minOccurs="0" type="project--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Identifier of the Project this Sales Invoice is for.
						 
						See “Project” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="projectLines" sme:label="SalesInvoice ProjectLine" minOccurs="0" type="projectLine--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Project Line value for this Sales Invoice.
						 
						See “Project Line” definition.
						Some applications may not have Project Lines.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="cases" sme:label="SalesInvoice Case reference" minOccurs="0" type="case--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The reference of the ‘case’ associated with the Sales Invoice e.g. if the Invoice is for repair/fix services.
						 
						A ‘Case’ entity hase been defined to support the F2F process.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="financialAccounts" sme:label="SalesInvoice FinancialAccount" minOccurs="0" type="financialAccount--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Financial accounting code applicable to the Sales Invoice.
						 
						See “FinancialAccount” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="interactions" sme:label="SalesInvoice Interaction" minOccurs="0" type="interaction--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Interaction(s) associated with the Sales Invoice.
						 
						See “Interaction” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="user" sme:label="SalesInvoice User" minOccurs="0" type="xs:string" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to the user who generated the Sales Invoice.
						 
						The ‘user’ resource will be defined in the future.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="notes" sme:label="SalesInvoice Note" minOccurs="0" type="note--list" sme:relationship="child" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Notes associated with a Sales Invoice (e.g. credit control).
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
	</xs:complexType>
	<xs:complexType name="salesInvoice--list">
		<xs:sequence minOccurs="0" maxOccurs="unbounded">
			<xs:element name="salesInvoice" type="salesInvoice--type" />
		</xs:sequence>
	</xs:complexType>
	<xs:element name="salesInvoiceLine" type="salesInvoiceLine--type" sme:role="resourceKind" sme:pluralName="salesInvoiceLines" sme:compliance="should" sme:canGet="true" sme:canPut="true" sme:canPost="true" />
	<xs:complexType name="salesInvoiceLine--type">
		<xs:all>
			<xs:element name="uuid" sme:label="SalesInvoiceLine Global ID" minOccurs="0" type="uuid--type" sme:isUniqueKey="true">
				<xs:annotation>
					<xs:documentation>
						The Sales Invoice Line unique identifier.
						 
						Globally unique identifier
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="id" sme:label="SalesInvoiceLine Application ID" minOccurs="0" type="xs:string" sme:isUniqueKey="true">
				<xs:annotation>
					<xs:documentation>
						The application specific identifier.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="label" sme:label="SalesInvoiceLine Label" minOccurs="0" type="xs:string">
				<xs:annotation>
					<xs:documentation>
						The label for the resource to be presented in the user interface. To support application-specific or language-specific descriptions of resources.
						 
						Default value “Sales Invoice Line”
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="originatorDocumentLine" sme:label="SalesInvoiceLine Originator Document Line reference" minOccurs="0" type="salesOrderLine--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
				<xs:annotation>
					<xs:documentation>
						reference to a Line on another document (e.g. the sales order Line or bill Line associated with the Invoice Line).
						 
						Sales Invoice Lines are often associated with Sales Order Lines.
						Could also be a link to the original Invoice Line if an Invoice is cancelled (reversed).
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="active" sme:label="SalesInvoiceLine Active Flag" minOccurs="0" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>
						Flag to indicate if the Sales Invoice Line is an ‘active’ one .
						 
						Default = True
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deleted" sme:label="SalesInvoiceLine Deleted" minOccurs="0" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>
						Flag to indicate if Sales Invoice Line is (soft) deleted.
						 
						Default = False
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference" sme:label="SalesInvoiceLine reference" minOccurs="0" type="xs:string" sme:isUniqueKey="true">
				<xs:annotation>
					<xs:documentation>
						reference for the Sales Invoice.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference2" sme:label="SalesInvoiceLine Reference2" minOccurs="0" type="xs:string" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Additional reference for the Sales Invoice.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="status" sme:label="SalesInvoiceLine Status" minOccurs="0" type="salesInvoiceStatus--enum" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Status of the Sales Invoice Line.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="type" sme:label="SalesInvoiceLine Type" minOccurs="0" type="lineType--enum" sme:mandatory="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Enumerator indicating the type of Sales Invoice Line.
						 
						Allowable values:
						Standard Line;
						Free text (with price);
						Commentary (without price, e.g. “thank you for your business!”);
						Component (with price but not in total);
						Discount/charges (included in price/total)

						Default = “Standard Line”

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="number" sme:label="SalesInvoiceLine Number" minOccurs="0" type="xs:decimal">
				<xs:annotation>
					<xs:documentation>
						The number of the Sales Invoice Line.
						 
						For sequencing and ordering of lines.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="commodity" sme:label="SalesInvoiceLine Commodity" minOccurs="0" type="commodity--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to the Commodity used on the Sales Invoice Line (if any).
						 
						See “Commodity” definition.

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="text" sme:label="SalesInvoiceLine Text" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Text applicable to the Sales Invoice Line (e.g. the Line may be a comment Line or a free text Line). If the Sales Invoice Line is for a Commodity then the Commodity description should be held here.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="commodityVariant" sme:label="SalesInvoiceLine Commodity Variant" minOccurs="0" type="commodityVariant--type" sme:relationship="reference" sme:isCollection="false" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The Variant of the Commodity (e.g. colour or size) on the Sales Invoice Line.
						 
						See “Variant” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="commodityDimension" sme:label="SalesInvoiceLine Commodity Dimension" minOccurs="0" type="commodityDimension--type" sme:relationship="reference" sme:isCollection="false" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The Dimension of the Commodity on the Sales Invoice Line.
						 
						See “Dimension” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deliveryTerms" sme:label="SalesInvoiceLine Delivery Terms" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The terms for Delivery.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deliveryDate" sme:label="SalesInvoiceLine Delivery Date" minOccurs="0" type="xs:date" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The anticipated date of the Delivery.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierCompany" sme:label="SalesInvoiceLine Carrier Company" minOccurs="0" type="company--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to the Company that will be the carrier / freight forwarder/ deliverer of the Sales Invoice Line.
						 
						See “Company” definition.
						Carriers are not used by all applications.

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierSupplier" sme:label="SalesInvoiceLine Carrier Supplier" minOccurs="0" type="supplier--type" sme:relationship="reference" sme:isCollection="false" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to the Supplier associated with the Company that wll be the carrier / freight forwarder / deliverer of the Sales Invoice Line.
						 
						See “Supplier” definition.
						Carriers are not used by all application.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierTradingAccount" sme:label="SalesInvoiceLine Carrier TradingAccount" minOccurs="0" type="tradingAccount--type" sme:relationship="reference" sme:isCollection="false" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to the Trading Account associated with the Supplier that wll be the carrier / freight forwarder / deliverer of the Sales Invoice Line.
						 
						See “TradingAccount” definition.
						Carriers are not used by all application.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierNetPrice" sme:label="SalesInvoiceLine Carrier Net Price" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The price of Delivery/carriage.
						 
						In Invoice currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierTaxPrice" sme:label="SalesInvoiceLine Carrier Tax Price" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The tax on Delivery/carriage.
						 
						In Invoice currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierTotalPrice" sme:label="SalesInvoiceLine Carrier Gross Price" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The total price of Delivery/carriage (net + VAT).
						 
						In Invoice currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierSalesInvoiceLine" sme:label="SalesInvoiceLine Carrier SalesInvoiceLine" minOccurs="0" type="salesInvoiceLine--type" sme:relationship="reference" sme:isCollection="false" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						A link to the Sales Invoice Line generated to recharge the carriage to the Customer.
						 
						See “SalesInvoiceLine” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierPurchaseInvoiceLine" sme:label="SalesInvoiceLine Carrier PurchaseInvoiceLine" minOccurs="0" type="purchaseInvoiceLine--type" sme:relationship="reference" sme:isCollection="false" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						A link to the purchase Invoice charged by the carrier Company for the Delivery to the Customer.
						 
						See “PurchaseInvoiceLine” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierTaxCodes" sme:label="SalesInvoiceLine Carrier Tax Code" minOccurs="0" type="taxCode--list" sme:relationship="association" sme:isCollection="true" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Tax code governing the tax regime applicable to the carriage on this Sales Invoice Line.
						 
						See “TaxCode” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierReference" sme:label="SalesInvoiceLine Carrier reference" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						reference number as used by the carrier during Delivery e.g. for Delivery tracking.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="unitOfMeasure" sme:label="SalesInvoiceLine UnitOfMeasure" minOccurs="0" type="unitOfMeasure--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
				<xs:annotation>
					<xs:documentation>
						A link to the scale to be used in the measurement of physical quantities of Commodity. Examples include "Each", "Box of 10", etc. May also be called "Pack Size".
						 
						See “UnitOfMeasure” definition.

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="quantity" sme:label="SalesInvoiceLine Quantity" minOccurs="0" type="xs:decimal" sme:mandatory="true">
				<xs:annotation>
					<xs:documentation>
