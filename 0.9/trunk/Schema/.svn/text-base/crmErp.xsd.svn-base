			<xs:element name="orderLineDiscountAmount" sme:label="SalesOrderLine Order Line Discount Amount" minOccurs="0" type="xs:decimal" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Discount applicable to the Sales Order Line, expressed as an amount.
						 
						From the Trading Account.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderLineDiscountPercent" sme:label="SalesOrderLine Order Line Discount Percent" minOccurs="0" type="xs:decimal" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Discount applicable to the Sales Order Line, expressed as a percentage.
						 
						From the Trading Account.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderLineSubtotalDiscountType" sme:label="SalesOrderLine Order Line Subtotal Discount Type" minOccurs="0" type="discountType--enum" sme:isExtension="true" nillable="true">
