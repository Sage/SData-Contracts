					<xs:documentation>
						The application specific identifier.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="label" sme:label="SalesOrderLine Label" minOccurs="0" type="xs:string">
				<xs:annotation>
					<xs:documentation>
						The label for the resource to be presented in the user interface. To support application-specific or language-specific descriptions of resources.
						 
						“Sales Order Line”
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="active" sme:label="SalesOrderLine Active Flag" minOccurs="0" type="xs:boolean">
