					<xs:documentation>
						reference for the Sales Order Line.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference2" sme:label="SalesOrderLine Reference2" minOccurs="0" type="xs:string" sme:isExtension="true" nillable="true">
