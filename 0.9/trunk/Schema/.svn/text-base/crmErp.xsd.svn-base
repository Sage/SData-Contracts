					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="financialAccounts" sme:label="PurchaseRequisitionLine FinancialAccount" minOccurs="0" type="financialAccount--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
