	<xs:complexType name="interaction--type">
		<xs:all>
			<xs:element name="id" sme:label="Interaction Application ID" minOccurs="0" type="xs:string" sme:isUniqueKey="true">
				<xs:annotation>
					<xs:documentation>
						The application specific identifier.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="active" sme:label="Interaction Active Flag" minOccurs="0" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>
						Flag to indicate if the Interaction is an ‘active’ one .
						 
						Default = True
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deleted" sme:label="Interaction Deleted" minOccurs="0" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>
						Flag to indicate if Interaction is (soft) deleted.
						 
						Default = False
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="label" sme:label="Interaction Label" minOccurs="0" type="xs:string">
				<xs:annotation>
					<xs:documentation>
						The label for the resource to be presented in the user interface. To support application-specific or language-specific descriptions of resources.
						 
						“Interaction”
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="type" sme:label="Interaction Type" minOccurs="0" type="interactionType--enum" sme:mandatory="true">
