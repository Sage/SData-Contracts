						See “TradingAccount” definition.

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="sellerContact" sme:label="PurchaseRequisition Seller Contact" minOccurs="0" type="contact--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
