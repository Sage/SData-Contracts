			<xs:element name="deliveryLineSubtotalDiscountAmount" sme:label="SalesReturnDeliveryLine Delivery Line Subtotal Discount Amount" minOccurs="0" type="xs:decimal" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Discount applicable to the Sales Return Delivery Line, expressed as an amount.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deliveryLineSubtotalDiscountPercent" sme:label="SalesReturnDeliveryLine Delivery Line Subtotal Discount Percent" minOccurs="0" type="xs:decimal" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Discount applicable to the Sales Return Delivery Line, expressed as a percentage.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="actualPrice" sme:label="SalesReturnDeliveryLine Actual Price" minOccurs="0" type="xs:decimal">
				<xs:annotation>
					<xs:documentation>
						The actual price of the Commodity ordered on the Sales Return Delivery Line (e.g. after discounts or adjustments).
						 
						In Delivery currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="netTotal" sme:label="SalesReturnDeliveryLine Net Total" minOccurs="0" type="xs:decimal">
				<xs:annotation>
					<xs:documentation>
						Total value of the Sales Return Delivery Line, including discounts, excluding tax (e.g. quantity * actual price)
						 
						In Delivery currency.
						Could be calculated.

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="chargesTotal" sme:label="SalesReturnDeliveryLine Charges Total" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Total value of any (sur)charges applied to the Sales Return Delivery Line (e.g. finance charges, interest).
						 
						In Delivery currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="discountTotal" sme:label="SalesReturnDeliveryLine Discount Total" minOccurs="0" type="xs:decimal">
				<xs:annotation>
					<xs:documentation>
						Total discounts applied to the return delivery line
						 
						In delivery currency
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxCodes" sme:label="SalesReturnDeliveryLine Tax Code" minOccurs="0" type="taxCode--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Tax code governing the tax regime applicable to this Sales Return Delivery Line (default value from sales order).
						 
						See “TaxCode” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="priceTax" sme:label="SalesReturnDeliveryLine Price Tax" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Tax applicable to the Sales Return Delivery Line price e.g. the per unit tax.
						 
						Both unit-tax and Line-tax values are required due to the way tax is handled differently by product/country.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxTotal" sme:label="SalesReturnDeliveryLine Tax Total" minOccurs="0" type="xs:decimal">
				<xs:annotation>
					<xs:documentation>
						Tax applicable to the Sales Return Delivery Line.
						 
						In Delivery currency.
						Could be calculated.
						Both unit-tax and Line-tax values are required due to the way tax is handled differently by product/country.

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="grossTotal" sme:label="SalesReturnDeliveryLine Gross Total" minOccurs="0" type="xs:decimal">
				<xs:annotation>
					<xs:documentation>
						Total value of the Sales Return Delivery Line, including discounts &amp; tax &amp; charges.
						 
						In Delivery currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="costTotal" sme:label="SalesReturnDeliveryLine Cost Total" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Total cost of the Sales Return Delivery Line.
						 
						In Delivery currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="profitTotal" sme:label="SalesReturnDeliveryLine Profit Total" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Total profit on the Sales Return Delivery Line (e.g. net total – cost total).
						 
						In Delivery currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="receipts" sme:label="SalesReturnDeliveryLine Receipt" minOccurs="0" type="receipt--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						A link to the (cash) receipt(s) associated with this Sales Order Line.
						 
						See “Receipt” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="financialAccounts" sme:label="SalesReturnDeliveryLine FinancialAccount" minOccurs="0" type="financialAccount--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Financial accounting code applicable to the Sales Return Delivery Line.
						 
						See “FinancialAccount” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="notes" sme:label="SalesReturnDeliveryLine Note" minOccurs="0" type="note--list" sme:relationship="child" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Notes associated with a Sales Return Delivery Line.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
	</xs:complexType>
	<xs:complexType name="salesReturnDeliveryLine--list">
		<xs:sequence minOccurs="0" maxOccurs="unbounded">
			<xs:element name="salesReturnDeliveryLine" type="salesReturnDeliveryLine--type" />
		</xs:sequence>
	</xs:complexType>
	<xs:element name="purchaseOrder" type="purchaseOrder--type" sme:role="resourceKind" sme:pluralName="purchaseOrders" sme:compliance="may" sme:canGet="true" sme:canPost="true" />
	<xs:complexType name="purchaseOrder--type">
