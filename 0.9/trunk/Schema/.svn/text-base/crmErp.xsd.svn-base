					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="commodityDimension" sme:label="SalesOrderLine Commodity Dimension" minOccurs="0" type="commodityDimension--type" sme:relationship="reference" sme:isCollection="false" sme:isExtension="true" nillable="true">
