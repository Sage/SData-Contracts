					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="commodityVariant" sme:label="SalesOrderLine Commodity Variant" minOccurs="0" type="commodityVariant--type" sme:relationship="reference" sme:isCollection="false" sme:isExtension="true" nillable="true">
