				<xs:annotation>
					<xs:documentation>
						Total value of any (sur)charges applied to the Sales Quotation Line (e.g. finance charges, interest).
						 
						In Quotation currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="discountTotal" sme:label="SalesQuotationLine Discount Total" minOccurs="0" type="xs:decimal">
				<xs:annotation>
					<xs:documentation>
						Total discounts applied to the quotation line
						 
						In quotation currency
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxCodes" sme:label="SalesQuotationLine Tax Code" minOccurs="0" type="taxCode--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Tax code governing the tax regime applicable to this Sales Quotation Line (default value from Sales Quotation).
						 
						See “TaxCode” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="priceTax" sme:label="SalesQuotationLine Price Tax" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Tax applicable to the Sales Quotation Line price e.g. the per unit tax.
						 
						Both unit-tax and Line-tax values are required due to the way tax is handled differently by product/country.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxTotal" sme:label="SalesQuotationLine Tax Total" minOccurs="0" type="xs:decimal">
				<xs:annotation>
					<xs:documentation>
						Tax applicable to the Sales Quotation Line.
						 
						In Quotation currency.
						Could be calculated.
						Both unit-tax and Line-tax values are required due to the way tax is handled differently by product/country.

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="grossTotal" sme:label="SalesQuotationLine Gross Total" minOccurs="0" type="xs:decimal">
				<xs:annotation>
					<xs:documentation>
						Total value of the Sales Quotation Line, including discounts &amp; tax &amp; charges.
						 
						In Quotation currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="costTotal" sme:label="SalesQuotationLine Cost Total" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Total cost of the Sales Quotation Line.
						 
						In Quotation currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="profitTotal" sme:label="SalesQuotationLine Profit Total" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Total profit on the Sales Quotation Line (e.g. net total – cost total).
						 
						In Quotation currency.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="buyerContact" sme:label="SalesQuotationLine Buyer Contact" minOccurs="0" type="contact--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to the Sales Quotation Line originator (buyer Contact).
						 
						See “Contact” definition.
						This could be derived from the Sales Quotation
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="project" sme:label="SalesQuotationLine Project" minOccurs="0" type="project--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Identifier of the Project this Sales Quotation Line is for.
						 
						See “Project” definition.

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="projectLine" sme:label="SalesQuotationLine ProjectLine" minOccurs="0" type="projectLine--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Project Line value for this Sales Quotation Line.
						 
						See “Project Line” definition.
						Some applications may not have Project Lines.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="bOMLine" sme:label="SalesQuotationLine BOM Line" minOccurs="0" type="xs:string" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						BOM Line associated with the Sales Quotation Line
						 
						The ‘BOM Line’ resource will be defined in the future.
						Not all applications support this feature.

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="financialAccounts" sme:label="SalesQuotationLine FinancialAccount" minOccurs="0" type="financialAccount--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Financial accounting code applicable to the Sales Quotation Line.
						 
						See “FinancialAccount” definition.
						This could be derived from the Sales Quotation
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="notes" sme:label="SalesQuotationLine Note" minOccurs="0" type="note--list" sme:relationship="child" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Notes associated with a Sales Quotation Line.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
	</xs:complexType>
	<xs:complexType name="salesQuotationLine--list">
		<xs:sequence minOccurs="0" maxOccurs="unbounded">
			<xs:element name="salesQuotationLine" type="salesQuotationLine--type" />
		</xs:sequence>
	</xs:complexType>
	<xs:element name="commodity" type="commodity--type" sme:role="resourceKind" sme:pluralName="commodities" sme:compliance="must" sme:canGet="true" />
	<xs:complexType name="commodity--type">
		<xs:all>
			<xs:element name="id" sme:label="Commodity Application ID" minOccurs="0" type="xs:string" sme:isUniqueKey="true">
				<xs:annotation>
					<xs:documentation>
						The application specific identifier.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="active" sme:label="Commodity Active Flag" minOccurs="0" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>
						Flag to indicate if the Commodity is an ‘active’ one .
						 
						Default = True
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deleted" sme:label="Commodity Deleted" minOccurs="0" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>
						Flag to indicate if Commodity is (soft) deleted.
						 
						Default = False
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="label" sme:label="Commodity Label" minOccurs="0" type="xs:string">
				<xs:annotation>
					<xs:documentation>
						The label for the resource to be presented in the user interface. To support application-specific or language-specific descriptions of resources.
						 
						“Commodity”
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="commodityGroup" sme:label="Commodity CommodityGroup" minOccurs="0" type="commodityGroup--type" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
						Identifier of the Commodity group this Commodity belongs to.
						 
						See “CommodityGroup” definition.
						Commodities can only belong to one Commodity Group.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="commodityIdentifierType" sme:label="Commodity CommodityIdentifier Type" minOccurs="0" type="commodityCommodityIdentifierType--enum" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Type of the Commodity Identifier associated with this Commodity (if any) (e.g. Total, Batch (lot), Serial Number).
						 
						Total
						Batch/Lot
						Serial
						Serial from Batch/Lot
						Unknown

						Default = “Total”

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="commodityIdentifiers" sme:label="Commodity CommodityIdentifier" minOccurs="0" type="commodityIdentifier--list" sme:relationship="child" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to the Commodity Identifier(s) for this Commodity, if it is traced to that level of detail.
						 
						See “CommodityIdentifier” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference" sme:label="Commodity reference" minOccurs="0" type="xs:string" sme:isUniqueKey="true">
				<xs:annotation>
					<xs:documentation>
						reference for the Commodity (typically the one used by the buying organization).
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference2" sme:label="Commodity Reference2" minOccurs="0" type="xs:string" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Another reference for the Commodity.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operatingCompany" sme:label="Commodity OperatingCompany" minOccurs="0" type="company--type" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
						Link to the OperatingCompany associated with the Commodity.
						 
						See “Company” definition (where Company Type = “Operating Company”).
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="status" sme:label="Commodity Status" minOccurs="0" type="xs:string" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Status of the Commodity.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="name" sme:label="Commodity Name" minOccurs="0" type="xs:string" sme:mandatory="true">
				<xs:annotation>
					<xs:documentation>
						Short name for the Commodity.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="shortName" sme:label="Commodity Short Name" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						A shorter name for the Commodity.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="description" sme:label="Commodity Description" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The Commodity description.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="alternativeDescription1" sme:label="Commodity Alternative Description 1" minOccurs="0" type="xs:string" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						An alternative Commodity description (e.g. buying or selling descriptions may be different).
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="alternativeDescription2" sme:label="Commodity Alternative Description 2" minOccurs="0" type="xs:string" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Another alternative Commodity description (e.g. could be used for special Delivery instructions, handling instructions, or alternative language descriptions, etc).
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="type" sme:label="Commodity Type" minOccurs="0" type="commodityType--enum" sme:mandatory="true">
				<xs:annotation>
					<xs:documentation>
						The Commodity type (e.g. product, service).
						 
						Default value should come from CommodityGroup.
						Product Stock (e.g. finished goods and/or raw materials)
						Product Non Stock (e.g. Misc or expense).
						Service (contracts).
						BillOfMaterial (Manufactured Item) (e.g. assembled Kit).
						(Built to Order) Kit.
						Subcontract
						Description (e.g. comment).
						User defined.
						Unknown.

						Default = “Unknown”
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="unitOfMeasure" sme:label="Commodity UnitOfMeasure" minOccurs="0" type="unitOfMeasure--type" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
						Link to the Unit of Measure (pack size / quantity) for the Commodity (e.g. each, six-pack, case, etc). Also known as stocking Unit of Measure.
						 
						See “UnitOfMeasure” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="buyingAllowedFlag" sme:label="Commodity Buying Allowed Flag" minOccurs="0" type="xs:boolean" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Flag to indicate that this Commodity can be ‘bought’ i.e. can be used on purchase orders/requisitions.
						 
						Default = Yes
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="buyingUnitOfMeasure" sme:label="Commodity Buying UnitOfMeasure" minOccurs="0" type="unitOfMeasure--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to the Unit of Measure (pack size / quantity) for the Commodity used during buying (e.g. each, six-pack, case, etc)
						 
						See “UnitOfMeasure” definition.

						Default = Commodity UnitOfMeasure
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="buyingUnitOfMeasureNumber" sme:label="Commodity Buying UnitOfMeasure Number" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The number (quantity) of items in a pack of the Commodity (e.g. 1, 6, 24, etc).
						 
						Default = 1
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="sellingAllowedFlag" sme:label="Commodity Selling Allowed Flag" minOccurs="0" type="xs:boolean" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Flag to indicate that this Commodity can be ‘sold’ i.e. can be used on sales orders/quotations.
						 
						Default = Yes
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="sellingUnitOfMeasure" sme:label="Commodity Selling UnitOfMeasure" minOccurs="0" type="unitOfMeasure--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to the Unit of Measure (pack size / quantity) for the Commodity used during selling/retail (e.g. each, six-pack, case, etc)
						 
						See “UnitOfMeasure” definition.

						Default = Commodity UnitOfMeasure
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="sellingUnitOfMeasureNumber" sme:label="Commodity Selling UnitOfMeasure Number" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The number (quantity) of items in a pack of the Commodity (e.g. 1, 6, 24, etc).
						 
						1
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="packingUnitOfMeasure" sme:label="Commodity Packing UnitOfMeasure" minOccurs="0" type="unitOfMeasure--type" sme:relationship="reference" sme:isCollection="false" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to the Unit of Measure (pack size / quantity) for the Commodity used during packing/dispatch (e.g. each, six-pack, case, etc)
						 
						See “UnitOfMeasure” definition.
						This could be the default or buying Unit of Measure.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="packingUnitOfMeasureNumber" sme:label="Commodity Packing UnitOfMeasure Number" minOccurs="0" type="xs:decimal" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The number (quantity) of items in a pack of the Commodity (e.g. 1, 6, 24, etc).
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="catalogueNumber" sme:label="Commodity Catalogue Number" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						reference for the Commodity in a Supplier catalogue.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="supplierTradingAccount" sme:label="Commodity Supplier TradingAccount" minOccurs="0" type="tradingAccount--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to the Commodity Supplier trading account.
						 
						See “TradingAccount” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="supplierReference" sme:label="Commodity Supplier reference" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						reference for the Commodity as used by the Supplier.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="manufacturerCompany" sme:label="Commodity Manufacturer Company" minOccurs="0" type="company--type" sme:relationship="reference" sme:isCollection="false" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to the Company associated with manufacturing the Commodity.
						 
						See “Company” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="manufacturerReference" sme:label="Commodity Manufacturer reference" minOccurs="0" type="xs:string" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						reference for the Commodity as used by the manufacturer.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="material" sme:label="Commodity Material" minOccurs="0" type="xs:string" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Material from which the Commodity is made.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="countryOfOrigin" sme:label="Commodity Country Of Origin" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The country where the Commodity originated.
						 
						ISO 3166-1 alpha-2 country codes only
						Could default from CommodityGroup.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="weight" sme:label="Commodity Weight" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Weight of the Commodity.
						 
						In system default weight unit (metric or imperial, tonnes, quintals, etc).
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="weightKg" sme:label="Commodity Weight KG" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Weight of the Commodity in SI units (Kilograms).
						 
						In kilograms – may be different to the standard weight.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="variant" sme:label="Commodity Variant" minOccurs="0" type="commodityVariant--type" sme:relationship="association" sme:isCollection="true" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The Variant of the Commodity (e.g. colour or size).
						 
						See “Variant” definition.

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dimension" sme:label="Commodity Dimension" minOccurs="0" type="commodityDimension--type" sme:relationship="association" sme:isCollection="true" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The physical Dimension of the Commodity (e.g. height).
						 
						See “Dimension” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="classification" sme:label="Commodity Classification" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Classification of the Commodity.
						 
						Based on standard classification system (Intrastat in EU)
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxCodes" sme:label="Commodity Tax Code" minOccurs="0" type="taxCode--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Tax code governing the tax treatment of this Commodity (e.g. VAT, GST).
						 
						See “TaxCode” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="financialAccounts" sme:label="Commodity FinancialAccount" minOccurs="0" type="financialAccount--list" sme:relationship="association" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Financial accounting code applicable to the Commodity.
						 
						See “FinancialAccount” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dutyApplicableFlag" sme:label="Commodity Duty Applicable Flag" minOccurs="0" type="xs:boolean" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Flag to indicate if duty (import tax?) is applicable to this Commodity.
						 
						Default = False
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dutyRate" sme:label="Commodity Duty Rate" minOccurs="0" type="xs:decimal" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Rate of duty applicable to this Commodity (if duty is applicable).
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tradeIdentificationNumber" sme:label="Commodity Trade Identification Number" minOccurs="0" type="xs:string" sme:isUniqueKey="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The GTIN (global trade identification number) for the Commodity (e.g. EAN-13, UPC-13 for barcodes).
						 
						Used for handling bar codes.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="replacement" sme:label="Commodity Replacement" minOccurs="0" type="commodity--type" sme:relationship="reference" sme:isCollection="false" nillable="true">
				<xs:annotation>
					<xs:documentation>
						If this Commodity has been superseded this is a link to the ‘new’ Commodity.
						 
						See “Commodity” definition.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="replacementDate" sme:label="Commodity Replacement Date" minOccurs="0" type="xs:date" nillable="true">
				<xs:annotation>
					<xs:documentation>
						For superceded/replaced commodities, this is the start date of the replacement.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="leadTime" sme:label="Commodity Lead Time" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						The (average) number of days to supply the Commodity from (all) Suppliers.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="cost" sme:label="Commodity Cost" minOccurs="0" type="xs:decimal" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Depending on the costing method used, this is the “cost” of the commodity (e.g. average cost)
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="associatedChildCommodities" sme:label="Associated child Commodity" minOccurs="0" type="commodity--list" sme:relationship="child" sme:isCollection="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Link to child Commodity associated with this Commodity (e.g. kit parts).
						 
						Commodities can be ‘nested’ to create Commodity hierarchies, typically for Kit or BOM items.
					</xs:documentation>
