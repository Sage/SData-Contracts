						Default = True
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deliveryTerms" sme:label="PurchaseCredit Delivery Terms" minOccurs="0" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>
