			<xs:element name="invoiceLineDiscountPercent" sme:label="PurchaseInvoiceLine Invoice Line Discount Percent" minOccurs="0" type="xs:decimal" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Any discount that may be applicable to the Purchase Invoice Line, expressed as a percentage.
						 
						From the Purchase Invoice.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceLineSubtotalDiscountType" sme:label="PurchaseInvoiceLine Invoice Line Subtotal Discount Type" minOccurs="0" type="discountType--enum" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Flag to indicate the type of discount applicable to a subtotal of Purchase Invoice Lines (percent or amount) of which this Line is part.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceLineSubtotalDiscountAmount" sme:label="PurchaseInvoiceLine Invoice Line Subtotal Discount Amount" minOccurs="0" type="xs:decimal" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Discount applicable to the Purchase Invoice Line, expressed as an amount.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceLineSubtotalDiscountPercent" sme:label="PurchaseInvoiceLine Invoice Line Subtotal Discount Percent" minOccurs="0" type="xs:decimal" sme:isExtension="true" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Discount applicable to the Purchase Invoice Line, expressed as a percentage.
						 

					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="actualPrice" sme:label="PurchaseInvoiceLine Actual Price" minOccurs="0" type="xs:decimal">
				<xs:annotation>
					<xs:documentation>
