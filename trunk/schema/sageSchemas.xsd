			<xs:element name="originatorDocument" type="salesQuotationOriginatorDocument--choice" nillable="true" minOccurs="0" sme:label="SalesQuotation Originator Document Reference" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
