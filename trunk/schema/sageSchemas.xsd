	<xs:element name="purchaseReturnDelivery" type="purchaseReturnDelivery--type" sme:role="resourceKind" sme:pluralName="purchaseReturnDeliveries" sme:compliance="may" sme:canGet="true"/>
	<xs:complexType name="purchaseReturnDelivery--type">
