          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="costCentres" type="costCentre--list" nillable="true" minOccurs="0" sme:label="Payment CostCentres" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
