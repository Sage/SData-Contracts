	<xs:element name="salesCredit" type="salesCredit--type" sme:role="resourceKind" sme:pluralName="salesCredits" sme:compliance="should" sme:canGet="true" sme:canPut="true" sme:canPost="true" sme:hasUuid="true"/>
	<xs:complexType name="salesCredit--type">
		<xs:all>
			<xs:element name="active" type="xs:boolean" minOccurs="0" sme:label="SalesCredit Active Flag">
				<xs:annotation>
					<xs:documentation>
