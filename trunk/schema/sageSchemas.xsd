          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="cases" type="case--list" nillable="true" minOccurs="0" sme:label="PurchaseReturn Cases" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
