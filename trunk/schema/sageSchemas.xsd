            True
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference" type="xs:normalizedString" minOccurs="0" sme:label="SalesCredit Reference" sme:isUniqueKey="true">
				<xs:annotation>
					<xs:documentation>Reference for the Sales Credit.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference2" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="SalesCredit Reference2">
				<xs:annotation>
					<xs:documentation>Additional reference for the Sales Credit.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="status" type="salesCreditStatus--enum" nillable="true" minOccurs="0" sme:label="SalesCredit Status">
				<xs:annotation>
					<xs:documentation>Status of the Sales Credit</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="statusFlag" type="xs:boolean" nillable="true" minOccurs="0" sme:label="SalesCredit Status Flag">
				<xs:annotation>
