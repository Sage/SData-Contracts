          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="status" type="purchaseReturnStatus--enum" nillable="true" minOccurs="0" sme:label="PurchaseReturnLine Status">
				<xs:annotation>
					<xs:documentation>Status of the Purchase Return Line.</xs:documentation>
				</xs:annotation>
			</xs:element>
