          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="statusFlagText" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="PurchaseInvoice Status Flag Text">
				<xs:annotation>
					<xs:documentation>
            Purchase Invoice status flag text. Default = On Hold
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="type" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="PurchaseInvoice Type">
				<xs:annotation>
					<xs:documentation>Type of Purchase Invoice.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tradingAccount" type="tradingAccount--type" minOccurs="0" sme:label="PurchaseInvoice Supplier TradingAccount" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
