				</xs:annotation>
			</xs:element>
			<xs:element name="supplierTradingAccountCurrencyExchangeRate" type="xs:decimal" nillable="true" minOccurs="0" sme:label="PurchaseOrderDelivery Supplier TradingAccount Currency Exchange Rate">
