          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="returnAdditionalDiscount1Percent" type="xs:decimal" nillable="true" minOccurs="0" sme:label="PurchaseReturn Return Additional Discount1 Percent">
				<xs:annotation>
					<xs:documentation>
            Discount applicable to the Purchase Return expressed as a percent.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="returnAdditionalDiscount2Type" type="discountType--enum" nillable="true" minOccurs="0" sme:label="PurchaseReturn Return Additional Discount2 Type">
				<xs:annotation>
					<xs:documentation>
