				<xs:annotation>
					<xs:documentation>
            The Payment this Payment Line is for. See Payment definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="active" type="xs:boolean" minOccurs="0" sme:label="PaymentLine Active Flag">
