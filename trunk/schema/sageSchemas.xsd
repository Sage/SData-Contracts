				</xs:annotation>
			</xs:element>
			<xs:element name="costCentres" type="costCentre--list" nillable="true" minOccurs="0" sme:label="SalesOrderLine CostCentres" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Cost Centre(s) associated with this Sales Order Line. See Cost Centre definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="departments" type="department--list" nillable="true" minOccurs="0" sme:label="SalesOrderLine Departments" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Department(s) associated with this Sales Order Line. See Department definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="notes" type="note--list" nillable="true" minOccurs="0" sme:label="SalesOrderLine Note" sme:relationship="child" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>Notes associated with a Sales Order Line.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
		<xs:anyAttribute/>
	</xs:complexType>
	<xs:complexType name="salesOrderLine--list">
		<xs:sequence>
			<xs:element name="salesOrderLine" type="salesOrderLine--type" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:anyAttribute/>
	</xs:complexType>
	<xs:element name="salesQuotation" type="salesQuotation--type" sme:role="resourceKind" sme:pluralName="salesQuotations" sme:compliance="should" sme:canGet="true" sme:canPut="true" sme:canPost="true" sme:hasUuid="true" sme:isSyncSource="true" sme:isSyncTarget="true" sme:syncOrder="170"/>
	<xs:complexType name="salesQuotation--type">
