          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference" type="xs:normalizedString" minOccurs="0" sme:label="SalesQuotation Reference" sme:isUniqueKey="true">
				<xs:annotation>
					<xs:documentation>Reference for the Sales Quotation.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference2" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="SalesQuotation Reference2">
