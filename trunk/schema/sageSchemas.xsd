            &amp; charges. In Return currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="receipts" type="receipt--list" nillable="true" minOccurs="0" sme:label="PurchaseReturn Receipt" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
