				</xs:annotation>
			</xs:element>
			<xs:element name="supplierTradingAccountCurrencyExchangeRateOperator" type="rateOperator--enum" nillable="true" minOccurs="0" sme:label="PurchaseOrderDelivery Supplier TradingAccount Currency Exchange Rate Operator">
