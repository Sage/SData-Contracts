	<xs:element name="tradingAccount" type="tradingAccount--type" sme:role="resourceKind" sme:pluralName="tradingAccounts" sme:compliance="must" sme:canGet="true" sme:canPut="true" sme:canPost="true" sme:canDelete="true" sme:hasUuid="true" sme:isSyncSource="true" sme:isSyncTarget="true" sme:syncOrder="50"/>
	<xs:complexType name="tradingAccount--type">
		<xs:all>
			<xs:element name="active" type="xs:boolean" minOccurs="0" sme:label="TradingAccount Active Flag">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate if the Trading Account is an active account.
            Default = True
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customerSupplierFlag" type="supplierFlag--enum" minOccurs="0" sme:label="TradingAccount Customer Supplier Flag">
				<xs:annotation>
					<xs:documentation>
            A flag to indicate if the Trading Account is a Customer or Supplier.
            This can be derived from the entity linked to this Trading Account (either Customer or
            Supplier).
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="companyPersonFlag" type="tradingAccountPersonFlag--enum" minOccurs="0" sme:label="TradingAccount Company Person Flag">
				<xs:annotation>
					<xs:documentation>
            A flag to indicate if the Trading Account is representative of a
            Company or a Person.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceTradingAccount" type="tradingAccount--type" nillable="true" minOccurs="0" sme:label="TradingAccount Invoice TradingAccount" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Link to the Trading Account to be invoiced for transactions (e.g. Sales
            Orders, Purchase Orders) if different from this TradingAccount. See TradingAccount
            definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="openedDate" type="xs:date" minOccurs="0" sme:label="TradingAccount Opened Date">
				<xs:annotation>
					<xs:documentation>
            Date the Trading Account was opened. W3C date time standard for XML
            (ISO 8601). The base timezone is UTC. The offset is included. UTC should be used. See the
            reference here: http://www.w3.org/TR/xmlschema-2/#isoformats
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference" type="xs:normalizedString" minOccurs="0" sme:label="TradingAccount Reference" sme:isUniqueKey="true">
				<xs:annotation>
					<xs:documentation>
            Reference for the Trading Account. A lot of applications have a
            user-entered reference for resources. E.g. in Sage 200 (UK) a customer will have a
            database ID (1234567890) as well as a user entered reference (ABB001). The reference
            field is used to hold the user-entered reference (ABB001) and the id (now
            applicationID) would hold the database ID. In some larger ERP's (Sage 1000 for example)
            there may be two user entered reference fields (e.g. ABB001 and ABBEY_KITCHENS).
            Reference 2 is provided for this case. In some applications the user-entered value
            (reference) may also be used as the database ID - in which case the same value would be
            used in reference and applicationID.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference2" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="TradingAccount Reference2">
				<xs:annotation>
					<xs:documentation>
            Additional reference for the Trading Account. A lot of applications
            have a user-entered reference for resources. E.g. in Sage 200 (UK) a customer will have
            a database ID (1234567890) as well as a user entered reference (ABB001). The
            reference field is used to hold the user-entered reference (ABB001) and the id (now
            applicationID) would hold the database ID. In some larger ERP's (Sage 1000 for example)
            there may be two user entered reference fields (e.g. ABB001 and ABBEY_KITCHENS).
            Reference 2 is provided for this case. In some applications the user-entered value
            (reference) may also be used as the database ID - in which case the same value would be
            used in reference and applicationID.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="status" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="TradingAccount Status">
				<xs:annotation>
					<xs:documentation>Status of the Trading Account.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="name" type="xs:normalizedString" minOccurs="0" sme:label="TradingAccount Name" sme:isMandatory="true">
				<xs:annotation>
					<xs:documentation>The Trading Account name.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="shortName" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="TradingAccount Short Name">
				<xs:annotation>
					<xs:documentation>A shorter name for the Trading Account.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="type" type="tradingAccountType--enum" nillable="true" minOccurs="0" sme:label="TradingAccount Type">
				<xs:annotation>
					<xs:documentation>The Trading Account type. Default = Unknown</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="registeredAddress" type="postalAddress--type" nillable="true" minOccurs="0" sme:label="TradingAccount Registered Address" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Link to the Trading Account registered address (e.g. main/office address).
            See PostalAddress definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="postalAddresses" type="postalAddress--list" nillable="true" minOccurs="0" sme:label="TradingAccount PostalAddress" sme:relationship="child" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Link to the Trading Account address(es) (e.g. invoice/billing address).
            See PostalAddress definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="shippingAddresses" type="postalAddress--list" nillable="true" minOccurs="0" sme:label="TradingAccount ShippingAddress" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Link to the Trading Account shipping address. See PostalAddress
            definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="billingAddresses" type="postalAddress--list" nillable="true" minOccurs="0" sme:label="TradingAccount BillingAddress" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Link to the Trading Account billing address. See PostalAddress
            definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="phones" type="phoneNumber--list" nillable="true" minOccurs="0" sme:label="TradingAccount Phone" sme:relationship="child" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Trading account telephone number(s). See PhoneNumber definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="emails" type="email--list" nillable="true" minOccurs="0" sme:label="TradingAccount Email" sme:relationship="child" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Trading account email address(es). See Email definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="website" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="TradingAccount Web Site">
				<xs:annotation>
					<xs:documentation>Trading Account web site address.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="contacts" type="contact--list" nillable="true" minOccurs="0" sme:label="TradingAccount Contact" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Link to the accounting Contact(s) for this tradingAccount. See
            Contact definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deliveryContact" type="contact--type" nillable="true" minOccurs="0" sme:label="TradingAccount Delivery Contact" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Link to the Contact for deliveries to this Trading Account. Default =
            Primary Contact
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="salesPersons" type="salesPerson--list" nillable="true" minOccurs="0" sme:label="TradingAccount SalesPersons" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Sales person(s) associated with the Trading Account. See
            SalesPerson definition. Also known as Sales Agent or Manager in some applications.
            There may be multiple and commission may be spread across multiple sales people. Could
            default from Customer (or Supplier).
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deliveryMethod" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="TradingAccount Delivery Method">
				<xs:annotation>
					<xs:documentation>
            Preferred Delivery method to be used for the Trading Account (e.g.
            Courier, Collection, etc). Could default from Customer (or Supplier).
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deliveryRule" type="xs:boolean" nillable="true" minOccurs="0" sme:label="TradingAccount Delivery Rule">
				<xs:annotation>
					<xs:documentation>
            Does the Trading Account accept full or partial deliveries. True =
            Accepts partial deliveries; False = Does not accept partial deliveries. Default = True
            Could default from Customer (or Supplier).
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="currency" type="xs:normalizedString" minOccurs="0" sme:label="TradingAccount Currency">
				<xs:annotation>
					<xs:documentation>
            The Trading Account currency (e.g. GBP, USD, EUR). ISO 4217.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxCodes" type="taxCode--list" nillable="true" minOccurs="0" sme:label="TradingAccount Tax Codes" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Tax code governing the tax regime applicable to this Trading Account.
            See TaxCode definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxReference" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="TradingAccount Tax Reference">
				<xs:annotation>
					<xs:documentation>
            Tax reference associated with the Trading Account.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxationCountry" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="TradingAccount Taxation Country">
				<xs:annotation>
					<xs:documentation>
            The country whose tax regime applies to this Trading Account. ISO
            3166-1 alpha-2 country codes only. Could default from PostalAddress.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="useTaxInclusivePrices" type="xs:boolean" nillable="true" minOccurs="0" sme:label="TradingAccount Use Tax Inclusive Prices">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate if prices associated with this Trading Account are
            inclusive of tax, or not. Default = No (prices are not inclusive of tax)
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="financeBalance" type="xs:decimal" nillable="true" minOccurs="0" sme:label="TradingAccount Finance Balance">
				<xs:annotation>
					<xs:documentation>
            Trading account financial balance. In TradingAccount currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="financeLimit" type="xs:decimal" nillable="true" minOccurs="0" sme:label="TradingAccount Finance Limit">
				<xs:annotation>
					<xs:documentation>
            Trading account financial (credit) limit. In TradingAccount currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="financeBalanceDate" type="xs:date" nillable="true" minOccurs="0" sme:label="TradingAccount Finance Balance Date">
				<xs:annotation>
					<xs:documentation>
            The date upon which the finance balance was based. W3C date time
            standard for XML (ISO 8601). The base timezone is UTC. The offset is included. UTC
            should be used. See the reference here: http://www.w3.org/TR/xmlschema-2/#isoformats
            Default = System date
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="financeStatusFlag" type="xs:boolean" nillable="true" minOccurs="0" sme:label="TradingAccount Finance Status Flag">
				<xs:annotation>
					<xs:documentation>
            Trading account financial status (e.g. on hold (or blocked), off
            hold). True is on hold, False is not on hold. Default = False
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="financeStatusText" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="TradingAccount Finance Status Text">
				<xs:annotation>
					<xs:documentation>
            Trading account financial status text. Please note: this is the label
            used to describe the Finance Status Flag above - it is not a status in its own right.
            Default = On Hold
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="settlementDiscountType" type="discountType--enum" nillable="true" minOccurs="0" sme:label="TradingAccount Settlement Discount Type">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate the type of settlement discount applicable to the
            Trading Account (percent or amount). Default = Percent
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="settlementDiscountAmount" type="xs:decimal" nillable="true" minOccurs="0" sme:label="TradingAccount Settlement Discount Amount">
				<xs:annotation>
					<xs:documentation>
            Settlement discount applicable to the Trading Account expressed as an
            amount.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="settlementDiscountPercent" type="xs:decimal" nillable="true" minOccurs="0" sme:label="TradingAccount Settlement Discount Percent">
				<xs:annotation>
					<xs:documentation>
            Settlement discount applicable to the Trading Account expressed as a
            percent.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="settlementDiscountTerms" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="TradingAccount Settlement Discount Terms">
				<xs:annotation>
					<xs:documentation>
            Terms applicable to the settlement discount, often expressed as a
            number of days.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="settlementDiscountTermsCommencement" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="TradingAccount Settlement Discount Terms Commencement">
				<xs:annotation>
					<xs:documentation>
            For some types of settlement discount it is necessary to know when the
            terms start, e.g. if terms are 30 days from end of month.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="settlementDiscountIncludedInTotal" type="xs:boolean" nillable="true" minOccurs="0" sme:label="TradingAccount Settlement Discount Included In Total">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate if settlement discount is included in document totals
            (e.g. Invoices, etc). Default = False
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="paymentTerms" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="TradingAccount Payment Terms">
				<xs:annotation>
					<xs:documentation>
            Period in which outstanding amounts must be paid (e.g. 30 days, by end
            of month EOM). Also after this period additional (sur)charges may apply.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="paymentTermsCommencement" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="TradingAccount Payment Terms Commencement">
				<xs:annotation>
					<xs:documentation>
            For some types of payment terms it is necessary to know when the terms
            start, e.g. if terms are 30 days from end of month.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderDiscountType" type="discountType--enum" nillable="true" minOccurs="0" sme:label="TradingAccount Order Discount Type">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate the type of order discount applicable to the Trading
            Account (percent or amount). Percent Amount Both Default = Percent
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderDiscountAmount" type="xs:decimal" nillable="true" minOccurs="0" sme:label="TradingAccount Order Discount Amount">
				<xs:annotation>
					<xs:documentation>
            Any discount that may be applicable to orders (or quotations) raised
            against the account, expressed as an amount.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderDiscountPercent" type="xs:decimal" nillable="true" minOccurs="0" sme:label="TradingAccount Order Discount Percent">
				<xs:annotation>
					<xs:documentation>
            Any discount that may be applicable to orders (or quotations) raised
            against the account, expressed as a percent.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderText1" type="xs:string" nillable="true" minOccurs="0" sme:label="TradingAccount Order Text1">
				<xs:annotation>
					<xs:documentation>
            Text to be shown by default on orders raised against this Trading
            Account. Header text.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderText2" type="xs:string" nillable="true" minOccurs="0" sme:label="TradingAccount Order Text2">
				<xs:annotation>
					<xs:documentation>
            Text to be shown by default on orders raised against this Trading
            Account. Footer text.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderLineDiscountType" type="discountType--enum" nillable="true" minOccurs="0" sme:label="TradingAccount Order Line Discount Type">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate the type of order Line discount applicable to the
            Trading Account (percent or amount). Percent Amount Both Default = Percent
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderLineDiscountAmount" type="xs:decimal" nillable="true" minOccurs="0" sme:label="TradingAccount Order Line Discount Amount">
				<xs:annotation>
					<xs:documentation>
            Any discount that may be applicable to order (or quotation) lines
            raised against the account, expressed as an amount.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderLineDiscountPercent" type="xs:decimal" nillable="true" minOccurs="0" sme:label="TradingAccount Order Line Discount Percent">
				<xs:annotation>
					<xs:documentation>
            Any discount that may be applicable to order (or quotation) lines
            raised against the account, expressed as a percent.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderLineText1" type="xs:string" nillable="true" minOccurs="0" sme:label="TradingAccount OrderLine Text1">
				<xs:annotation>
					<xs:documentation>
            Text to be shown by default on order lines raised against this Trading
            Account.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderLineText2" type="xs:string" nillable="true" minOccurs="0" sme:label="TradingAccount OrderLine Text2">
				<xs:annotation>
					<xs:documentation>
            Text to be shown by default on order lines raised against this Trading
            Account.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceDiscountType" type="discountType--enum" nillable="true" minOccurs="0" sme:label="TradingAccount Invoice Discount Type">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate the type of discount applicable to invoices raised
            against the Trading Account (percent or amount). Percent Amount Both Default =
            Percent
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceDiscountAmount" type="xs:decimal" nillable="true" minOccurs="0" sme:label="TradingAccount Invoice Discount Amount">
				<xs:annotation>
					<xs:documentation>
            Any discount that may be applicable to invoices raised against the
            Trading Account, expressed as an amount.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceDiscountPercent" type="xs:decimal" nillable="true" minOccurs="0" sme:label="TradingAccount Invoice Discount Percent">
				<xs:annotation>
					<xs:documentation>
            Any discount that may be applicable to invoices raised against the
            Trading Account, expressed as a percentage.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceText1" type="xs:string" nillable="true" minOccurs="0" sme:label="TradingAccount Invoice Text1">
				<xs:annotation>
					<xs:documentation>
            Text to be shown by default on Invoice raised against this Trading
            Account. Header text
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="invoiceText2" type="xs:string" nillable="true" minOccurs="0" sme:label="TradingAccount Invoice Text2">
				<xs:annotation>
					<xs:documentation>
            Text to be shown by default on Invoice raised against this Trading
            Account. Footer text
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="bankAccounts" type="bankAccount--list" nillable="true" minOccurs="0" sme:label="TradingAccount BankAccount" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Bank account associated with the Trading Account, e.g. for electronic
            Payment, direct debits, bank transfers, etc. See BankAccount definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tenderType" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="TradingAccount Default Tender Type">
				<xs:annotation>
					<xs:documentation>
            Tender Type used by Trading Account (e.g. if this Trading Account is
            associated with a Customer then this is the tender type used on Receipts/Payments
            associated with the Customer, e.g. BACS, check, Cash, Commercial Papers, etc).
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="defaultFulfillmentLocation" type="location--type" nillable="true" minOccurs="0" sme:label="TradingAccount Default Fulfillment Location" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            The default Location to be used for fulfillment (e.g. the warehouse to
            take deliveries of purchase orders or from which to deliver sales orders). See
            Location definition. Could default from Customer (or Supplier).
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="pricelists" type="priceList--list" nillable="true" minOccurs="0" sme:label="TradingAccount PriceList" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Price list associated with the Trading Account. See PriceList
            definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="pricingOverride" type="priceList--type" nillable="true" minOccurs="0" sme:label="TradingAccount Pricing Override" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Pricing override applicable with the Trading Account (e.g. for
            discounting). See PriceList definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="accountingType" type="tradingAccountAccountingType--enum" nillable="true" minOccurs="0" sme:label="TradingAccount Accounting Type">
				<xs:annotation>
					<xs:documentation>
            Accounting type applicable to the Trading Account (e.g. cash, open
            item, balance forward, etc). Default = Unknown
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="financialAccounts" type="financialAccount--list" nillable="true" minOccurs="0" sme:label="TradingAccount FinancialAccount" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Financial accounting code applicable to the Trading Account (typically
            the revenue account). See FinancialAccount definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="costCentres" type="costCentre--list" nillable="true" minOccurs="0" sme:label="TradingAccount CostCentre" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Cost Centre(s) associated with this Trading Account. See Cost Centre definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="departments" type="department--list" nillable="true" minOccurs="0" sme:label="TradingAccount Departments" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Department(s) associated with this Trading Account. See Department definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="primacyIndicator" type="xs:boolean" nillable="true" minOccurs="0" sme:label="TradingAccount Primacy Indicator">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate if this is the primary Trading Account associated with
            a Customer (or Supplier). Only 1 Trading Account can be the primary for an associated
            Customer (or Supplier).
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="opportunities" type="opportunity--list" nillable="true" minOccurs="0" sme:label="TradingAccount Opportunity" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Opportunities associated with the Trading Account. See Opportunity
            definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="salesQuotations" type="salesQuotation--list" nillable="true" minOccurs="0" sme:label="TradingAccount Sales Quotation" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Sales Quotation(s) associated with the Trading Account. See
            SalesQuotation definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="salesOrders" type="salesOrder--list" nillable="true" minOccurs="0" sme:label="TradingAccount Sales Order" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Sales Order(s) associated with the Trading Account. See SalesOrder
            definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="salesOrderDeliveries" type="salesOrderDelivery--list" nillable="true" minOccurs="0" sme:label="TradingAccount Sales Order Delivery" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Sales Order Delivery(s) associated with the Trading Account. See
            SalesOrderDelivery definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="salesInvoices" type="salesInvoice--list" nillable="true" minOccurs="0" sme:label="TradingAccount Sales Invoice" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Sales Invoice(s) associated with the Trading Account. See
            SalesInvoice definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="salesReturns" type="salesReturn--list" nillable="true" minOccurs="0" sme:label="TradingAccount Sales Return" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Sales Return(s) associated with the Trading Account. See
            SalesReturn definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="salesReturnDeliveries" type="salesReturnDelivery--list" nillable="true" minOccurs="0" sme:label="TradingAccount Sales Return Delivery" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Sales Return Delivery(s) associated with the Trading Account. See
            SalesReturnDelivery definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="salesCredits" type="salesCredit--list" nillable="true" minOccurs="0" sme:label="TradingAccount Sales Credit" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Sales Credit(s) associated with the Trading Account. See
            SalesCredit definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="purchaseRequisitions" type="purchaseRequisition--list" nillable="true" minOccurs="0" sme:label="TradingAccount Purchase Requisition" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Purchase Requisition(s) associated with the Trading Account. See
            PurchaseRequisition definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="purchaseOrders" type="purchaseOrder--list" nillable="true" minOccurs="0" sme:label="TradingAccount Purchase Order" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
