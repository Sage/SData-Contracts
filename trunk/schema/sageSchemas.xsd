	<xs:element name="contactGroup" type="contactGroup--type" sme:role="resourceKind" sme:pluralName="contactGroups" sme:compliance="may" sme:canGet="true"/>
	<xs:complexType name="contactGroup--type">
		<xs:all>
			<xs:element name="active" type="xs:boolean" minOccurs="0" sme:label="ContactGroup Active Flag">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate if the Contact Group is an active one. Default =
            True
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="name" type="xs:normalizedString" minOccurs="0" sme:label="ContactGroup Name" sme:isMandatory="true">
				<xs:annotation>
					<xs:documentation>
