          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="departments" type="department--list" nillable="true" minOccurs="0" sme:label="Payment Departments" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
