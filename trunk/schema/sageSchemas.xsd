			<xs:element name="additionalDiscount1Percent" type="xs:decimal" minOccurs="0" sme:label="Order Additional Discount1 Percent">
				<xs:annotation>
					<xs:documentation>
            Additional discount 1 applicable to the Pricing Document expressed as a percent.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="additionalDiscount2Type" type="discountType--enum" minOccurs="0" sme:label="Order Additional Discount2 Type">
				<xs:annotation>
					<xs:documentation>
