			</xs:element>
			<xs:element name="supplierTradingAccountCurrencyExchangeRateDate" type="xs:date" nillable="true" minOccurs="0" sme:label="PurchaseOrderDelivery Supplier TradingAccount Currency Exchange Rate Date">
				<xs:annotation>
					<xs:documentation>
