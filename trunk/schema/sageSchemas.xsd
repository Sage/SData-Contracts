			<xs:element name="projectLines" type="projectLine--list" nillable="true" minOccurs="0" sme:label="Payment ProjectLine" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Project Line value for this Payment. See ProjectLine definition.
            Payment Line values may be used instead. Some applications may not have Project Lines.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="interactions" type="interaction--list" nillable="true" minOccurs="0" sme:label="Payment Interactions" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Interaction(s) associated with the payment. See Interaction
            definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="notes" type="note--list" nillable="true" minOccurs="0" sme:label="Payment Note" sme:relationship="child" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>Notes associated with the Payment.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<?Extensions for Payment Processing contract?>
			<xs:element name="tenderTransactionType" type="tenderTransactionType--enum" nillable="true" minOccurs="0" sme:label="Payment Tender Transaction Type">
				<xs:annotation>
					<xs:documentation>The type of transaction associated with the receipt tender (e.g. credit card) e.g. credit card receipt, credit card authenticate (only), </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="statusText" type="xs:string" nillable="true" minOccurs="0" sme:label="Payment Status Text">
