			<xs:element name="notes" type="note--list" nillable="true" minOccurs="0" sme:label="PurchaseReturnDelivery Note" sme:relationship="child" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>Notes associated with a Purchase Return Delivery.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
		<xs:anyAttribute/>
