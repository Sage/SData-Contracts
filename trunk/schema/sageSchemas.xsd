      </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="No Change"/>
			<xs:enumeration value="Price Up"/>
			<xs:enumeration value="Price Down"/>
		</xs:restriction>
	</xs:simpleType>
