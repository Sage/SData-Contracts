          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierNetPrice" type="xs:decimal" nillable="true" minOccurs="0" sme:label="PurchaseOrderDelivery Carrier Net Price">
				<xs:annotation>
					<xs:documentation>
            The price of Delivery/carriage. In Delivery currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierTaxPrice" type="xs:decimal" nillable="true" minOccurs="0" sme:label="PurchaseOrderDelivery Carrier Tax Price">
				<xs:annotation>
					<xs:documentation>
            The tax on Delivery/carriage. In Delivery currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierTotalPrice" type="xs:decimal" nillable="true" minOccurs="0" sme:label="PurchaseOrderDelivery Carrier Gross Price">
				<xs:annotation>
					<xs:documentation>
            The total price of Delivery/carriage (net + VAT). In Delivery
            currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierPurchaseInvoice" type="purchaseInvoice--type" nillable="true" minOccurs="0" sme:label="PurchaseOrderDelivery Carrier PurchaseInvoice" sme:relationship="reference" sme:isCollection="false">
