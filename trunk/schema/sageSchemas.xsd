          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="destinationCountry" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="PurchaseReturn Destination Country">
				<xs:annotation>
					<xs:documentation>
