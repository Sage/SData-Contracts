				</xs:annotation>
			</xs:element>
			<xs:element name="financialAccounts" type="financialAccount--list" nillable="true" minOccurs="0" sme:label="PurchaseReturn FinancialAccount" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Financial accounting code applicable to the Purchase Return. See
            FinancialAccount definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="costCentres" type="costCentre--list" nillable="true" minOccurs="0" sme:label="PurchaseReturn CostCentres" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Cost Centre(s) associated with this Purchase Return. See Cost Centre definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="departments" type="department--list" nillable="true" minOccurs="0" sme:label="PurchaseReturn Departments" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Department(s) associated with this Purchase Return. See Department definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="interactions" type="interaction--list" nillable="true" minOccurs="0" sme:label="PurchaseReturn Interactions" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Interaction(s) associated with the purchase return. See Interaction
            definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="notes" type="note--list" nillable="true" minOccurs="0" sme:label="PurchaseReturn Note" sme:relationship="child" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>Notes associated with a Purchase Return.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
		<xs:anyAttribute/>
	</xs:complexType>
	<xs:complexType name="purchaseReturn--list">
		<xs:sequence>
			<xs:element name="purchaseReturn" type="purchaseReturn--type" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:anyAttribute/>
	</xs:complexType>
	<xs:element name="purchaseReturnLine" type="purchaseReturnLine--type" sme:role="resourceKind" sme:pluralName="purchaseReturnLines" sme:compliance="may" sme:canGet="true"/>
	<xs:complexType name="purchaseReturnLine--type">
		<xs:all>
			<xs:element name="purchaseReturn" type="purchaseReturn--type" minOccurs="0" sme:label="PurchaseReturnLine Purchase Return" sme:relationship="parent" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>The parent PurchaseReturn</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="active" type="xs:boolean" minOccurs="0" sme:label="PurchaseReturnLine Active Flag">
