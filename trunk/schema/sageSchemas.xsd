          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference" type="xs:normalizedString" minOccurs="0" sme:label="PurchaseReturnDelivery Reference" sme:isUniqueKey="true">
				<xs:annotation>
					<xs:documentation>Reference for the Purchase Return Delivery.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference2" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="PurchaseReturnDelivery Reference2">
