			<xs:element name="carrierPurchaseInvoices" type="purchaseInvoice--list" nillable="true" minOccurs="0" sme:label="SalesCredit Carrier PurchaseInvoices" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            A link to the purchase Invoice charged by the carrier Company for the
            Delivery to the Customer. See PurchaseInvoice definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierTaxCodes" type="taxCode--list" nillable="true" minOccurs="0" sme:label="SalesCredit Carrier Tax Code" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Tax code governing the tax regime applicable to the carriage on this
            Sales Credit. See TaxCode definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="carrierReference" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="SalesCredit Carrier Reference">
