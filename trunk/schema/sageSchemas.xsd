          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="returnAdditionalDiscount2Amount" type="xs:decimal" nillable="true" minOccurs="0" sme:label="PurchaseReturn Return Additional Discount2 Amount">
				<xs:annotation>
					<xs:documentation>
            Discount applicable to the Purchase Return expressed as an amount.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="returnAdditionalDiscount2Percent" type="xs:decimal" nillable="true" minOccurs="0" sme:label="PurchaseReturn Return Additional Discount2 Percent">
				<xs:annotation>
					<xs:documentation>
            Discount applicable to the Purchase Return expressed as a percent.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="text1" type="xs:string" nillable="true" minOccurs="0" sme:label="PurchaseReturn Text1">
