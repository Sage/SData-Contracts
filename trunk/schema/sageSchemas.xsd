			<xs:element name="reference2" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="PurchaseReturnLine Reference2">
				<xs:annotation>
					<xs:documentation>
            Additional reference for the Purchase Return Line.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="number" type="xs:decimal" minOccurs="0" sme:label="PurchaseReturnLine Number">
				<xs:annotation>
					<xs:documentation>
