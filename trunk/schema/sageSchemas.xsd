				</xs:annotation>
			</xs:element>
			<xs:element name="returnAdditionalDiscount1Amount" type="xs:decimal" nillable="true" minOccurs="0" sme:label="PurchaseReturn Return Additional Discount1 Amount">
				<xs:annotation>
					<xs:documentation>
