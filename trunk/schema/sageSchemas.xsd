          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="requestedDeliveryDate" type="xs:date" nillable="true" minOccurs="0" sme:label="PurchaseReturnDelivery Requested Delivery Date">
				<xs:annotation>
					<xs:documentation>
