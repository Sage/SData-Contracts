            Contact definition. Default = Primary TradingAccount Contact
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="salesPersons" type="salesPerson--list" nillable="true" minOccurs="0" sme:label="SalesQuotation SalesPersons" sme:relationship="association" sme:isCollection="true">
