			<xs:element name="carrierReference" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="SalesXXXLine Carrier Reference">
				<xs:annotation>
					<xs:documentation>
            Reference number as used by the carrier during Delivery, e.g. for Delivery tracking.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="unitOfMeasure" type="unitOfMeasure--type" minOccurs="0" sme:label="SalesQuotationLine UnitOfMeasure" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            A link to the scale to be used in the measurement of physical
            quantities of Commodity. Examples include Each, Box of 10, etc. May also be called
            Pack Size. See UnitOfMeasure definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="quantity" type="xs:decimal" minOccurs="0" sme:label="SalesQuotationLine Quantity" sme:isMandatory="true">
				<xs:annotation>
					<xs:documentation>
            The quantity of the Commodity ordered on the Sales Quotation Line.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="initialPrice" type="xs:decimal" minOccurs="0" sme:label="SalesQuotationLine Initial Price">
				<xs:annotation>
					<xs:documentation>
            The initial price of the Commodity ordered on the Sales Quotation Line
            (e.g. before discounts). In order currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="pricelist" type="priceList--type" nillable="true" minOccurs="0" sme:label="SalesQuotationLine PriceList" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            A reference to the Price List used to derive the price on the Sales
            Quotation Line. See PriceList definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="postalAddresses" type="postalAddress--list" nillable="true" minOccurs="0" sme:label="SalesQuotationLine PostalAddress" sme:relationship="child" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Link to Postal Address(es) associated with the Sales Quotation Line
            (ship to).
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderLineDiscountType" type="discountType--enum" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Order Line Discount Type">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate the type of discount applicable to this Sales
            Quotation Line (percent or amount). From the Trading Account.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderLineDiscountAmount" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Order Line Discount Amount">
				<xs:annotation>
					<xs:documentation>
            Discount applicable to the Sales Quotation Line, expressed as an
            amount. From the Trading Account.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderLineDiscountPercent" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Order Line Discount Percent">
				<xs:annotation>
					<xs:documentation>
            Discount applicable to the Sales Quotation Line, expressed as a
            percentage. From the Trading Account.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderLineSubtotalDiscountType" type="discountType--enum" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Order Line Subtotal Discount Type">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate the type of discount applicable to a subtotal of Sales
            Quotation lines (percent or amount) of which this Line is part.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderLineSubtotalDiscountAmount" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Order Line Subtotal Discount Amount">
				<xs:annotation>
					<xs:documentation>
            Discount applicable to the Sales Quotation Line, expressed as an
            amount.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderLineSubtotalDiscountPercent" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Order Line Subtotal Discount Percent">
				<xs:annotation>
					<xs:documentation>
            Discount applicable to the Sales Quotation Line, expressed as a
            percentage.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="actualPrice" type="xs:decimal" minOccurs="0" sme:label="SalesQuotationLine Actual Price">
				<xs:annotation>
					<xs:documentation>
            The actual price of the Commodity ordered on the Sales Quotation Line
            (e.g. after discounts or adjustments). In Quotation currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="netTotal" type="xs:decimal" minOccurs="0" sme:label="SalesQuotationLine Net Total">
				<xs:annotation>
					<xs:documentation>
            Total value of the Sales Quotation Line, including discounts, excluding
            tax (e.g. quantity * actual price). In Quotation currency. Could be calculated.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="chargesTotal" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Charges Total">
				<xs:annotation>
					<xs:documentation>
            Total value of any (sur)charges applied to the Sales Quotation Line
            (e.g. finance charges, interest). In Quotation currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="discountTotal" type="xs:decimal" minOccurs="0" sme:label="SalesQuotationLine Discount Total">
				<xs:annotation>
					<xs:documentation>
            Total discounts applied to the quotation line. In quotation currency
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxCodes" type="taxCode--list" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Tax Code" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Tax code governing the tax regime applicable to this Sales Quotation
            Line (default value from Sales Quotation). See TaxCode definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="priceTax" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Price Tax">
				<xs:annotation>
					<xs:documentation>
            Tax applicable to the Sales Quotation Line price, e.g. the per unit tax.
            Both unit-tax and Line-tax values are required due to the way tax is handled
            differently by product/country.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxTotal" type="xs:decimal" minOccurs="0" sme:label="SalesQuotationLine Tax Total">
				<xs:annotation>
					<xs:documentation>
            Tax applicable to the Sales Quotation Line. In Quotation currency.
            Could be calculated. Both unit-tax and Line-tax values are required due to the way tax
            is handled differently by product/country.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="grossTotal" type="xs:decimal" minOccurs="0" sme:label="SalesQuotationLine Gross Total">
				<xs:annotation>
					<xs:documentation>
            Total value of the Sales Quotation Line, including discounts &amp;
            tax &amp; charges. In Quotation currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="costTotal" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Cost Total">
				<xs:annotation>
					<xs:documentation>
            Total cost of the Sales Quotation Line. In Quotation currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="profitTotal" type="xs:decimal" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Profit Total">
				<xs:annotation>
					<xs:documentation>
            Total profit on the Sales Quotation Line (e.g. net total - cost total).
            In Quotation currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="buyerContact" type="contact--type" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Buyer Contact" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Link to the Sales Quotation Line originator (buyer Contact). See
            Contact definition. This could be derived from the Sales Quotation
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="project" type="project--type" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Project" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Identifier of the Project this Sales Quotation Line is for. See
            Project definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="projectLine" type="projectLine--type" nillable="true" minOccurs="0" sme:label="SalesQuotationLine ProjectLine" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Project Line value for this Sales Quotation Line. See Project Line
            definition. Some applications may not have Project Lines.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="bomLine" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="SalesQuotationLine BOM Line">
				<xs:annotation>
					<xs:documentation>
            BOM Line associated with the Sales Quotation Line. The BOM Line
            resource will be defined in the future. Not all applications support this feature.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="financialAccounts" type="financialAccount--list" nillable="true" minOccurs="0" sme:label="SalesQuotationLine FinancialAccount" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Financial accounting code applicable to the Sales Quotation Line. See
            FinancialAccount definition. This could be derived from the Sales Quotation
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="costCentres" type="costCentre--list" nillable="true" minOccurs="0" sme:label="SalesQuotationLine CostCentres" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Cost Centre(s) associated with this Sales Quotation Line. See Cost Centre definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="departments" type="department--list" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Departments" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Department(s) associated with this Sales Quotation Line. See Department definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="notes" type="note--list" nillable="true" minOccurs="0" sme:label="SalesQuotationLine Note" sme:relationship="child" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>Notes associated with a Sales Quotation Line.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
		<xs:anyAttribute/>
	</xs:complexType>
	<xs:complexType name="salesQuotationLine--list">
		<xs:sequence>
			<xs:element name="salesQuotationLine" type="salesQuotationLine--type" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:anyAttribute/>
	</xs:complexType>
	<xs:element name="commodity" type="commodity--type" sme:role="resourceKind" sme:pluralName="commodities" sme:compliance="must" sme:canGet="true" sme:hasUuid="true" sme:isSyncSource="true" sme:syncOrder="120"/>
	<xs:complexType name="commodity--type">
		<xs:all>
			<xs:element name="active" type="xs:boolean" minOccurs="0" sme:label="Commodity Active Flag">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate if the Commodity is an active one. Default = True
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="commodityGroup" type="commodityGroup--type" minOccurs="0" sme:label="Commodity CommodityGroup" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Identifier of the Commodity group this Commodity belongs to. See
            CommodityGroup definition. Commodities can only belong to one Commodity Group.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="commodityIdentifierType" type="commodityCommodityIdentifierType--enum" nillable="true" minOccurs="0" sme:label="Commodity CommodityIdentifier Type">
				<xs:annotation>
					<xs:documentation>
            Type of the Commodity Identifier associated with this Commodity (if
            any) (e.g. Total, Batch (lot), Serial Number, Serial from
            Batch/Lot, Unknown). Default = Total
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="commodityIdentifiers" type="commodityIdentifier--list" nillable="true" minOccurs="0" sme:label="Commodity CommodityIdentifier" sme:relationship="child" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Link to the Commodity Identifier(s) for this Commodity, if it is traced
            to that level of detail. See CommodityIdentifier definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference" type="xs:normalizedString" minOccurs="0" sme:label="Commodity Reference" sme:isUniqueKey="true">
				<xs:annotation>
					<xs:documentation>
            Reference for the Commodity (typically the one used by the buying
            organization).
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference2" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Commodity Reference2">
				<xs:annotation>
					<xs:documentation>Another reference for the Commodity.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="status" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Commodity Status">
				<xs:annotation>
					<xs:documentation>Status of the Commodity.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="name" type="xs:normalizedString" minOccurs="0" sme:label="Commodity Name" sme:isMandatory="true">
				<xs:annotation>
					<xs:documentation>Short name for the Commodity.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="shortName" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Commodity Short Name">
				<xs:annotation>
					<xs:documentation>A shorter name for the Commodity.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="description" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Commodity Description">
				<xs:annotation>
					<xs:documentation>The Commodity description.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="alternativeDescription1" type="xs:string" nillable="true" minOccurs="0" sme:label="Commodity Alternative Description1">
				<xs:annotation>
					<xs:documentation>
            An alternative Commodity description (e.g. buying or selling
            descriptions may be different).
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="alternativeDescription2" type="xs:string" nillable="true" minOccurs="0" sme:label="Commodity Alternative Description2">
				<xs:annotation>
					<xs:documentation>
            Another alternative Commodity description (e.g. could be used for
            special Delivery instructions, handling instructions, or alternative language
            descriptions, etc).
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="type" type="commodityType--enum" minOccurs="0" sme:label="Commodity Type" sme:isMandatory="true">
				<xs:annotation>
					<xs:documentation>
            The Commodity type (e.g. product, service). Default value should come
            from CommodityGroup. Product Stock (e.g. finished goods and/or raw materials) Product
            Non Stock (e.g. Misc or expense). Service (contracts). Bill Of Material (Manufactured
            Item) (e.g. assembled Kit). (Built to Order) Kit. Subcontract Description (e.g.
            comment). User Defined. Unknown. Default = Unknown
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="unitOfMeasure" type="unitOfMeasure--type" minOccurs="0" sme:label="Commodity UnitOfMeasure" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Link to the Unit of Measure (pack size / quantity) for the Commodity
            (e.g. each, six-pack, case, etc). Also known as stocking Unit of Measure. See
            UnitOfMeasure definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="buyingAllowedFlag" type="xs:boolean" nillable="true" minOccurs="0" sme:label="Commodity Buying Allowed Flag">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate that this Commodity can be bought i.e. can be used
            on purchase orders/requisitions. Default = Yes
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="buyingUnitOfMeasure" type="unitOfMeasure--type" nillable="true" minOccurs="0" sme:label="Commodity Buying UnitOfMeasure" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Link to the Unit of Measure (pack size / quantity) for the Commodity
            used during buying (e.g. each, six-pack, case, etc). See UnitOfMeasure definition.
            Default = Commodity UnitOfMeasure
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="buyingUnitOfMeasureNumber" type="xs:decimal" nillable="true" minOccurs="0" sme:label="Commodity Buying UnitOfMeasure Number">
				<xs:annotation>
					<xs:documentation>
            The number (quantity) of items in a pack of the Commodity (e.g. 1, 6,
            24, etc). Default = 1
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="sellingAllowedFlag" type="xs:boolean" nillable="true" minOccurs="0" sme:label="Commodity Selling Allowed Flag">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate that this Commodity can be sold i.e. can be used on
            sales orders/quotations. Default = Yes
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="sellingUnitOfMeasure" type="unitOfMeasure--type" nillable="true" minOccurs="0" sme:label="Commodity Selling UnitOfMeasure" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Link to the Unit of Measure (pack size / quantity) for the Commodity
            used during selling/retail (e.g. each, six-pack, case, etc). See UnitOfMeasure
            definition. Default = Commodity UnitOfMeasure
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="sellingUnitOfMeasureNumber" type="xs:decimal" nillable="true" minOccurs="0" sme:label="Commodity Selling UnitOfMeasure Number">
				<xs:annotation>
					<xs:documentation>
            The number (quantity) of items in a pack of the Commodity (e.g. 1, 6,
            24, etc). 1
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="packingUnitOfMeasure" type="unitOfMeasure--type" nillable="true" minOccurs="0" sme:label="Commodity Packing UnitOfMeasure" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Link to the Unit of Measure (pack size / quantity) for the Commodity
            used during packing/dispatch (e.g. each, six-pack, case, etc). See UnitOfMeasure
            definition. This could be the default or buying Unit of Measure.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="packingUnitOfMeasureNumber" type="xs:decimal" nillable="true" minOccurs="0" sme:label="Commodity Packing UnitOfMeasure Number">
				<xs:annotation>
					<xs:documentation>
            The number (quantity) of items in a pack of the Commodity (e.g. 1, 6,
            24, etc).
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="catalogueNumber" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Commodity Catalogue Number">
				<xs:annotation>
					<xs:documentation>
            Reference for the Commodity in a Supplier catalogue.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="supplierTradingAccounts" type="tradingAccount--list" nillable="true" minOccurs="0" sme:label="Commodity Supplier TradingAccounts" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Link to the Commodity Supplier trading account. See TradingAccount
            definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="supplierReference" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Commodity Supplier Reference">
				<xs:annotation>
					<xs:documentation>
            Reference for the Commodity as used by the Supplier.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="manufacturerCompany" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Commodity Manufacturer Company">
				<xs:annotation>
					<xs:documentation>
            Link to the Company associated with manufacturing the Commodity. See
            Company definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="manufacturerReference" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Commodity Manufacturer Reference">
				<xs:annotation>
					<xs:documentation>
            Reference for the Commodity as used by the manufacturer.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="material" type="xs:string" nillable="true" minOccurs="0" sme:label="Commodity Material">
				<xs:annotation>
					<xs:documentation>Material from which the Commodity is made.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="countryOfOrigin" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Commodity Country Of Origin">
				<xs:annotation>
					<xs:documentation>
            The country where the Commodity originated. ISO 3166-1 alpha-2
            country codes only Could default from CommodityGroup.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="weight" type="xs:decimal" nillable="true" minOccurs="0" sme:label="Commodity Weight">
				<xs:annotation>
					<xs:documentation>
            Weight of the Commodity. In system default weight unit (metric or
            imperial, tonnes, quintals, etc).
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="weightKg" type="xs:decimal" nillable="true" minOccurs="0" sme:label="Commodity Weight KG">
				<xs:annotation>
					<xs:documentation>
            Weight of the Commodity in SI units (Kilograms). In kilograms - may
            be different to the standard weight.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="variants" type="commodityVariant--list" nillable="true" minOccurs="0" sme:label="Commodity Variants" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            The Variant of the Commodity (e.g. colour or size). See Variant
            definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dimensions" type="commodityDimension--list" nillable="true" minOccurs="0" sme:label="Commodity Dimensions" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            The physical Dimension of the Commodity (e.g. height). See
            Dimension definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="classification" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Commodity Classification">
				<xs:annotation>
					<xs:documentation>
            Classification of the Commodity. Based on standard classification
            system (Intrastat in EU)
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxCodes" type="taxCode--list" nillable="true" minOccurs="0" sme:label="Commodity Tax Code" sme:relationship="association" sme:isCollection="true">
