            True
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference" type="xs:normalizedString" minOccurs="0" sme:label="PaymentLine Reference" sme:isUniqueKey="true">
				<xs:annotation>
					<xs:documentation>
            Reference for the Payment Line (e.g. check or BACS number).
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference2" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="PaymentLine Reference2">
