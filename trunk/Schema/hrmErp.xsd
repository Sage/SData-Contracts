<?xml version="1.0" encoding="utf-8"?>
<?xml-stylesheet type="text/xsl" href="../Tools/XsdToHtml.xslt"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:sme="http://schemas.sage.com/sdata/sme/2007" xmlns="http://schemas.sage.com/hrmErp/2009" xmlns:hrmErp="http://schemas.sage.com/hrmErp/2009" xmlns:sc="http://schemas.sage.com/sc/2009" xmlns:crmErp="http://schemas.sage.com/crmErp/2008" xmlns:famErp="http://schemas.sage.com/famErp/2009" targetNamespace="http://schemas.sage.com/hrmErp/2009" elementFormDefault="qualified" version="1.1.2" id="hrmErp">
	<xs:import namespace="http://schemas.sage.com/sc/2009" schemaLocation="common.xsd"/>
	<xs:import namespace="http://schemas.sage.com/crmErp/2008" schemaLocation="crmErp.xsd"/>
	<xs:import namespace="http://schemas.sage.com/famErp/2009" schemaLocation="famErp.xsd"/>
	<xs:element name="organization" type="organization--type" sme:role="resourceKind" sme:pluralName="organizations" sme:compliance="may" sme:canGet="true" sme:canPut="true" sme:canPost="true" sme:canDelete="true"/>
	<xs:complexType name="organization--type">
		<xs:all>
			<xs:element name="active" type="xs:boolean" minOccurs="0" sme:label="Organization Active Flag">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate if the Organization is an active Organization .
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operatingCompany" type="sc:operatingCompany--type" minOccurs="0" sme:label="Organization OperatingCompany" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Link to the OperatingCompany associated with the Organization.
             
            See OperatingCompany definition Company where type = Operating Company.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference" type="xs:normalizedString" minOccurs="0" sme:label="Organization Reference" sme:isUniqueKey="true">
				<xs:annotation>
					<xs:documentation>
            Reference for the Organization.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference2" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Organization Reference2">
				<xs:annotation>
					<xs:documentation>
            Additional reference for the Organization.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="status" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Organization Status">
				<xs:annotation>
					<xs:documentation>
            Status of the Organization.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="name" type="xs:normalizedString" minOccurs="0" sme:label="Organization Name" sme:isMandatory="true">
				<xs:annotation>
					<xs:documentation>
            The Organization name e.g. Career Family, Research and Development, Sales
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="type" type="xs:normalizedString" minOccurs="0" sme:label="Organization Type">
				<xs:annotation>
					<xs:documentation>
            The Organization type e.g. Job Family, Unit
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="positions" type="position--list" nillable="true" minOccurs="0" sme:label="Organization Position" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Link to the Positions associated with the Organization.
             
            See Position definition.

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="levelNumber" type="xs:decimal" minOccurs="0" sme:label="Organization Level Number" sme:isMandatory="true">
				<xs:annotation>
					<xs:documentation>
            The level associated with the Position within the Organization in numeric format e.g. 1-15 in the Sage UK Career Framework or Sorting Level of Organizational Units.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="levelText" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Organization Level Text">
				<xs:annotation>
					<xs:documentation>
            The level associated with the Position within the Organization – in textual format.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="bandNumber" type="xs:decimal" nillable="true" minOccurs="0" sme:label="Organization Band Number">
				<xs:annotation>
					<xs:documentation>
            The band associated with the Position within the Organization in numeric format.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="bandText" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Organization Band Text">
				<xs:annotation>
					<xs:documentation>
            The band associated with the Position within the Organization – in textual format e.g. A-D in the Sage UK Career Framework.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="associatedParentOrganization" type="organization--type" nillable="true" minOccurs="0" sme:label="Organization Associated Parent Organization" sme:relationship="parent" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Link to Parent Organization associated with this Organization e.g. into which an Employee would be promoted.
             
            Organization are usually hierarchical to represent a career path.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="associatedChildOrganizations" type="organization--list" nillable="true" minOccurs="0" sme:label="Organization Associated Child Organization" sme:relationship="child" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Link to Child Organization associated with this Organization e.g. from which an Employee has been promoted or could be demoted.
             
            Organization are usually hierarchical to represent a career path.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="postalAddresses" type="sc:postalAddress--list" nillable="true" minOccurs="0" sme:label="Organization PostalAddresses" sme:relationship="child" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Link to the main Organization addresses
             
            See PostalAddress definition.

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="phones" type="sc:phoneNumber--list" nillable="true" minOccurs="0" sme:label="Organization Phones" sme:relationship="child" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Organization telephone numbers.
             
            See PhoneNumber definition.

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="defaultWorkplace" type="workplace--type" nillable="true" minOccurs="0" sme:label="Organization Default Workplace" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            The default Workplace associated with the Organization.
             
            See Workplace definition.
            .
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="financialAccounts" type="financialAccount--list" nillable="true" minOccurs="0" sme:label="Organization FinancialAccount" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Financial e.g. cost centre account associated with the Organization.
             
            See FinancialAccount definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="costCentres" type="sc:costCentre--list" nillable="true" minOccurs="0" sme:label="Organization CostCentre" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation/>
				</xs:annotation>
			</xs:element>
			<xs:element name="departments" type="sc:department--list" nillable="true" minOccurs="0" sme:label="Organization Department" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation/>
				</xs:annotation>
			</xs:element>
			<xs:element name="text1" type="xs:string" nillable="true" minOccurs="0" sme:label="Organization Text1">
				<xs:annotation>
					<xs:documentation>
            Textual information associated with the Organization.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="text2" type="xs:string" nillable="true" minOccurs="0" sme:label="Organization Text2">
				<xs:annotation>
					<xs:documentation>
            Textual information associated with the Organization.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="startDate" type="xs:date" nillable="true" minOccurs="0" sme:label="Organization Start Date">
				<xs:annotation>
					<xs:documentation>
            A start date associated with the Organization if applicable.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="endDate" type="xs:date" nillable="true" minOccurs="0" sme:label="Organization End Date">
				<xs:annotation>
					<xs:documentation>
            A start date associated with the Organization if applicable – e,g, to support restructuring of an Organization.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="notes" type="sc:note--list" nillable="true" minOccurs="0" sme:label="Organization Note" sme:relationship="child" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Notes associated with an Organization.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
		<xs:anyAttribute/>
	</xs:complexType>
	<xs:complexType name="organization--list">
		<xs:sequence minOccurs="0" maxOccurs="unbounded">
			<xs:element name="organization" type="organization--type"/>
		</xs:sequence>
		<xs:anyAttribute/>
	</xs:complexType>
	<xs:element name="position" type="position--type" sme:role="resourceKind" sme:pluralName="positions" sme:compliance="may" sme:canGet="true" sme:canPut="true" sme:canPost="true" sme:canDelete="true"/>
	<xs:complexType name="position--type">
		<xs:all>
			<xs:element name="active" type="xs:boolean" minOccurs="0" sme:label="Position Active Flag">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate if the Position is an active Position.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operatingCompany" type="sc:operatingCompany--type" minOccurs="0" sme:label="Position OperatingCompany" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Link to the OperatingCompany associated with the Position.
             
            See OperatingCompany definition Company where type = Operating Company.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="department" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Position Department">
				<xs:annotation>
					<xs:documentation>
            Department within the Operating Company the Position is for.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="organization" type="organization--type" nillable="true" minOccurs="0" sme:label="Position Organization" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Link to the Organization associated with this Position.
             
            See Organization definition.

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference" type="xs:normalizedString" minOccurs="0" sme:label="Position Reference" sme:isUniqueKey="true">
				<xs:annotation>
					<xs:documentation>
            Reference for the Position e.g. JOB001.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference2" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Position Reference2">
				<xs:annotation>
					<xs:documentation>
            Additional reference for the Position.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="status" type="positionStatus--enum" minOccurs="0" sme:label="Position Status">
				<xs:annotation>
					<xs:documentation>
            Status of the Position
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="name" type="xs:normalizedString" minOccurs="0" sme:label="Position Name" sme:isMandatory="true">
				<xs:annotation>
					<xs:documentation>
            The Position name typically the Job name, e.g. Product Development Manager
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="shortName" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Position Short Name">
				<xs:annotation>
					<xs:documentation>
            A shorter name for the Position e.g. PDM.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="type" type="positionType--enum" minOccurs="0" sme:label="Position Type">
				<xs:annotation>
					<xs:documentation>
            The Position type e.g. Permanent, Part-Time, etc.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="employment" type="employment--type" nillable="true" minOccurs="0" sme:label="Position Employment" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Employment contract associated with the Position. This may be prospective terms if the Position is Vacant, or actual terms if the Position is filled.
             
            See Employment definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="workplace" type="workplace--type" nillable="true" minOccurs="0" sme:label="Position Workplace" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            The Workplace associated with the Position.
             
            See Workplace definition.
            .
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="postalAddresses" type="sc:postalAddress--list" nillable="true" minOccurs="0" sme:label="Position PostalAddresses" sme:relationship="child" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Link to the current Position addresses
             
            See PostalAddress definition.

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="phones" type="sc:phoneNumber--list" nillable="true" minOccurs="0" sme:label="Position Phones" sme:relationship="child" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Position telephone numbers.
             
            See PhoneNumber definition.

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="emails" type="sc:email--list" nillable="true" minOccurs="0" sme:label="Position Emails" sme:relationship="child" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Position email addresses.
             
            See Email definition.

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="employees" type="employee--list" nillable="true" minOccurs="0" sme:label="Position Employee" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Link to the Employees associated with the Position if any – may be Vacant.
             
            See Employee definition.

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="managerEmployee" type="employee--type" nillable="true" minOccurs="0" sme:label="Position Manager Employee" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            A link to managers Employee record.
             
            See Employee definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="text1" type="xs:string" nillable="true" minOccurs="0" sme:label="Position Text1">
				<xs:annotation>
					<xs:documentation>
            Textual information associated with the Position.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="text2" type="xs:string" nillable="true" minOccurs="0" sme:label="Position Text2">
				<xs:annotation>
					<xs:documentation>
            Textual information associated with the Position.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="startedDate" type="xs:date" nillable="true" minOccurs="0" sme:label="Position Started Date">
				<xs:annotation>
					<xs:documentation>
            Date the Position was or is to be originated e.g. Secondment.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="endedDate" type="xs:date" nillable="true" minOccurs="0" sme:label="Position Ended Date">
				<xs:annotation>
					<xs:documentation>
            Date the Position was or is to be closed.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="competenciesRequired" type="competency--list" nillable="true" minOccurs="0" sme:label="Position Competencies Required" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            A link to the Competencies required of the Position.
             
            See Competency definition.

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="primacyIndicator" type="xs:boolean" nillable="true" minOccurs="0" sme:label="Position Primacy Indicator">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate if this Position is the primary one e.g. for the associated Employee.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="externalPositionFlag" type="xs:boolean" nillable="true" minOccurs="0" sme:label="Position External Position Flag">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate if this position is held externally to the employing company e.g. where a record is being kept of voluntary, governmental, supplementary positions.
             
            True = External position

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="notes" type="sc:note--list" nillable="true" minOccurs="0" sme:label="Position Note" sme:relationship="child" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Notes associated with a Position.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
		<xs:anyAttribute/>
	</xs:complexType>
	<xs:complexType name="position--list">
		<xs:sequence minOccurs="0" maxOccurs="unbounded">
			<xs:element name="position" type="position--type"/>
		</xs:sequence>
		<xs:anyAttribute/>
	</xs:complexType>
	<xs:element name="vacancy" type="vacancy--type" sme:role="resourceKind" sme:pluralName="vacancies" sme:compliance="may" sme:canGet="true" sme:canPut="true" sme:canPost="true" sme:canDelete="true"/>
	<xs:complexType name="vacancy--type">
		<xs:all>
			<xs:element name="active" type="xs:boolean" minOccurs="0" sme:label="Vacancy Active Flag">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate if the Vacancy is an active Vacancy.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operatingCompany" type="sc:operatingCompany--type" minOccurs="0" sme:label="Vacancy OperatingCompany" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Link to the OperatingCompany associated with the Vacancy.
             
            See OperatingCompany definition Company where type = Operating Company.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="organization" type="organization--type" nillable="true" minOccurs="0" sme:label="Vacancy Organization" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Link to the Organization associated with this Vacancy.
             
            See Organization definition.

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="position" type="position--type" nillable="true" minOccurs="0" sme:label="Vacancy Position" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Link to the Position the Vacancy is for.
             
            See Position definition.

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference" type="xs:normalizedString" minOccurs="0" sme:label="Vacancy Reference" sme:isUniqueKey="true">
				<xs:annotation>
					<xs:documentation>
            Reference for the Vacancy e.g. 000456.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference2" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Vacancy Reference2">
				<xs:annotation>
					<xs:documentation>
            Additional reference for the Vacancy.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="status" type="vacancyStatus--enum" minOccurs="0" sme:label="Vacancy Status">
				<xs:annotation>
					<xs:documentation>
            Status of the Vacancy
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="name" type="xs:normalizedString" minOccurs="0" sme:label="Vacancy Name" sme:isMandatory="true">
				<xs:annotation>
					<xs:documentation>
            The Vacancy name typically the Job name, e.g. Product Development Manager
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="shortName" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Vacancy Short Name">
				<xs:annotation>
					<xs:documentation>
            A shorter name for the Vacancy e.g. PDM.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="type" type="vacancyType--enum" minOccurs="0" sme:label="Vacancy Type">
				<xs:annotation>
					<xs:documentation>
            The Vacancy type e.g. Permanent, Part-Time, etc.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="positionsAvailable" type="xs:decimal" nillable="true" minOccurs="0" sme:label="Vacancy Positions Available" sme:isMandatory="true">
				<xs:annotation>
					<xs:documentation>
            A numeric value indicating the number of Vacancy Positions that are available.
             
            Default = 1
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="createdDate" type="xs:date" nillable="true" minOccurs="0" sme:label="Vacancy Created Date">
				<xs:annotation>
					<xs:documentation>
            Date the Vacancy was originated.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="targetStartDate" type="xs:date" nillable="true" minOccurs="0" sme:label="Vacancy Target Start Date">
				<xs:annotation>
					<xs:documentation>
            Target start date for the Vacancy i.e. when Employee expected to start Employment.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="applicationClosingDate" type="xs:date" nillable="true" minOccurs="0" sme:label="Vacancy Application Closing Date">
				<xs:annotation>
					<xs:documentation>
            Application closing date, after which no new Candidates will be accepted.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="applicationMethod" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Vacancy Application Method">
				<xs:annotation>
					<xs:documentation>
            Value indicating the preferred application method for the Vacancy e.g. Web, Post, Email, Agency, etc.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="assessmentStartingDate" type="xs:date" nillable="true" minOccurs="0" sme:label="Vacancy Assessment Starting Date">
				<xs:annotation>
					<xs:documentation>
            Date when Candidates will be assessed e.g. when interviews, etc commence
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="assessingEmployees" type="employee--list" nillable="true" minOccurs="0" sme:label="Vacancy Assessing Employee" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Details of the Employee doing the Candidate assessment. Could be many as this could be a recruiting panel.
             
