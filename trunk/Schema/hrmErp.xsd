          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="entryUser" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Payroll Entry User">
				<xs:annotation>
					<xs:documentation>
