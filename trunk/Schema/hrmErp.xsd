			<xs:element name="workType" type="employmentWorkType--enum" nillable="true" minOccurs="0" sme:label="Employment Work Type">
				<xs:annotation>
					<xs:documentation>
            The type of work
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="contractCategory" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Employment Contract Category ">
				<xs:annotation>
					<xs:documentation>
            The legal category of the contract, as agreed with the relevant authorities e.g. cadre in France.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="contractSubCategory" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Employment Contract Sub-Category ">
				<xs:annotation>
					<xs:documentation>
            The legal sub category of the contract, as agreed with the relevant authorities e.g. cadre in France.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="acceptedDate" type="xs:date" nillable="true" minOccurs="0" sme:label="Employment Accepted Date">
				<xs:annotation>
					<xs:documentation>
            The date the Employment offer was accepted.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reason" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Employment Reason">
				<xs:annotation>
					<xs:documentation>
            Reason for Employment, e.g., New Hire, Secondment, Maternity Cover.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="justification" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Employment Justification">
				<xs:annotation>
					<xs:documentation>
            For some types of Employment contract it may be necessary to justify the type of contract e.g. if contract type = CDD in France, then this will be mandatory.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="duration" type="xs:decimal" nillable="true" minOccurs="0" sme:label="Employment Duration">
				<xs:annotation>
					<xs:documentation>
            The duration of the contract, as expressed in duration units e.g. 6 months.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="durationUnit" type="employmentDurationUnit--enum" nillable="true" minOccurs="0" sme:label="Employment Duration Unit">
				<xs:annotation>
					<xs:documentation>
            The unit of duration associated with the Employment contract e.g. months.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="startedDate" type="xs:date" nillable="true" minOccurs="0" sme:label="Employment Started Date">
				<xs:annotation>
					<xs:documentation>
            Date the Employment was originated.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="noticePeriod" type="xs:decimal" nillable="true" minOccurs="0" sme:label="Employment Notice Period">
				<xs:annotation>
					<xs:documentation>
            Period of notice expected when terminating the Employment contract.
             
            In Employment Notice Unit
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="noticeUnit" type="employmentNoticeUnit--enum" nillable="true" minOccurs="0" sme:label="Employment Notice Unit">
				<xs:annotation>
					<xs:documentation>
            Time unit of measure applicable to the Employments notice period typically weeks or months.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="employerNoticePeriod" type="xs:decimal" nillable="true" minOccurs="0" sme:label="Employment Employer Notice Period">
				<xs:annotation>
					<xs:documentation>
            Period of notice expected from an Employer when terminating the Employment contract.
             
            In Employer Notice Unit
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="employerNoticeUnit" type="employmentEmployerNoticeUnit--enum" nillable="true" minOccurs="0" sme:label="Employment Employer Notice Unit">
				<xs:annotation>
					<xs:documentation>
            Time unit of measure applicable to the Employers notice period typically weeks or months.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="noticeGivenBy" type="employmentNoticeGivenBy--enum" nillable="true" minOccurs="0" sme:label="Employment Notice Given By">
				<xs:annotation>
					<xs:documentation>
            Flag indicating who is giving notice of leaving – Employee or Employer.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="noticeGivenDate" type="xs:date" nillable="true" minOccurs="0" sme:label="Employment Notice Given Date">
				<xs:annotation>
					<xs:documentation>
            Date notice was given.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="noticeType" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Employment Notice Type">
				<xs:annotation>
					<xs:documentation>
            The type of notice given by the Employment. This could be something decided by the Employment, the Employer, or through mutual agreement.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="probationPeriod" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Employment Probation Period">
				<xs:annotation>
					<xs:documentation>
            Period of probation given to an Employee when taking the Employment contract typically applies to new Employments.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="probationUnit" type="employmentProbationUnit--enum" nillable="true" minOccurs="0" sme:label="Employment Probation Unit">
				<xs:annotation>
					<xs:documentation>
            Time unit of measure applicable to the Employments probation period typically weeks or months.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="probationEndDate" type="xs:date" nillable="true" minOccurs="0" sme:label="Employment Probation End Date">
				<xs:annotation>
					<xs:documentation>
            Date a period of probation will expire or require review if applicable.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="probationRenewed" type="xs:boolean" nillable="true" minOccurs="0" sme:label="Employment Probation Renewed Flag">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate if probation has been renewed.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="endedDate" type="xs:date" nillable="true" minOccurs="0" sme:label="Employment Ended Date">
				<xs:annotation>
					<xs:documentation>
            Date the Employment is to be closed.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="endedReason" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Employment Ended Reason">
				<xs:annotation>
					<xs:documentation>
            Reason for Employment termination e.g. Resignation, Dismissal, End of Contract. This is different from the Employee Leave Reason on the Employee record as a particular instance of Employment may end but another one for the same Employee may commence.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="endedFlag" type="xs:boolean" nillable="true" minOccurs="0" sme:label="Employment Ended Flag">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate if the Employment has in fact ended.
             
            Default = False
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="workplaces" type="workplace--list" nillable="true" minOccurs="0" sme:label="Employment Workplace" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            The Workplace associated with the Employment.
             
            See Workplace definition.
            .
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="phones" type="sc:phoneNumber--list" nillable="true" minOccurs="0" sme:label="Employment Phones" sme:relationship="child" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Employment telephone numbers e.g. office, works mobile, etc
             
            See PhoneNumber definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="emails" type="sc:email--list" nillable="true" minOccurs="0" sme:label="Employment Emails" sme:relationship="child" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Employment email addresses e.g. work email.
             
            See Email definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="jobTitle" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Employment Job Title">
				<xs:annotation>
					<xs:documentation>
            Job Title associated with the Employment.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="jobType" type="employmentJobType--enum" nillable="true" minOccurs="0" sme:label="Employment Job Type">
				<xs:annotation>
					<xs:documentation>
            Job type relevant to the Job Title e.g. Manager, Supervisor, etc
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="jobQualificationLevel" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Employment Job Qualification Level">
				<xs:annotation>
					<xs:documentation>
            Job qualification level relevant to the job e.g. manual, skilled, technical, etc.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="hazardousIndicator" type="xs:boolean" nillable="true" minOccurs="0" sme:label="Employment Hazardous Indicator">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate if the Employment is hazardous e.g. working in radioactive, dangerous situations.
             
            Default = False
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="hazardousText" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Employment Hazardous text">
				<xs:annotation>
					<xs:documentation>
            If the Employment is hazardous then this allows for a description of the hazard.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="text1" type="xs:string" nillable="true" minOccurs="0" sme:label="Employment Text1">
				<xs:annotation>
					<xs:documentation>
            Textual information associated with the Employment.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="text2" type="xs:string" nillable="true" minOccurs="0" sme:label="Employment Text2">
				<xs:annotation>
					<xs:documentation>
            Textual information associated with the Employment.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="workTimes" type="workTime--list" nillable="true" minOccurs="0" sme:label="Employment Work Times" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Working times applicable to the Employment.
             
            See WorkTime definition.

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="benefits" type="benefit--list" nillable="true" minOccurs="0" sme:label="Employment Benefit" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Benefits applicable to the Employment.
             
            See Benefit definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="payrollType" type="employmentPayrollType--enum" nillable="true" minOccurs="0" sme:label="Employment Payroll Type">
				<xs:annotation>
					<xs:documentation>
            The type of payroll that will be used/associated with the Employment contract.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="payrollPaymentMethod" type="employmentPayrollPaymentMethod--enum" minOccurs="0" sme:label="Employment Payroll Payment Method">
				<xs:annotation>
					<xs:documentation>
            The payment method used in Payrolls associated with this Employment.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="payrollPayFrequency" type="employmentPayrollPayFrequency--enum" minOccurs="0" sme:label="Employment Payroll Pay Frequency">
				<xs:annotation>
					<xs:documentation>
            Frequency with which payments are made in association with this Employment there will usually be one Payroll entry per pay frequency.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="payrollTaxCodes" type="taxCode--list" nillable="true" minOccurs="0" sme:label="Employment Payroll Tax Code" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Code used during the calculation of taxes for the Payrolls associated with this Employment.
             
            May be derived from Employee
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="payrollCurrency" type="xs:normalizedString" minOccurs="0" sme:label="Employment Payroll Currency">
				<xs:annotation>
					<xs:documentation>
            Currency of the Payrolls associated with this Employment.
             
            ISO 4217.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operatingCompanyCurrency" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Employment OperatingCompany Currency">
				<xs:annotation>
					<xs:documentation>
            The currency as used by the Operating Company, the business operating the Sage application the base currency.
             
            ISO 4217.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operatingCompanyCurrencyExchangeRate" type="xs:decimal" nillable="true" minOccurs="0" sme:label="Employment OperatingCompany Currency Exchange Rate">
				<xs:annotation>
					<xs:documentation>
            Rate used to convert from Employment Payroll Currency into Employment Operating Company Currency, if different.
             
            Default = 1
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operatingCompanyCurrencyExchangeRateOperator" type="employmentOperatingCompanyCurrencyExchangeRateOperator--enum" nillable="true" minOccurs="0" sme:label="Employment OperatingCompany Currency Exchange Rate Operator">
				<xs:annotation>
					<xs:documentation>
            Operator to be used multiplication or division to convert from Employment Payroll Currency to Employment Operating Company Currency.
             
            * multiplication or / division.

            E.g. if the operator is division then it is Employment Values in Employment Payroll Currency / Employment OperatingCompany Currency Exchange Rate = Employment Values in Employment OperatingCompany currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operatingCompanyCurrencyExchangeRateDate" type="xs:date" nillable="true" minOccurs="0" sme:label="Employment OperatingCompany Currency Exchange Rate Date">
				<xs:annotation>
					<xs:documentation>
            Date of the exchange rate used to convert from Employment Payroll Currency to Employment Operating Company Currency, if different.
             
            Default = System date
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="payrollSalary" type="xs:decimal" nillable="true" minOccurs="0" sme:label="Employment Payroll Salary">
				<xs:annotation>
					<xs:documentation>
            Salary applicable to the Payroll associated with this Employment.
             
            In Employment Payroll currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="payrollSalaryPer" type="employmentPayrollSalaryPer--enum" nillable="true" minOccurs="0" sme:label="Employment Payroll Salary Per">
				<xs:annotation>
					<xs:documentation>
            What the salary is applicable to e.g. per Hour, per Day, etc.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="cumulativePayrollSalary" type="xs:decimal" minOccurs="0" sme:label="Employment Cumulative Payroll Salary">
				<xs:annotation>
					<xs:documentation>
            Total of all salary/Payroll for the current Employment Period usually fiscal year.
             
            In Employment Payroll currency
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="cumulativeTax" type="xs:decimal" minOccurs="0" sme:label="Employment Cumulative Tax">
				<xs:annotation>
					<xs:documentation>
            Total of all Tax for the current Employment Period usually fiscal year.
             
            In Employment Payroll currency
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="cumulativeBenefit" type="xs:decimal" nillable="true" minOccurs="0" sme:label="Employment Cumulative Benefit">
				<xs:annotation>
					<xs:documentation>
            Total of all Benefits for the current Period usually fiscal year.
             
            In Employment Payroll currency
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="cumulativeDeduction" type="xs:decimal" nillable="true" minOccurs="0" sme:label="Employment Cumulative Deduction">
				<xs:annotation>
					<xs:documentation>
            Total of all Deductions for the current Employment Period usually fiscal year.
             
            In Employment Payroll currency
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="workObligation" type="xs:decimal" nillable="true" minOccurs="0" sme:label="Employment Work Obligation">
				<xs:annotation>
					<xs:documentation>
            The work obligation associated with the Employment and the Payroll, e.g. 35 hours per week, 12 months per year.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="workObligationPer" type="employmentWorkObligationPer--enum" nillable="true" minOccurs="0" sme:label="Employment Work Obligation Per">
				<xs:annotation>
					<xs:documentation>
            What the work obligation is applicable to e.g. per Hour, per Day, etc.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="cumulativeWorkObligation" type="xs:decimal" minOccurs="0" sme:label="Employment Cumulative Work Obligation">
				<xs:annotation>
					<xs:documentation>
            Total of all Work Obligations for the current Employment Period usually fiscal year.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="maximumWork" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Employment Maximum Work ">
				<xs:annotation>
					<xs:documentation>
            The maximum amount of work permitted in association with the Employment contract, expressed maximum work units but see Employee Working Time Reg Opt Out Flag on Employee record – this allows an Employee to exceed maximum work where legislation allows
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="maximumWorkPer" type="employmentMaximumWorkPer--enum" nillable="true" minOccurs="0" sme:label="Employment Maximum Work Per">
				<xs:annotation>
					<xs:documentation>
            What the maximum work is applicable to e.g. per Hour, per Day, etc.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="holidayEntitlement" type="xs:decimal" nillable="true" minOccurs="0" sme:label="Employment Holiday Entitlement">
				<xs:annotation>
					<xs:documentation>
            The holiday vacation entitlement associated with the Employment e.g. 25 days – CURRENT ACCUMULATION.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="holidayEntitlementPer" type="employmentHolidayEntitlementPer--enum" nillable="true" minOccurs="0" sme:label="Employment Holiday Entitlement Per">
				<xs:annotation>
					<xs:documentation>
            What the holiday entitlement is applicable to e.g. per Hour, per Day, etc.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="cumulativeHoliday" type="xs:decimal" minOccurs="0" sme:label="Employment Cumulative Holiday ">
				<xs:annotation>
					<xs:documentation>
            Total of all Holiday for the current Employment Period usually fiscal year.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="holidayRolloverAllowed" type="xs:boolean" nillable="true" minOccurs="0" sme:label="Employment Holiday Rollover Allowed Flag">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate if Holiday entitlement can be rolled over or carried forward e.g. from current year to next year.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="holidayRolloverEntitlement" type="xs:decimal" nillable="true" minOccurs="0" sme:label="Employment Holiday Rollover Entitlement">
				<xs:annotation>
					<xs:documentation>
            The holiday vacation entitlement rolled over from previous periods if allowed e.g. 25 days.
             
            In Holiday Entitlement Per unit.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="holidayRolloverMaximumEntitlement" type="xs:decimal" nillable="true" minOccurs="0" sme:label="Employment Holiday Rollover Maximum Entitlement">
				<xs:annotation>
					<xs:documentation>
            The maximum holiday vacation entitlement that can be rolled over from previous periods if allowed e.g. 5 days.
             
            In Holiday Entitlement Per unit.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="cumulativeHolidayRollover" type="xs:decimal" minOccurs="0" sme:label="Employment Cumulative Holiday Rollover ">
				<xs:annotation>
					<xs:documentation>
            Total of all Rollover Holiday taken for the current Employment Period usually fiscal year.
             
            In Holiday Entitlement Per unit.

            Default to zero if not used
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="holidayRolloverExpiry" type="xs:date" nillable="true" minOccurs="0" sme:label="Employment Holiday Rollover Expiry">
				<xs:annotation>
					<xs:documentation>
            Date any Holiday entitlement rolled over or carried forward e.g. from current year to next year expires.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="earnedHolidayAllowed" type="xs:boolean" nillable="true" minOccurs="0" sme:label="Employment Earned Holiday Allowed Flag">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate if earned Holiday time off in lieu – TOIL is allowed.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="earnedHolidayEntitlement" type="xs:decimal" nillable="true" minOccurs="0" sme:label="Employment Earned Holiday Entitlement">
				<xs:annotation>
					<xs:documentation>
            The earned holiday vacation current entitlement e.g. 3 days.
             
            In Holiday Entitlement Per unit.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="earnedHolidayMaximumEntitlement" type="xs:decimal" nillable="true" minOccurs="0" sme:label="Employment Earned Holiday Maximum Entitlement">
				<xs:annotation>
					<xs:documentation>
            The maximum amount of earned holiday vacation that can be earned e.g. 5 days.
             
            In Holiday Entitlement Per unit.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="cumulativeEarnedHoliday" type="xs:decimal" minOccurs="0" sme:label="Employment Cumulative Earned Holiday ">
				<xs:annotation>
					<xs:documentation>
            Total of all earned Holiday taken for the current Employment Period usually fiscal year.
             
            In Holiday Entitlement Per unit.

            Default to zero if not used
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="earnedHolidayExpiry" type="xs:date" nillable="true" minOccurs="0" sme:label="Employment Earned Holiday Expiry">
				<xs:annotation>
					<xs:documentation>
            Date any earned Holiday expires.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="sicknessEntitlement" type="xs:decimal" nillable="true" minOccurs="0" sme:label="Employment Sickness Entitlement">
				<xs:annotation>
					<xs:documentation>
            The sickness entitlement associated with the Employment e.g. 0 days.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="sicknessEntitlementPer" type="employmentSicknessEntitlementPer--enum" nillable="true" minOccurs="0" sme:label="Employment Sickness Entitlement Per">
				<xs:annotation>
					<xs:documentation>
            What the sickness entitlement is applicable to e.g. per Hour, per Day, etc.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="cumulativeSickness" type="xs:decimal" minOccurs="0" sme:label="Employment Cumulative Sickness ">
