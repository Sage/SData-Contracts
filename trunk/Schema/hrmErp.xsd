          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="workUnit" type="payrollWorkUnit--enum" nillable="true" minOccurs="0" sme:label="Payroll Work Unit">
				<xs:annotation>
					<xs:documentation>
            Unit associated with the Payroll
             
            And/or on the PayrollLine.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="workQuantity" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Payroll Work Quantity">
				<xs:annotation>
					<xs:documentation>
            Quantity of units worked for the Payroll.
             
            And/or on the PayrollLine.

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="cumulativeWork" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Payroll Cumulative Work">
