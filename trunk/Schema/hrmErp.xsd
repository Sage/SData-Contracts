            In Payroll currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="salaryCumulativeAmount" type="xs:decimal" nillable="true" minOccurs="0" sme:label="Payroll Salary Cumulative Amount">
