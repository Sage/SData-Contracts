			<xs:element name="active" type="xs:boolean" minOccurs="0" sme:label="Vehicle Active Flag">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate if the Vehicle is an active one .
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference" type="xs:normalizedString" minOccurs="0" sme:label="Vehicle Reference" sme:isUniqueKey="true">
				<xs:annotation>
					<xs:documentation>
            Reference for the Vehicle
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference2" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Vehicle Reference2">
				<xs:annotation>
					<xs:documentation>
            Additional reference for the Vehicle
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ownership" type="vehicleOwnership--enum" minOccurs="0" sme:label="Vehicle Ownership">
				<xs:annotation>
					<xs:documentation>
            Details on the ownership of the vehicle.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="status" type="vehicleStatus--enum" nillable="true" minOccurs="0" sme:label="Vehicle Status">
				<xs:annotation>
					<xs:documentation>
            Status of the Vehicle
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operatingCompany" type="sc:operatingCompany--type" nillable="true" minOccurs="0" sme:label="Vehicle OperatingCompany" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            OperatingCompany associated with the Vehicle
             
            See OperatingCompany definition Company where type = Operating Company.

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="employees" type="employee--list" nillable="true" minOccurs="0" sme:label="Vehicle Employee" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Employees receiving the Vehicle
             
            See Employee definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="employments" type="employment--list" nillable="true" minOccurs="0" sme:label="Vehicle Employment" sme:relationship="association" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Employments associated with i.e. entitle to the Vehicle
             
            See Employment definition.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="registeredKeeper" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Vehicle Registered Keeper">
				<xs:annotation>
					<xs:documentation>
            If there is no link to the Employee then this allows the details of the registered keeper to be recorded.
             
            Normally the registered keeper is the associated Employee.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="type" type="vehicleType--enum" minOccurs="0" sme:label="Vehicle Type">
				<xs:annotation>
					<xs:documentation>
            The type of Vehicle.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="make" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Vehicle Make">
				<xs:annotation>
					<xs:documentation>
            The make of the Vehicle e.g. BMW, Ford, etc.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="model" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Vehicle Model">
				<xs:annotation>
					<xs:documentation>
            The model descriptor for the Vehicle e.g. 3 series, Focus
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="engineSize" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Vehicle Engine Size">
				<xs:annotation>
					<xs:documentation>
            Engine size in cc of the Vehicle e.g. 2000 cc.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fuelType" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Vehicle Fuel Type">
				<xs:annotation>
					<xs:documentation>
            The fuel type for the Vehicle.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="colour" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Vehicle Colour">
				<xs:annotation>
					<xs:documentation>
            The colour of the Vehicle.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="registration" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Vehicle Registration Number">
				<xs:annotation>
					<xs:documentation>
            The registration number of the Vehicle.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="identificationNumber" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Vehicle Identification Number">
				<xs:annotation>
					<xs:documentation>
            The Vehicle identification number.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dateOfRegistration" type="xs:date" nillable="true" minOccurs="0" sme:label="Vehicle Date of Registration">
				<xs:annotation>
					<xs:documentation>
            Date the Vehicle was registered as new.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="issueDate" type="xs:date" nillable="true" minOccurs="0" sme:label="Vehicle Issue Date">
				<xs:annotation>
					<xs:documentation>
            Date the Vehicle was issued made available for use.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="returnDate" type="xs:date" nillable="true" minOccurs="0" sme:label="Vehicle Return Date">
				<xs:annotation>
					<xs:documentation>
            Date the Vehicle was returned.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="disposalDate" type="xs:date" nillable="true" minOccurs="0" sme:label="Vehicle Disposal Date">
				<xs:annotation>
					<xs:documentation>
            Date the Vehicle was disposed of e.g. sold or scrapped
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="disposalReason" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Vehicle Disposal Reason">
				<xs:annotation>
					<xs:documentation>
            Textual reason as to why Vehicle was disposed of.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxationTypeIndicator" type="vehicleTaxationTypeIndicator--enum" nillable="true" minOccurs="0" sme:label="Vehicle Taxation Type Indicator">
				<xs:annotation>
					<xs:documentation>
            Flag indicating what the taxation of the vehicle is based on – CO2, HP horse power, Mileage, Exempt, other.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="emissions" type="xs:decimal" nillable="true" minOccurs="0" sme:label="Vehicle Emissions">
				<xs:annotation>
					<xs:documentation>
            The emissions associated with the Vehicle, express in CO2, used for tax calculation.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="horsepower" type="xs:decimal" nillable="true" minOccurs="0" sme:label="Vehicle Horse Power">
				<xs:annotation>
					<xs:documentation>
            The associated HP of the Vehicle, used for tax calculation.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="weight" type="xs:decimal" nillable="true" minOccurs="0" sme:label="Vehicle Weight">
				<xs:annotation>
					<xs:documentation>
            Weight of vehicle in SI units KG.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="listPrice" type="xs:decimal" nillable="true" minOccurs="0" sme:label="Vehicle List Price">
				<xs:annotation>
					<xs:documentation>
            List price of the Vehicle.
             
            In Benefit Currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="accessoriesPrice" type="xs:decimal" nillable="true" minOccurs="0" sme:label="Vehicle Accessories Price">
				<xs:annotation>
					<xs:documentation>
            Price of any accessories added to the Vehicle.
             
            In Benefit Currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="numberOfSeats" type="xs:normalizedString" nillable="true" minOccurs="0" sme:label="Vehicle Number Of Seats">
				<xs:annotation>
					<xs:documentation>
            Number of seats the vehicle has.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="employeeContribution" type="xs:decimal" nillable="true" minOccurs="0" sme:label="Vehicle Employee Contribution">
				<xs:annotation>
					<xs:documentation>
            If an Employee has made a contribution to the purchase cost of the Vehicle it is recorded here.
             
            In Benefit Currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxablePrice" type="xs:decimal" nillable="true" minOccurs="0" sme:label="Vehicle Taxable Price">
				<xs:annotation>
					<xs:documentation>
            The taxable value of the Vehicle typically List Price + Accessories – Employee Contribution.
             
            In Benefit Currency.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="firstUseDate" type="xs:date" nillable="true" minOccurs="0" sme:label="Vehicle Date of First Use">
				<xs:annotation>
					<xs:documentation>
            The date the vehicle was first used.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="lastTaxDate" type="xs:date" nillable="true" minOccurs="0" sme:label="Vehicle Last Tax Date">
				<xs:annotation>
					<xs:documentation>
            Date any vehicular taxes were last paid.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="nextTaxDate" type="xs:date" nillable="true" minOccurs="0" sme:label="Vehicle Next Tax Date">
				<xs:annotation>
					<xs:documentation>
            Date any vehicular taxes are due to be paid.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="lastServiceDate" type="xs:date" nillable="true" minOccurs="0" sme:label="Vehicle Last Service Date">
				<xs:annotation>
					<xs:documentation>
            Date of last Vehicle service.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="nextServiceDate" type="xs:date" nillable="true" minOccurs="0" sme:label="Vehicle Next Service Date">
				<xs:annotation>
					<xs:documentation>
            Date next Vehicle service is due.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="lastTestDate" type="xs:date" nillable="true" minOccurs="0" sme:label="Vehicle Last Test Date">
				<xs:annotation>
					<xs:documentation>
            Date any Vehicle tests e.g. UK MOT were last done.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="nextTestDate" type="xs:date" nillable="true" minOccurs="0" sme:label="Vehicle Next Test Date">
				<xs:annotation>
					<xs:documentation>
            Date any Vehicle tests e.g. UK MOT are next due.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="supplier" type="crmErp:tradingAccount--type" nillable="true" minOccurs="0" sme:label="Vehicle Supplier" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Link to the Supplier associated with the Vehicle e.g. leasing company if Vehicle is leased.
             
            See Supplier definition. CRM schema.
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="associatedFixedAsset" type="famErp:fixedAsset--type" nillable="true" minOccurs="0" sme:label="Vehicle Associated Fixed Asset" sme:relationship="reference" sme:isCollection="false">
				<xs:annotation>
					<xs:documentation>
            Link to the asset record associated with the Vehicle.
             
            Fixed Assets are defined in the FAM schema

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="specialDesignationFlag" type="xs:boolean" nillable="true" minOccurs="0" sme:label="Vehicle Special Designation Flag">
				<xs:annotation>
					<xs:documentation>
            Flag to indicate if Vehicle has any special designations e.g. UK Classic Car designation.
             
            Default = False
          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="text1" type="xs:string" nillable="true" minOccurs="0" sme:label="Vehicle Text1">
				<xs:annotation>
					<xs:documentation>
            Text/commentary for the Vehicle
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="text2" type="xs:string" nillable="true" minOccurs="0" sme:label="Vehicle Text2">
				<xs:annotation>
					<xs:documentation>
            Further text/commentary for the Vehicle.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="mileage" type="xs:decimal" nillable="true" minOccurs="0" sme:label="Vehicle Mileage">
				<xs:annotation>
					<xs:documentation>
            Mileage associated with Vehicle as at Mileage Date, below.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="mileageDate" type="xs:date" nillable="true" minOccurs="0" sme:label="Vehicle Mileage Date">
				<xs:annotation>
					<xs:documentation>
            Date of the mileage figure given above.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="disposalMileage" type="xs:decimal" nillable="true" minOccurs="0" sme:label="Vehicle Disposal Mileage">
				<xs:annotation>
					<xs:documentation>
            Mileage when Vehicle is or was disposed of.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="notes" type="sc:note--list" nillable="true" minOccurs="0" sme:label="Vehicle Note" sme:relationship="child" sme:isCollection="true">
				<xs:annotation>
					<xs:documentation>
            Notes associated with a Vehicle.
             

          </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
		<xs:anyAttribute/>
	</xs:complexType>
	<xs:complexType name="vehicle--list">
		<xs:sequence minOccurs="0" maxOccurs="unbounded">
			<xs:element name="vehicle" type="vehicle--type"/>
		</xs:sequence>
		<xs:anyAttribute/>
	</xs:complexType>
	<xs:element name="payroll" type="payroll--type" sme:role="resourceKind" sme:pluralName="payrolls" sme:compliance="should" sme:canGet="true" sme:canPut="true" sme:canPost="true" sme:canDelete="true"/>
	<xs:complexType name="payroll--type">
		<xs:all>
			<xs:element name="active" type="xs:boolean" minOccurs="0" sme:label="Payroll Active Flag">
